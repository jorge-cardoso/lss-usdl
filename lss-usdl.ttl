    rdfs:range lss-usdl:Role .

lss-usdl:hasTime a rdf:Property;
    rdfs:isDefinedBy <http://genssiz.dei.uc.pt/lss-usdl>;
    rdfs:label "has time";
    rdfs:domain lss-usdl:Interaction;
    rdfs:range lss-usdl:Time .

lss-usdl:hasGoal a rdf:Property;
    rdfs:isDefinedBy <http://genssiz.dei.uc.pt/lss-usdl>;
    rdfs:label "has goal";
    rdfs:domain lss-usdl:Interaction;
