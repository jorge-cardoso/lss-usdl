    skos:prefLabel "Knowledge Base" ;
    skos:related itil:Knowledge_Management,
        itil:Management_System,
        itil:Service,
        itil:Service_Knowledge_Management_System,
        itil:System .

itil:Management_of_Risk a skos:concept ;
    rdfs:label "Management of Risk" ;
    skos:definition "The OGC methodology for managing Risks. MoR includes all the Activities required to identify and Control the exposure to Risk which may have an impact on the achievement of an Organisation&rsquo;s Business Objectives. See http://www.m-o-r.org/ for more details." ;
    skos:prefLabel "Management of Risk" ;
    skos:related itil:Business,
        itil:Business_Objective,
        itil:Control,
        itil:Objective,
        itil:Organisation,
        itil:Risk .

itil:Marginal_Cost a skos:concept ;
    rdfs:label "Marginal Cost" ;
    skos:definition "The Cost of continuing to provide the IT Service." ;
    skos:example "Marginal Cost does not include investment already made, for example the cost of developing new software and delivering training." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Marginal Cost" ;
    skos:related itil:Cost,
        itil:IT_Service,
        itil:Service .

itil:Maturity_Level a skos:concept ;
    rdfs:label "Maturity Level" ;
    skos:definition "A named level in a Maturity model such as the Carnegie Mellon Capability Maturity Model Integration." ;
    skos:prefLabel "Maturity Level" ;
    skos:related itil:Capability,
        itil:Capability_Maturity_Model,
        itil:Capability_Maturity_Model_Integration,
        itil:Maturity,
        itil:Model .

itil:Mean_Time_Between_Failures a skos:concept ;
    rdfs:label "Mean Time Between Failures" ;
    skos:definition "A Metric for measuring and reporting Reliability. MTBF is the average time that a Configuration Item or IT Service can perform its agreed Function without interruption. This is measured from when the CI or IT Service starts working, until it next fails." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Mean Time Between Failures" ;
    skos:related itil:Configuration,
        itil:Configuration_Item,
        itil:Function,
        itil:IT_Service,
        itil:Metric,
        itil:Reliability,
        itil:Service .

itil:Mean_Time_Between_Service_Incidents a skos:concept ;
    rdfs:label "Mean Time Between Service Incidents" ;
    skos:definition "A Metric used for measuring and reporting Reliability. MTBSI is the mean time from when a System or IT Service fails, until it next fails. MTBSI is equal to MTBF + MTRS." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Mean Time Between Service Incidents" ;
    skos:related itil:IT_Service,
        itil:Metric,
        itil:Reliability,
        itil:Service,
        itil:System .

itil:Mean_Time_To_Repair a skos:concept ;
    rdfs:label "Mean Time To Repair" ;
    skos:definition "The average time taken to repair a Configuration Item or IT Service after a Failure. MTTR is measured from when the CI or IT Service fails until it is Repaired. MTTR does not include the time required to Recover or Restore. MTTR is sometimes incorrectly used to mean Mean Time to Restore Service." ;
    skos:prefLabel "Mean Time To Repair" ;
    skos:related itil:Configuration,
        itil:Configuration_Item,
        itil:Failure,
        itil:IT_Service,
        itil:Mean_Time_to_Restore_Service,
        itil:Repair,
        itil:Restore,
        itil:Service .

itil:Monitor_Control_Loop a skos:concept ;
    rdfs:label "Monitor Control Loop" ;
    skos:definition "Monitoring the output of a Task, Process, IT Service or Configuration Item; comparing this output to a predefined norm; and taking appropriate action based on this comparison." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Monitor Control Loop" ;
    skos:related itil:Configuration,
        itil:Configuration_Item,
        itil:IT_Service,
        itil:Monitoring,
        itil:Process,
        itil:Service .

itil:Nearshore a skos:concept ;
    rdfs:label "Nearshore" ;
    skos:definition "Provision of Services from a country near the country where the Customer is based. This can be the provision of an IT Service, or of supporting Functions such as Service Desk. See On-shore, Off-shore." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Nearshore" ;
    skos:related itil:Customer,
        itil:Function,
        itil:IT_Service,
        itil:Service,
        itil:Service_Desk .

itil:Office_of_Government_Commerce a skos:concept ;
    rdfs:label "Office of Government Commerce" ;
    skos:definition "OGC owns the ITIL brand (copyright and trademark). OGC is a UK Government department that supports the delivery of the government's procurement agenda through its work in collaborative procurement and in raising levels of procurement skills and capability with departments. It also provides support for complex public sector projects" ;
    skos:prefLabel "Office of Government Commerce" ;
    skos:related itil:ITIL .

itil:Office_of_Public_Sector_Information a skos:concept ;
    rdfs:label "Office of Public Sector Information" ;
    skos:definition "OPSI license the Crown Copyright material used in the ITIL publications. They are a UK Government department who provide online access to UK legislation, license the re-use of Crown copyright material, manage the Information Fair Trader Scheme, maintain the Government&rsquo;s Information Asset Register and provide advice and guidance on official publishing and Crown copyright." ;
    skos:prefLabel "Office of Public Sector Information" ;
    skos:related itil:Asset,
        itil:Asset_Register,
        itil:ITIL .

itil:Offshore a skos:concept ;
    rdfs:label "Offshore" ;
    skos:definition "Provision of Services from a location outside the country where the Customer is based, often in a different continent. This can be the provision of an IT Service, or of supporting Functions such as Service Desk. See On-shore, Near-shore." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Offshore" ;
    skos:related itil:Customer,
        itil:Function,
        itil:IT_Service,
        itil:Service,
        itil:Service_Desk .

itil:Onshore a skos:concept ;
    rdfs:label "Onshore" ;
    skos:definition "Provision of Services from a location within the country where the Customer is based. See Off-shore, Near-shore." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Onshore" ;
    skos:related itil:Customer,
        itil:Service .

itil:Operational_Expenditure a skos:concept ;
    rdfs:label "Operational Expenditure" ;
    skos:definition "Synonym for Operational Cost" ;
    skos:prefLabel "Operational Expenditure" ;
    skos:related itil:Cost,
        itil:Operation,
        itil:Operational,
        itil:Operational_Cost .

itil:Optimise a skos:concept ;
    rdfs:label "Optimise" ;
    skos:definition "Review, Plan and request Changes, in order to obtain the maximum Efficiency and Effectiveness from a Process, Configuration Item, Application etc." ;
    skos:prefLabel "Optimise" ;
    skos:related itil:Application,
        itil:Change,
        itil:Configuration,
        itil:Configuration_Item,
