        itil:Specification .

itil:Relationship a skos:concept ;
    rdfs:label "Relationship" ;
    skos:definition "A connection or interaction between two people or things. In Business Relationship Management it is the interaction between the IT Service Provider and the Business. In Configuration Management it is a link between two Configuration Items that identifies a dependency or connection between them." ;
    skos:example "For example Applications may be linked to the Servers they run on, IT Services have many links to all the CIs that contribute to them." ;
    skos:prefLabel "Relationship" ;
    skos:related itil:Business,
        itil:Business_Relationship_Management,
        itil:Configuration,
        itil:Configuration_Item,
        itil:Configuration_Management,
        itil:IT_Service,
        itil:IT_Service_Provider,
        itil:Relationship,
        itil:Service,
        itil:Service_Provider .

itil:Configuration_Management a skos:concept ;
    rdfs:label "Configuration Management" ;
    skos:definition "Component The Process responsible for maintaining information about Configuration Items required to deliver an IT Service, including their Relationships. This information is managed throughout the Lifecycle of the CI. Configuration Management is part of an overall Service Asset and Configuration Management Process." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Configuration Management" ;
