        itil:IT_Service_Continuity_Management,
        itil:Process,
        itil:Service,
        itil:Service_Continuity_Management,
        itil:Vital_Business_Function .

itil:Asset_Management a skos:concept ;
    rdfs:label "Asset Management" ;
    skos:definition "Asset Management is the Process responsible for tracking and reporting the value and ownership of financial Assets throughout their Lifecycle. Asset Management is part of an overall Service Asset and Configuration Management Process." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Asset Management" ;
    skos:related itil:Asset,
        itil:Asset_Management,
        itil:Configuration,
        itil:Configuration_Management,
        itil:Lifecycle,
        itil:Process,
        itil:Service,
        itil:Service_Asset,
        itil:Service_Asset_and_Configuration_Management .

itil:Business_Continuity_Management a skos:concept ;
    rdfs:label "Business Continuity Management" ;
    skos:definition "The Business Process responsible for managing Risks that could seriously impact the Business. BCM safeguards the interests of key stakeholders, reputation, brand and value creating activities. The BCM Process involves reducing Risks to an acceptable level and planning for the recovery of Business Processes should a disruption to the Business occur. BCM sets the Objectives, Scope and Requirements for IT Service Continuity Management." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Business Continuity Management" ;
    skos:related itil:Business,
        itil:Business_Process,
        itil:IT_Service,
        itil:IT_Service_Continuity_Management,
        itil:Objective,
        itil:Process,
        itil:Requirement,
        itil:Risk,
        itil:Scope,
        itil:Service,
        itil:Service_Continuity_Management .

itil:Business_Continuity_Plan a skos:concept ;
    rdfs:label "Business Continuity Plan" ;
    skos:definition "A Plan defining the steps required to Restore Business Processes following a disruption. The Plan will also identify the triggers for Invocation, people to be involved, communications etc. IT Service Continuity Plans form a significant part of Business Continuity Plans." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Business Continuity Plan" ;
    skos:related itil:Business,
        itil:Business_Continuity_Plan,
        itil:Business_Process,
        itil:IT_Service,
        itil:IT_Service_Continuity_Plan,
        itil:Invocation,
        itil:Plan,
        itil:Process,
        itil:Restore,
        itil:Service .

itil:Business_Relationship_Manager a skos:concept ;
    rdfs:label "Business Relationship Manager" ;
    skos:definition "A Role responsible for maintaining the Relationship with one or more Customers. This Role is often combined with the Service Level Manager Role." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Business Relationship Manager" ;
    skos:related itil:Customer,
        itil:Relationship,
        itil:Role,
        itil:Service,
        itil:Service_Level .

itil:Change_Advisory_Board a skos:concept ;
    rdfs:label "Change Advisory Board" ;
    skos:definition "A group of people that advises the Change Manager in the Assessment, prioritisation and scheduling of Changes. This board is usually made up of representatives from all areas within the IT Service Provider, the Business, and Third Parties such as Suppliers." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Change Advisory Board" ;
    skos:related itil:Assessment,
        itil:Business,
        itil:Change,
        itil:IT_Service,
        itil:IT_Service_Provider,
        itil:Service,
        itil:Service_Provider,
        itil:Supplier .

itil:Change_Record a skos:concept ;
    rdfs:label "Change Record" ;
    skos:definition "A Record containing the details of a Change. Each Change Record documents the Lifecycle of a single Change. A Change Record is created for every Request for Change that is received, even those that are subsequently rejected. Change Records should reference the Configuration Items that are affected by the Change. Change Records are stored in the Configuration Management System." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Change Record" ;
    skos:related itil:Change,
        itil:Change_Record,
        itil:Configuration,
        itil:Configuration_Item,
        itil:Configuration_Management,
        itil:Configuration_Management_System,
        itil:Lifecycle,
        itil:Management_System,
        itil:Record,
        itil:Request_for_Change,
        itil:System .

itil:Change_Window a skos:concept ;
    rdfs:label "Change Window" ;
    skos:definition "regular, agreed time when Changes or Releases may be implemented with minimal impact on Services. Change Windows are usually documented in SLAs." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Change Window" ;
    skos:related itil:Change,
        itil:Change_Window,
        itil:Release,
        itil:Service .

itil:Configuration_Management_Database a skos:concept ;
    rdfs:label "Configuration Management Database" ;
    skos:definition "A database used to store Configuration Records throughout their Lifecycle. The Configuration Management System maintains one or more CMDBs, and each CMDB stores Attributes of CIs, and Relationships with other CIs" ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Configuration Management Database" ;
    skos:related itil:Attribute,
        itil:Configuration,
        itil:Configuration_Management,
        itil:Configuration_Management_System,
        itil:Configuration_Record,
        itil:Lifecycle,
        itil:Management_System,
        itil:Record,
        itil:Relationship,
        itil:System .

itil:Cost_Benefit_Analysis a skos:concept ;
    rdfs:label "Cost Benefit Analysis" ;
    skos:definition "An Activity that analyses and compares the Costs and the benefits involved in one or more alternative courses of action. See Business Case, Net Present Value, Internal Rate of Return, Return on Investment, Value on Investment." ;
    skos:prefLabel "Cost Benefit Analysis" ;
    skos:related itil:Activity,
        itil:Business,
        itil:Business_Case,
        itil:Cost,
        itil:Internal_Rate_of_Return,
        itil:Net_Present_Value,
        itil:Return_on_Investment,
        itil:Value_on_Investment .

itil:Culture a skos:concept ;
    rdfs:label "Culture" ;
    skos:definition "A set of values that is shared by a group of people, including expectations about how people should behave, ideas, beliefs, and practices." ;
    skos:prefLabel "Culture" .

itil:Event_Management a skos:concept ;
    rdfs:label "Event Management" ;
    skos:definition "Service Operation) The Process responsible for managing Events throughout their Lifecycle. Event Management is one of the main Activities of IT Operations." ;
    skos:prefLabel "Event Management" ;
    skos:related itil:Event,
        itil:Event_Management,
        itil:IT_Operations,
        itil:Lifecycle,
        itil:Operation,
        itil:Process,
        itil:Service,
        itil:Service_Operation .

itil:Fast_Recovery a skos:concept ;
    rdfs:label "Fast Recovery" ;
    skos:definition "A Recovery Option which is also known as Hot Standby. Provision is made to Recover the IT Service in a short period of time, typically less than 24 hours. Fast Recovery typically uses a dedicated Fixed Facility with computer Systems, and software configured ready to run the IT Services. Immediate Recovery may take up to 24 hours if there is a need to Restore data from Backups." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Fast Recovery" ;
    skos:related itil:Backup,
        itil:Fast_Recovery,
        itil:Fixed_Facility,
        itil:Hot_Standby,
        itil:IT_Service,
        itil:Immediate_Recovery,
        itil:Recovery,
        itil:Recovery_Option,
        itil:Restore,
        itil:Service,
        itil:Standby,
        itil:System .

itil:Fault_Tolerance a skos:concept ;
    rdfs:label "Fault Tolerance" ;
    skos:definition "The ability of an IT Service or Configuration Item to continue to Operate correctly after Failure of a Component part. See Resilience, Countermeasure." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Fault Tolerance" ;
    skos:related itil:Component,
        itil:Configuration,
        itil:Configuration_Item,
        itil:Countermeasure,
        itil:Failure,
        itil:IT_Service,
        itil:Operate,
        itil:Resilience,
        itil:Service .

itil:Fit_for_Purpose a skos:concept ;
    rdfs:label "Fit for Purpose" ;
    skos:definition "An informal term used to describe a Process, Configuration Item, IT Service etc. that is capable of meeting its Objectives or Service Levels. Being Fit for Purpose requires suitable Design, implementation, Control and maintenance." ;
    skos:prefLabel "Fit for Purpose" ;
    skos:related itil:Configuration,
        itil:Configuration_Item,
        itil:Control,
        itil:Design,
        itil:Fit_for_Purpose,
        itil:IT_Service,
        itil:Objective,
        itil:Process,
        itil:Service,
        itil:Service_Level .

itil:Gradual_Recovery a skos:concept ;
    rdfs:label "Gradual Recovery" ;
    skos:definition "A Recovery Option which is also known as Cold Standby. Provision is made to Recover the IT Service in a period of time greater than 72 hours. Gradual Recovery typically uses a Portable or Fixed Facility that has environmental support and network cabling, but no computer Systems. The hardware and software are installed as part of the IT Service Continuity Plan." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Gradual Recovery" ;
    skos:related itil:Cold_Standby,
        itil:Fixed_Facility,
        itil:Gradual_Recovery,
        itil:IT_Service,
        itil:IT_Service_Continuity_Plan,
        itil:Plan,
        itil:Recovery,
        itil:Recovery_Option,
        itil:Service,
        itil:Standby,
        itil:System .

itil:IT_Operations_Control a skos:concept ;
    rdfs:label "IT Operations Control" ;
    skos:definition "The Function responsible for Monitoring and Control of the IT Services and IT Infrastructure. See Operations Bridge." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "IT Operations Control" ;
    skos:related itil:Control,
        itil:Function,
        itil:IT_Infrastructure,
        itil:IT_Service,
        itil:Monitoring,
        itil:Operation,
        itil:Operations_Bridge,
        itil:Service .

itil:Incident_Record a skos:concept ;
    rdfs:label "Incident Record" ;
    skos:definition "Record containing the details of an Incident. Each Incident record documents the Lifecycle of a single Incident" ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Incident Record" ;
    skos:related itil:Incident,
        itil:Lifecycle,
        itil:Record .

itil:Infrastructure_Service a skos:concept ;
    rdfs:label "Infrastructure Service" ;
    skos:definition "IT Service that is not directly used by the Business, but is required by the IT Service Provider so they can provide other IT Services." ;
    skos:example "For example Directory Services, naming services, or communication services." ;
    skos:prefLabel "Infrastructure Service" ;
    skos:related itil:Business,
        itil:IT_Service,
        itil:IT_Service_Provider,
        itil:Service,
        itil:Service_Provider .

itil:Intermediate_Recovery a skos:concept ;
    rdfs:label "Intermediate Recovery" ;
    skos:definition "A Recovery Option which is also known as Warm Standby. Provision is made to Recover the IT Service in a period of time between 24 and 72 hours. Intermediate Recovery typically uses a shared Portable or Fixed Facility that has computer Systems and network Components. The hardware and software will need to be configured, and data will need to be restored, as part of the IT Service Continuity Plan." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Intermediate Recovery" ;
    skos:related itil:Component,
        itil:Fixed_Facility,
        itil:IT_Service,
        itil:IT_Service_Continuity_Plan,
        itil:Intermediate_Recovery,
        itil:Plan,
        itil:Recovery,
        itil:Recovery_Option,
        itil:Service,
        itil:Standby,
        itil:System,
        itil:Warm_Standby .

itil:Maturity a skos:concept ;
    rdfs:label "Maturity" ;
    skos:definition "A measure of the Reliability, Efficiency and Effectiveness of a Process, Function, Organisation etc. The most mature Processes and Functions are formally aligned to Business Objectives and Strategy, and are supported by a framework for continual improvement." ;
    skos:member itil:Continual_Service_Improvement ;
    skos:prefLabel "Maturity" ;
    skos:related itil:Business,
        itil:Business_Objective,
        itil:Effectiveness,
        itil:Efficiency,
        itil:Function,
        itil:Objective,
        itil:Organisation,
        itil:Process,
        itil:Reliability,
        itil:Strategy .

itil:Modelling a skos:concept ;
    rdfs:label "Modelling" ;
    skos:definition "A technique that is used to predict the future behaviour of a System, Process, IT Service, Configuration Item etc. Modelling is commonly used in Financial Management, Capacity Management and Availability Management." ;
    skos:prefLabel "Modelling" ;
    skos:related itil:Availability,
        itil:Availability_Management,
        itil:Capacity,
        itil:Capacity_Management,
