    skos:prefLabel "Service Owner" ;
    skos:related itil:IT_Service,
        itil:Role,
        itil:Service .

itil:Service_Potential a skos:concept ;
    rdfs:label "Service Potential" ;
    skos:definition "The total possible value of the overall Capabilities and Resources of the IT Service Provider." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Service Potential" ;
    skos:related itil:IT_Service,
        itil:IT_Service_Provider,
        itil:Resource,
        itil:Service,
        itil:Service_Provider .

itil:Service_Provisioning_Optimization a skos:concept ;
    rdfs:label "Service Provisioning Optimization" ;
    skos:definition "Analysing the finances and constraints of an IT Service to decide if alternative approaches to Service delivery might reduce Costs or improve Quality." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Service Provisioning Optimization" ;
    skos:related itil:Cost,
        itil:IT_Service,
        itil:Quality,
        itil:Service .

itil:Shift a skos:concept ;
    rdfs:label "Shift" ;
    skos:definition "A group or team of people who carry out a specific Role for a fixed period of time." ;
    skos:example "For example there could be four shifts of IT Operations Control personnel to support an IT Service that is used 24 hours a day." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Shift" ;
    skos:related itil:Role .

itil:Simulation_modelling a skos:concept ;
    rdfs:label "Simulation modelling" ;
    skos:definition "A technique that creates a detailed Model to predict the behaviour of a Configuration Item or IT Service. Simulation Models can be very accurate but are expensive and time consuming to create. A Simulation Model is often created by using the actual Configuration Items that are being modelled, with artificial Workloads or Transactions. They are used in Capacity Management when accurate results are important. A simulation model is sometimes called a Performance Benchmark." ;
    skos:member itil:Continual_Service_Improvement,
        itil:Service_Design ;
    skos:prefLabel "Simulation modelling" ;
    skos:related itil:Benchmark,
        itil:Capacity,
        itil:Capacity_Management,
        itil:Configuration,
        itil:Configuration_Item,
        itil:IT_Service,
        itil:Model,
        itil:Performance,
        itil:Service,
        itil:Transaction,
        itil:Workload .

itil:Single_Point_of_Failure a skos:concept ;
    rdfs:label "Single Point of Failure" ;
