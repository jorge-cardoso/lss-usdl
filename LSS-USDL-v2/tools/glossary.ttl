        itil:Customer,
        itil:External_Service_Provider,
        itil:IT_Service,
        itil:IT_Service_Provider,
        itil:Internal_Service_Provider,
        itil:Organisation,
        itil:Service,
        itil:Service_Provider .

itil:Configuration a skos:concept ;
    rdfs:label "Configuration" ;
    skos:definition "A generic term, used to describe a group of Configuration Items that work together to deliver an IT Service, or a recognizable part of an IT Service. Configuration is also used to describe the parameter settings for one or more CIs." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Configuration" ;
    skos:related itil:Configuration,
        itil:Configuration_Item,
        itil:IT_Service,
        itil:Service .

itil:Service_Operation a skos:concept ;
    rdfs:label "Service Operation" ;
    skos:definition "A stage in the Lifecycle of an IT Service. Service Operation includes a number of Processes and Functions and is the title of one of the Core ITIL publications." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Service Operation" ;
    skos:related itil:Function,
        itil:ITIL,
        itil:IT_Service,
        itil:Lifecycle,
