    skos:definition "See PRINCE2" ;
    skos:prefLabel "PRojects IN Controlled Environments" ;
    skos:related itil:PRINCE2 .

itil:Pain_Value_Analysis a skos:concept ;
    rdfs:label "Pain Value Analysis" ;
    skos:definition "A technique used to help identify the Business Impact of one or more Problems. A formula is used to calculate Pain Value based on the number of Users affected, the duration of the Downtime, the Impact on each User, and the cost to the Business (if known)." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Pain Value Analysis" ;
    skos:related itil:Business,
        itil:Downtime,
        itil:Impact,
        itil:Problem,
        itil:User .

itil:Passive_Monitoring a skos:concept ;
    rdfs:label "Passive Monitoring" ;
    skos:definition "Monitoring of a Configuration Item, an IT Service or a Process that relies on an Alert or notification to discover the current status. See Active Monitoring." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Passive Monitoring" ;
    skos:related itil:Active_Monitoring,
        itil:Alert,
        itil:Configuration,
        itil:Configuration_Item,
        itil:IT_Service,
        itil:Monitoring,
        itil:Process,
        itil:Service .

itil:Percentage_utilisation a skos:concept ;
    rdfs:label "Percentage utilisation" ;
    skos:definition "The amount of time that a Component is busy over a given period of time." ;
    skos:example "For example, if a CPU is busy for 1800 seconds in a one hour period, its utilisation is 50%" ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Percentage utilisation" ;
    skos:related itil:Component .

itil:Performance_Anatomy a skos:concept ;
    rdfs:label "Performance Anatomy" ;
    skos:definition "An approach to Organisational Culture that integrates, and actively manages, leadership and strategy, people development, technology enablement, performance management and innovation." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Performance Anatomy" ;
    skos:related itil:Culture,
        itil:Organisation .

itil:Prerequisite_for_Success a skos:concept ;
    rdfs:label "Prerequisite for Success" ;
    skos:definition "PFS, An Activity that needs to be completed, or a condition that needs to be met, to enable successful implementation of a Plan or Process. A PFS is often an output from one Process that is a required input to another Process." ;
    skos:prefLabel "Prerequisite for Success" ;
    skos:related itil:Activity,
        itil:Plan,
        itil:Process .

itil:Pricing a skos:concept ;
    rdfs:label "Pricing" ;
    skos:definition "The Activity for establishing how much Customers will be Charged." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Pricing" ;
    skos:related itil:Activity,
        itil:Customer .

itil:Process_Control a skos:concept ;
    rdfs:label "Process Control" ;
    skos:definition "The Activity of planning and regulating a Process, with the Objective of performing the Process in an Effective, Efficient, and consistent manner." ;
    skos:prefLabel "Process Control" ;
