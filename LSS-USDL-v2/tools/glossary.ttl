    skos:prefLabel "Development" ;
    skos:related itil:Application,
        itil:Development,
        itil:IT_Service,
        itil:Process,
        itil:Role,
        itil:Service .

itil:External_Service_Provider a skos:concept ;
    rdfs:label "External Service Provider" ;
    skos:definition "An IT Service Provider which is part of a different Organisation to their Customer. An IT Service Provider may have both Internal Customers and External Customers. See Type III Service Provider." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "External Service Provider" ;
