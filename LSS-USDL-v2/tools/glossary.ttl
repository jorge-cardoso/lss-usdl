        itil:Operation,
        itil:Operational,
        itil:Operational_Cost .

itil:SWOT_Analysis a skos:concept ;
    rdfs:label "SWOT Analysis" ;
    skos:definition "A technique that reviews and analyses the internal strengths and weaknesses of an Organisation and the external opportunities and threats which it faces SWOT stands for Strengths, Weaknesses, Opportunities and Threats." ;
    skos:member itil:Continual_Service_Improvement ;
    skos:prefLabel "SWOT Analysis" ;
    skos:related itil:Organisation,
        itil:Threat .

itil:Scalability a skos:concept ;
    rdfs:label "Scalability" ;
    skos:definition "The ability of an IT Service, Process, Configuration Item etc. to perform its agreed Function when the Workload or Scope changes." ;
    skos:prefLabel "Scalability" ;
    skos:related itil:Configuration,
        itil:Configuration_Item,
        itil:Function,
        itil:IT_Service,
        itil:Process,
        itil:Scope,
        itil:Service,
        itil:Workload .

itil:Second-line_Support a skos:concept ;
    rdfs:label "Second-line Support" ;
    skos:definition "The second level in a hierarchy of Support Groups involved in the resolution of Incidents and investigation of Problems. Each level contains more specialist skills, or has more time or other Resources." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Second-line Support" ;
    skos:related itil:Incident,
        itil:Problem,
        itil:Resource,
        itil:Support_Group .

itil:Separation_of_Concerns a skos:concept ;
    rdfs:label "Separation of Concerns" ;
    skos:definition "An approach to Designing a solution or IT Service that divides the problem into pieces that can be solved independently. This approach separates \"what\" is to be done from \"how\" it is to be done." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Separation of Concerns" ;
    skos:related itil:Design,
        itil:IT_Service,
        itil:Service .

itil:Service_Acceptance_Criteria a skos:concept ;
    rdfs:label "Service Acceptance Criteria" ;
    skos:definition "A set of criteria used to ensure that an IT Service meets its functionality and Quality Requirements and that the IT Service Provider is ready to Operate the new IT Service when it has been Deployed." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Service Acceptance Criteria" ;
    skos:related itil:IT_Service,
        itil:IT_Service_Provider,
        itil:Operate,
        itil:Quality,
        itil:Requirement,
        itil:Service,
        itil:Service_Provider .

itil:Service_Failure_Analysis a skos:concept ;
    rdfs:label "Service Failure Analysis" ;
    skos:definition "An Activity that identifies underlying causes of one or more IT Service interruptions. SFA identifies opportunities to improve the IT Service Provider's Processes and tools, and not just the IT Infrastructure. SFA is a time constrained, project-like activity, rather than an ongoing process of analysis. See Root Cause Analysis." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Service Failure Analysis" ;
    skos:related itil:Activity,
        itil:IT_Infrastructure,
        itil:IT_Service,
        itil:IT_Service_Provider,
        itil:Process,
        itil:Root_Cause,
        itil:Service,
        itil:Service_Provider .

itil:Service_Improvement_Plan a skos:concept ;
    rdfs:label "Service Improvement Plan" ;
    skos:definition "SIP, A formal Plan to implement improvements to a Process or IT Service." ;
    skos:member itil:Continual_Service_Improvement ;
    skos:prefLabel "Service Improvement Plan" ;
    skos:related itil:IT_Service,
        itil:Plan,
        itil:Process,
        itil:Service .

itil:Service_Maintenance_Objective a skos:concept ;
    rdfs:label "Service Maintenance Objective" ;
    skos:definition "The expected time that a Configuration Item will be unavailable due to planned maintenance Activity." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Service Maintenance Objective" ;
    skos:related itil:Activity,
        itil:Configuration,
        itil:Configuration_Item .

itil:Service_Owner a skos:concept ;
