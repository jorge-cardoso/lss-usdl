        itil:Strategy,
        itil:Vision .

itil:Call a skos:concept ;
    rdfs:label "Call" ;
    skos:definition "A telephone call to the Service Desk from a User. A Call could result in an Incident or a Service Request being logged." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Call" ;
    skos:related itil:Call,
        itil:Incident,
        itil:Service,
        itil:Service_Desk,
        itil:Service_Request,
        itil:User .

itil:Code_of_Practice a skos:concept ;
    rdfs:label "Code of Practice" ;
    skos:definition "A Guideline published by a public body or a Standards Organisation, such as ISO or BSI. Many Standards consist of a Code of Practice and a Specification. The Code of Practice describes recommended Best Practice" ;
    skos:prefLabel "Code of Practice" ;
    skos:related itil:Best_Practice,
        itil:Code_of_Practice,
