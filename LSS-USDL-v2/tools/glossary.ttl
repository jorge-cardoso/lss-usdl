    skos:member itil:Service_Design ;
    skos:prefLabel "Single Point of Failure" ;
    skos:related itil:Activity,
        itil:Component,
        itil:Configuration,
        itil:Configuration_Item,
        itil:Countermeasure,
        itil:IT_Infrastructure,
        itil:Incident,
        itil:Process .

itil:Snapshot a skos:concept ;
    rdfs:label "Snapshot" ;
    skos:definition "Service Transition) The current state of a Configuration as captured by a discovery tool. Also used as a synonym for Benchmark. See Baseline." ;
    skos:prefLabel "Snapshot" ;
    skos:related itil:Baseline,
        itil:Benchmark,
        itil:Configuration,
        itil:Service,
        itil:Service_Transition,
        itil:Transition .

itil:Source a skos:concept ;
    rdfs:label "Source" ;
    skos:definition "See Service Sourcing" ;
    skos:prefLabel "Source" ;
    skos:related itil:Service,
        itil:Service_Sourcing .

itil:Standard_Operating_Procedures a skos:concept ;
    rdfs:label "Standard Operating Procedures" ;
    skos:definition "Procedures used by IT Operations Management" ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Standard Operating Procedures" ;
    skos:related itil:IT_Operations,
        itil:IT_Operations_Management,
        itil:Operation,
        itil:Operations_Management,
        itil:Procedure .

itil:Status_Accounting a skos:concept ;
