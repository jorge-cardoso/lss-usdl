        itil:Maintainability,
        itil:Performance,
        itil:Practice,
        itil:Reliability,
        itil:Security,
        itil:Service,
        itil:Serviceability .

itil:Recovery a skos:concept ;
    rdfs:label "Recovery" ;
    skos:definition "Returning a Configuration Item or an IT Service to a working state. Recovery of an IT Service often includes recovering data to a known consistent state. After Recovery, further steps may be needed before the IT Service can be made available to the Users (Restoration)." ;
    skos:member itil:Service_Design,
        itil:Service_Operation ;
    skos:prefLabel "Recovery" ;
    skos:related itil:Configuration,
        itil:Configuration_Item,
        itil:IT_Service,
        itil:Recovery,
        itil:Service,
        itil:User .

itil:Resource a skos:concept ;
    rdfs:label "Resource" ;
    skos:definition "A generic term that includes IT Infrastructure, people, money or anything else that might help to deliver an IT Service. Resources are considered to be Assets of an Organisation. See Capability, Service Asset." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Resource" ;
