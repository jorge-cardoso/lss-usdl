        itil:Service_Strategy,
        itil:Strategy,
        itil:Supporting_Service .

itil:Incident_Management a skos:concept ;
    rdfs:label "Incident Management" ;
    skos:definition "The Process responsible for managing the Lifecycle of all Incidents. The primary Objective of Incident Management is to return the IT Service to Users as quickly as possible." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Incident Management" ;
    skos:related itil:IT_Service,
        itil:Incident,
        itil:Incident_Management,
        itil:Lifecycle,
        itil:Objective,
        itil:Process,
        itil:Service,
        itil:User .

itil:Information_Technology a skos:concept ;
    rdfs:label "Information Technology" ;
    skos:definition "The use of technology for the storage, communication or processing of information. The technology typically includes computers, telecommunications, Applications and other software. The information may include Business data, voice, images, video, etc. Information Technology is often used to support Business Processes through IT Services." ;
    skos:prefLabel "Information Technology" ;
    skos:related itil:Application,
        itil:Business,
        itil:Business_Process,
        itil:IT_Service,
        itil:Information_Technology,
        itil:Process,
        itil:Service .

itil:Internal_Customer a skos:concept ;
    rdfs:label "Internal Customer" ;
    skos:definition "A Customer who works for the same Business as the IT Service Provider. See Internal Service Provider, External Customer." ;
    skos:prefLabel "Internal Customer" ;
