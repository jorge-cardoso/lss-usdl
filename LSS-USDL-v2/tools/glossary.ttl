        itil:Document,
        itil:Plan,
        itil:Service,
        itil:Service_Level,
        itil:Test .

itil:Quality_Assurance a skos:concept ;
    rdfs:label "Quality Assurance" ;
    skos:definition "The Process responsible for ensuring that the Quality of a product, Service or Process will provide its intended Value." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Quality Assurance" ;
    skos:related itil:Process,
        itil:Quality,
        itil:Service .

itil:Qualitycycle_DEMING a skos:concept ;
    rdfs:label "Qualitycycle DEMING" ;
    skos:definition "Synonym for Plan-Do-Check-Act" ;
    skos:prefLabel "Qualitycycle DEMING" ;
    skos:related itil:Plan,
        itil:Plan-Do-Check-Act .

itil:Quick_Win a skos:concept ;
    rdfs:label "Quick Win" ;
    skos:definition "An improvement Activity which is expected to provide a Return on Investment in a short period of time with relatively small Cost and effort. See Pareto Principle." ;
    skos:member itil:Continual_Service_Improvement ;
    skos:prefLabel "Quick Win" ;
    skos:related itil:Activity,
        itil:Cost,
        itil:Pareto_Principle,
        itil:Return_on_Investment .

itil:Reactive_Monitoring a skos:concept ;
    rdfs:label "Reactive Monitoring" ;
    skos:definition "Monitoring that takes action in response to an Event. See Proactive Monitoring." ;
    skos:example "For example submitting a batch job when the previous job completes, or logging an Incident when an Error occurs." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Reactive Monitoring" ;
    skos:related itil:Event,
        itil:Monitoring,
        itil:Proactive_Monitoring .

itil:Redundancy a skos:concept ;
    rdfs:label "Redundancy" ;
    skos:definition "Synonym for Fault Tolerance. The term Redundant also has a generic meaning of obsolete, or no longer needed." ;
    skos:prefLabel "Redundancy" ;
    skos:related itil:Fault,
        itil:Fault_Tolerance .

itil:Relationship_Processes a skos:concept ;
    rdfs:label "Relationship Processes" ;
    skos:definition "The ISO/IEC 20000 Process group that includes Business Relationship Management and Supplier Management." ;
    skos:prefLabel "Relationship Processes" ;
