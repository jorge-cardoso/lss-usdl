    skos:prefLabel "Root Cause" ;
    skos:related itil:Incident,
        itil:Problem .

itil:Service_Knowledge_Management_System a skos:concept ;
    rdfs:label "Service Knowledge Management System" ;
    skos:definition "A set of tools and databases that are used to manage knowledge and information. The SKMS includes the Configuration Management System, as well as other tools and databases. The SKMS stores, manages, updates, and presents all information that an IT Service Provider needs to manage the full Lifecycle of IT Services." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Service Knowledge Management System" ;
