    skos:prefLabel "Status Accounting" ;
    skos:related itil:Activity,
        itil:Configuration,
        itil:Configuration_Item,
        itil:Lifecycle .

itil:Storage_Management a skos:concept ;
    rdfs:label "Storage Management" ;
    skos:definition "The Process responsible for managing the storage and maintenance of data throughout its Lifecycle." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Storage Management" ;
    skos:related itil:Lifecycle,
        itil:Process .

