        itil:Service .

itil:Repair a skos:concept ;
    rdfs:label "Repair" ;
    skos:definition "The replacement or correction of a failed Configuration Item." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Repair" ;
    skos:related itil:Configuration,
        itil:Configuration_Item .

itil:Root_Cause a skos:concept ;
    rdfs:label "Root Cause" ;
    skos:definition "The underlying or original cause of an Incident or Problem" ;
