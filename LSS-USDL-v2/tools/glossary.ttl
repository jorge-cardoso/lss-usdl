        itil:Service_Provider .

itil:Internal_Service_Provider a skos:concept ;
    rdfs:label "Internal Service Provider" ;
    skos:definition "An IT Service Provider which is part of the same Organisation as their Customer. An IT Service Provider may have both Internal Customers and External Customers. See Type I Service Provider, Type II Service Provider, Insource." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Internal Service Provider" ;
    skos:related itil:Customer,
