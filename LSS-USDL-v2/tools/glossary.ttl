        itil:Internal_Customer,
        itil:Organisation,
        itil:Service,
        itil:Service_Provider .

itil:Service_Transition a skos:concept ;
    rdfs:label "Service Transition" ;
    skos:definition "A stage in the Lifecycle of an IT Service. Service Transition includes a number of Processes and Functions and is the title of one of the Core ITIL publications." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Service Transition" ;
