itil:Agreement a skos:concept ;
    rdfs:label "Agreement" ;
    skos:definition "A Document that describes a formal understanding between two or more parties. An Agreement is not legally binding, unless it forms part of a Contract. See Service Level Agreement, Operational Level Agreement." ;
    skos:prefLabel "Agreement" ;
