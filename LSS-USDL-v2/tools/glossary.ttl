        itil:Resource,
        itil:Service,
        itil:Service_Strategy,
        itil:Strategy .

itil:Capacity_Plan a skos:concept ;
    rdfs:label "Capacity Plan" ;
    skos:definition "A Capacity Plan is used to manage the Resources required to deliver IT Services. The Plan contains scenarios for different predictions of Business demand, and costed options to deliver the agreed Service Level Targets." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Capacity Plan" ;
    skos:related itil:Business,
        itil:Capacity,
        itil:Capacity_Plan,
        itil:IT_Service,
        itil:Plan,
        itil:Resource,
        itil:Service,
        itil:Service_Level,
        itil:Service_Level_Target .

itil:Configuration_Record a skos:concept ;
    rdfs:label "Configuration Record" ;
    skos:definition "A Record containing the details of a Configuration Item. Each Configuration Record documents the Lifecycle of a single CI. Configuration Records are stored in a Configuration Management Database." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Configuration Record" ;
    skos:related itil:Configuration,
        itil:Configuration_Item,
        itil:Configuration_Management,
        itil:Configuration_Management_Database,
        itil:Configuration_Record,
        itil:Lifecycle,
        itil:Record .

itil:Core_Service a skos:concept ;
    rdfs:label "Core Service" ;
    skos:definition "Service Strategy) An IT Service that delivers basic Outcomes desired by one or more Customers. See Supporting Service, Core Service Package." ;
    skos:prefLabel "Core Service" ;
    skos:related itil:Core_Service,
        itil:Core_Service_Package,
        itil:Customer,
        itil:IT_Service,
        itil:Outcome,
