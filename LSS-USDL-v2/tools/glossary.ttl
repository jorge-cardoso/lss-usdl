        itil:Test,
        itil:Validation .

itil:Support_Group a skos:concept ;
    rdfs:label "Support Group" ;
    skos:definition "A group of people with technical skills. Support Groups provide the Technical Support needed by all of the IT Service Management Processes. See Technical Management" ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Support Group" ;
