        itil:Problem_Management,
        itil:Root_Cause,
        itil:Supplier,
        itil:Workaround .

itil:Live_Environment a skos:concept ;
    rdfs:label "Live Environment" ;
    skos:definition "A controlled Environment containing Live Configuration Items used to deliver IT Services to Customers." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Live Environment" ;
    skos:related itil:Configuration,
        itil:Configuration_Item,
