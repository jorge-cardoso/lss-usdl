itil: dc:creator itil:jcardoso .

itil:Analytical_Modelling a skos:concept ;
    rdfs:label "Analytical Modelling" ;
    skos:definition "A technique that uses mathematical Models to predict the behaviour of a Configuration Item or IT Service. Analytical Models are commonly used in Capacity Management and Availability Management." ;
    skos:member itil:Continual_Service_Improvement,
        itil:Service_Design,
        itil:Service_Strategy ;
    skos:prefLabel "Analytical Modelling" ;
    skos:related itil:Availability,
        itil:Availability_Management,
        itil:Capacity,
        itil:Capacity_Management,
        itil:Configuration,
        itil:Configuration_Item,
        itil:IT_Service,
        itil:Model,
        itil:Service .

itil:Application_Management a skos:concept ;
    rdfs:label "Application Management" ;
    skos:definition "A database or structured Document used to manage Applications throughout their Lifecycle. The Application Portfolio contains key Attributes of all Applications. The Application Portfolio is sometimes implemented as part of the Service Portfolio, or as part of the Configuration Management System." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Application Management" ;
    skos:related itil:Application,
        itil:Application_Portfolio,
        itil:Attribute,
        itil:Configuration,
        itil:Configuration_Management,
        itil:Configuration_Management_System,
        itil:Document,
        itil:Lifecycle,
        itil:Management_System,
        itil:Service,
        itil:Service_Portfolio,
        itil:System .

itil:Application_Service_Provider a skos:concept ;
    rdfs:label "Application Service Provider" ;
    skos:definition "The Function responsible for managing Applications throughout their Lifecycle." ;
    skos:member itil:Service_Design,
        itil:Service_Operation ;
    skos:prefLabel "Application Service Provider" ;
    skos:related itil:Application,
        itil:Function,
        itil:Lifecycle .

itil:Application_Sizing a skos:concept ;
    rdfs:label "Application Sizing" ;
    skos:definition "An External Service Provider that provides IT Services using Applications running at the Service Provider's premises. Users access the Applications by network connections to the Service Provider." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Application Sizing" ;
    skos:related itil:Application,
        itil:External_Service_Provider,
        itil:IT_Service,
        itil:Service,
        itil:Service_Provider,
        itil:User .

itil:Authority_Matrix a skos:concept ;
    rdfs:label "Authority Matrix" ;
    skos:definition "Synonym f√ºr RACI" ;
    skos:prefLabel "Authority Matrix" ;
    skos:related itil:RACI .

itil:Availability_Management_Information_System a skos:concept ;
    rdfs:label "Availability Management Information System" ;
    skos:definition "virtual repository of all Availability Management data, usually stored in multiple physical locations.  See Service Knowledge Management System." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Availability Management Information System" ;
    skos:related itil:Availability,
        itil:Availability_Management,
        itil:Knowledge_Management,
        itil:Management_System,
        itil:Service,
        itil:Service_Knowledge_Management_System,
        itil:System .

itil:Availability_Plan a skos:concept ;
    rdfs:label "Availability Plan" ;
    skos:definition "A Plan to ensure that existing and future Availability Requirements for IT Services can be provided Cost Effectively." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Availability Plan" ;
    skos:related itil:Availability,
        itil:Cost,
        itil:IT_Service,
        itil:Plan,
        itil:Requirement,
        itil:Service .

itil:Back-out a skos:concept ;
    rdfs:label "Back-out" ;
    skos:definition "Synonym for Remediation" ;
    skos:prefLabel "Back-out" ;
    skos:related itil:Remediation .

itil:British_Standards_Institution a skos:concept ;
    rdfs:label "British Standards Institution" ;
    skos:definition "The UK National Standards body, responsible for creating and maintaining British Standards. See http://www.bsi-global.com for more information." ;
    skos:prefLabel "British Standards Institution" ;
    skos:related itil:Standard .

itil:Build_Environment a skos:concept ;
    rdfs:label "Build Environment" ;
    skos:definition "controlled Environment where Applications, IT Services and other Builds are assembled prior to being moved into a Test or Live Environment." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Build Environment" ;
    skos:related itil:Application,
        itil:Build,
        itil:Environment,
        itil:IT_Service,
        itil:Live,
        itil:Live_Environment,
        itil:Service,
        itil:Test .

itil:Business_Operations a skos:concept ;
    rdfs:label "Business Operations" ;
    skos:definition "The day-to-day execution, monitoring and management of Business Processes." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Business Operations" ;
    skos:related itil:Business,
        itil:Business_Process,
        itil:Process .

itil:Business_Perspective a skos:concept ;
    rdfs:label "Business Perspective" ;
    skos:definition "An understanding of the Service Provider and IT Services from the point of view of the Business, and an understanding of the Business from the point of view of the Service Provider." ;
    skos:member itil:Continual_Service_Improvement ;
    skos:prefLabel "Business Perspective" ;
    skos:related itil:Business,
        itil:IT_Service,
        itil:Service,
        itil:Service_Provider .

itil:Business_Service_Management a skos:concept ;
    rdfs:label "Business Service Management" ;
    skos:definition "An IT Service that directly supports a Business Process, as opposed to an Infrastructure Service which is used internally by the IT Service Provider and is not usually visible to the Business. The term Business Service is also used to mean a Service that is delivered to Business Customers by Business Units. Successful delivery of Business Services often depends on one or more IT Services." ;
    skos:example "For example delivery of financial services to Customers of a bank, or goods to the Customers of a retail store." ;
    skos:member itil:Service_Design,
        itil:Service_Strategy ;
    skos:prefLabel "Business Service Management" ;
    skos:related itil:Business,
        itil:Business_Customer,
        itil:Business_Process,
        itil:Business_Service,
        itil:Business_Unit,
        itil:Customer,
        itil:IT_Service,
        itil:IT_Service_Provider,
        itil:Infrastructure_Service,
        itil:Process,
        itil:Service,
        itil:Service_Provider .

itil:Call_Centre a skos:concept ;
    rdfs:label "Call Centre" ;
    skos:definition "An Organisation or Business Unit which handles large numbers of incoming and outgoing telephone calls. See Service Desk." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Call Centre" ;
    skos:related itil:Business,
        itil:Business_Unit,
        itil:Organisation,
        itil:Service,
        itil:Service_Desk .

itil:Capacity_Management_Information_System a skos:concept ;
    rdfs:label "Capacity Management Information System" ;
    skos:definition "A virtual repository of all Capacity Management data, usually stored in multiple physical locations. See Service Knowledge Management System." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Capacity Management Information System" ;
    skos:related itil:Capacity,
        itil:Capacity_Management,
        itil:Knowledge_Management,
        itil:Management_System,
        itil:Service,
        itil:Service_Knowledge_Management_System,
        itil:System .

itil:Capacity_Planning a skos:concept ;
    rdfs:label "Capacity Planning" ;
    skos:definition "Activity within Capacity Management responsible for creating a Capacity Plan." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Capacity Planning" ;
    skos:related itil:Activity,
        itil:Capacity,
        itil:Capacity_Management,
        itil:Capacity_Plan,
        itil:Plan .

itil:Capital_Item a skos:concept ;
    rdfs:label "Capital Item" ;
    skos:definition "An Asset that is of interest to Financial Management because it is above an agreed financial value." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Capital Item" ;
    skos:related itil:Asset,
        itil:Financial_Management .

itil:Capitalization a skos:concept ;
    rdfs:label "Capitalization" ;
    skos:definition "Identifying major Cost as capital, even though no Asset is purchased. This is done to spread the impact of the Cost over multiple accounting periods." ;
    skos:example "The most common example of this is software development, or purchase of a software license." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Capitalization" ;
    skos:related itil:Asset,
        itil:Cost .

itil:Change_Request a skos:concept ;
    rdfs:label "Change Request" ;
    skos:definition "Synonym for Request for Change." ;
    skos:prefLabel "Change Request" ;
    skos:related itil:Change,
        itil:Request_for_Change .

itil:Chronological_Analysis a skos:concept ;
    rdfs:label "Chronological Analysis" ;
    skos:definition "A technique used to help identify possible causes of Problems. All available data about the Problem is collected and sorted by date and time to provide a detailed timeline. This can make it possible to identify which Events may have been triggered by others." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Chronological Analysis" ;
    skos:related itil:Event,
        itil:Problem .

itil:Commercial_off_the_Shelf a skos:concept ;
    rdfs:label "Commercial off the Shelf" ;
    skos:definition "Service Design) Application software or Middleware that can be purchased from a Third Party." ;
    skos:prefLabel "Commercial off the Shelf" ;
    skos:related itil:Application,
        itil:Design,
        itil:Middleware,
        itil:Service,
        itil:Service_Design,
        itil:Third_Party .

itil:Component_CI a skos:concept ;
    rdfs:label "Component CI" ;
    skos:definition "A Configuration Item that is part of an Assembly." ;
    skos:example "For example, a CPU or Memory CI may be part of a Server CI." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Component CI" ;
    skos:related itil:Assembly,
        itil:Configuration,
        itil:Configuration_Item .

itil:Component_Failure_Impact_Analysis a skos:concept ;
    rdfs:label "Component Failure Impact Analysis" ;
    skos:definition "A technique that helps to identify the impact of CI failure on IT Services. A matrix is created with IT Services on one edge and CIs on the other. This enables the identification of critical CIs (that could cause the failure of multiple IT Services) and of fragile IT Services (that have multiple Single Points of Failure)." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Component Failure Impact Analysis" ;
    skos:related itil:Failure,
        itil:IT_Service,
        itil:Service .

itil:Computer_Telephony_Integration a skos:concept ;
    rdfs:label "Computer Telephony Integration" ;
    skos:definition "CTI is a general term covering any kind of integration between computers and telephone Systems. It is most commonly used to refer to Systems where an Application displays detailed screens relating to incoming or outgoing telephone calls. See Automatic Call Distribution, Interactive Voice Response." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Computer Telephony Integration" ;
    skos:related itil:Application,
        itil:Automatic_Call_Distribution,
        itil:Call,
        itil:Interactive_Voice_Response,
        itil:System .

itil:Concurrency a skos:concept ;
    rdfs:label "Concurrency" ;
    skos:definition "A measure of the number of Users engaged in the same Operation at the same time." ;
    skos:prefLabel "Concurrency" ;
    skos:related itil:Operation,
        itil:User .

itil:Configuration_Control a skos:concept ;
    rdfs:label "Configuration Control" ;
    skos:definition "The Activity responsible for ensuring that adding, modifying or removing a CI is properly managed, for example by submitting a Request for Change or Service Request." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Configuration Control" ;
    skos:related itil:Activity,
        itil:Change,
        itil:Request_for_Change,
        itil:Service,
        itil:Service_Request .

itil:Configuration_Structure a skos:concept ;
    rdfs:label "Configuration Structure" ;
    skos:definition "The hierarchy and other Relationships between all the Configuration Items that comprise a Configuration." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Configuration Structure" ;
    skos:related itil:Configuration,
        itil:Configuration_Item,
        itil:Relationship .

itil:Continuous_Availability a skos:concept ;
    rdfs:label "Continuous Availability" ;
    skos:definition "An approach or design to achieve 100% Availability. A Continuously Available IT Service has no planned or unplanned Downtime." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Continuous Availability" ;
    skos:related itil:Availability,
        itil:Downtime,
        itil:IT_Service,
        itil:Service .

itil:Continuous_Operation a skos:concept ;
    rdfs:label "Continuous Operation" ;
    skos:definition "An approach or design to eliminate planned Downtime of an IT Service. Note that individual Configuration Items may be down even though the IT Service is Available." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Continuous Operation" ;
    skos:related itil:Configuration,
        itil:Configuration_Item,
        itil:Downtime,
        itil:IT_Service,
        itil:Service .

itil:Control_Processes a skos:concept ;
    rdfs:label "Control Processes" ;
    skos:definition "The ISO/IEC 20000 Process group that includes Change Management and Configuration Management." ;
    skos:prefLabel "Control Processes" ;
    skos:related itil:Change,
        itil:Change_Management,
        itil:Configuration,
        itil:Configuration_Management,
        <http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_20000>,
        itil:Process .

itil:Control_perspective a skos:concept ;
    rdfs:label "Control perspective" ;
    skos:definition "An approach to the management of IT Services, Processes, Functions, Assets etc. There can be several different Control Perspectives on the same IT Service, Process etc., allowing different individuals or teams to focus on what is important and relevant to their specific Role." ;
    skos:example "Example Control Perspectives include Reactive and Proactive management within IT Operations, or a Lifecycle view for an Application Project team." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Control perspective" ;
    skos:related itil:Asset,
        itil:Control,
        itil:Function,
        itil:IT_Service,
        itil:Process,
        itil:Role,
        itil:Service .

itil:Cost_Center a skos:concept ;
    rdfs:label "Cost Center" ;
    skos:definition "A Business Unit or Project to which Costs are assigned. A Cost Centre does not charge for Services provided. An IT Service Provider can be run as a Cost Centre or a Profit Centre." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Cost Center" ;
    skos:related itil:Business,
        itil:Business_Unit,
        itil:Cost,
        itil:IT_Service,
        itil:IT_Service_Provider,
        itil:Profit_Centre,
        itil:Project,
        itil:Service,
        itil:Service_Provider .

itil:Cost_Management a skos:concept ;
    rdfs:label "Cost Management" ;
    skos:definition "A general term that is used to refer to Budgeting and Accounting, sometimes used as a synonym for Financial Management" ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Cost Management" ;
    skos:related itil:Accounting,
        itil:Budget,
        itil:Budgeting,
        itil:Financial_Management .

itil:Course_Corrections a skos:concept ;
    rdfs:label "Course Corrections" ;
    skos:definition "Changes made to a Plan or Activity that has already started, to ensure that it will meet its Objectives. Course corrections are made as a result of Monitoring progress." ;
    skos:prefLabel "Course Corrections" ;
    skos:related itil:Activity,
        itil:Change,
        itil:Monitoring,
        itil:Objective,
        itil:Plan .

itil:Data-to-Information-to-Knowledge-to-Wisdom a skos:concept ;
    rdfs:label "Data-to-Information-to-Knowledge-to-Wisdom" ;
    skos:definition "A way of understanding the relationships between data, information, knowledge, and wisdom. DIKW shows how each of these builds on the others." ;
    skos:prefLabel "Data-to-Information-to-Knowledge-to-Wisdom" .

itil:Definitive_Media_Library a skos:concept ;
    rdfs:label "Definitive Media Library" ;
    skos:definition "One or more locations in which the definitive and approved versions of all software Configuration Items are securely stored. The DML may also contain associated CIs such as licenses and documentation. The DML is a single logical storage area even if there are multiple locations. All software in the DML is under the control of Change and Release Management and is recorded in the Configuration Management System. Only software from the DML is acceptable for use in a Release." ;
    skos:member itil:Service_Transition ;
    skos:prefLabel "Definitive Media Library" ;
    skos:related itil:Change,
        itil:Configuration,
        itil:Configuration_Item,
        itil:Configuration_Management,
        itil:Configuration_Management_System,
        itil:Management_System,
        itil:Release,
        itil:Release_Management,
        itil:System .

itil:Dependency a skos:concept ;
    rdfs:label "Dependency" ;
    skos:definition "The direct or indirect reliance of one Process or Activity upon another." ;
    skos:prefLabel "Dependency" ;
    skos:related itil:Activity,
        itil:Process .

itil:Directory_Service a skos:concept ;
    rdfs:label "Directory Service" ;
    skos:definition "An Application that manages information about IT Infrastructure available on a network, and corresponding User access Rights." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Directory Service" ;
    skos:related itil:Application,
        itil:IT_Infrastructure,
        itil:Rights,
        itil:User .

itil:Driver a skos:concept ;
    rdfs:label "Driver" ;
    skos:definition "Something that influences Strategy, Objectives or Requirements." ;
    skos:example "For example new legislation or the actions of competitors." ;
    skos:prefLabel "Driver" ;
    skos:related itil:Objective,
        itil:Requirement,
        itil:Strategy .

itil:Economies_of_scale a skos:concept ;
    rdfs:label "Economies of scale" ;
    skos:definition "Service Strategy) The reduction in average Cost that is possible from increasing the usage of an IT Service or Asset. See Economies of Scope." ;
    skos:prefLabel "Economies of scale" ;
    skos:related itil:Asset,
        itil:Cost,
        itil:IT_Service,
        itil:Scope,
        itil:Service,
        itil:Service_Strategy,
        itil:Strategy .

itil:Economies_of_scope a skos:concept ;
    rdfs:label "Economies of scope" ;
    skos:definition "The reduction in Cost that is allocated to an IT Service by using an existing Asset for an additional purpose. See Economies of Scale." ;
    skos:example "For example delivering a new IT Service from existing IT Infrastructure." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Economies of scope" ;
    skos:related itil:Asset,
        itil:Cost,
        itil:IT_Service,
        itil:Service .

itil:Exception_Report a skos:concept ;
    rdfs:label "Exception Report" ;
    skos:definition "A Document containing details of one or more KPIs or other important targets that have exceeded defined Thresholds." ;
    skos:example "Examples include SLA targets being missed or about to be missed, and a Performance Metric indicating a potential Capacity problem." ;
    skos:prefLabel "Exception Report" ;
    skos:related itil:Document,
        itil:Threshold .

itil:External_Sourcing a skos:concept ;
    rdfs:label "External Sourcing" ;
    skos:definition "Synonym for Outsourcing." ;
    skos:prefLabel "External Sourcing" ;
    skos:related itil:Outsourcing .

itil:Failure_Modes_and_Effects_Analysis a skos:concept ;
    rdfs:label "Failure Modes and Effects Analysis" ;
    skos:definition "An approach to assessing the potential Impact of Failures. FMEA involves analysing what would happen after Failure of each Configuration Item, all the way up to the effect on the Business. FMEA is often used in Information Security Management and in IT Service Continuity Planning." ;
    skos:prefLabel "Failure Modes and Effects Analysis" ;
    skos:related itil:Business,
        itil:Configuration,
        itil:Configuration_Item,
        itil:Failure,
        itil:IT_Service,
        itil:IT_Service_Continuity_Plan,
        itil:Impact,
        itil:Information_Security_Management,
        itil:Plan,
        itil:Planning,
        itil:Security,
        itil:Security_Management,
        itil:Service .

itil:First-line_Support a skos:concept ;
    rdfs:label "First-line Support" ;
    skos:definition "The first level in a hierarchy of Support Groups involved in the resolution of Incidents. Each level contains more specialist skills, or has more time or other Resources. See Escalation." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "First-line Support" ;
    skos:related itil:Escalation,
        itil:Incident,
        itil:Resource,
        itil:Support_Group .

itil:Fishbone_Diagram a skos:concept ;
    rdfs:label "Fishbone Diagram" ;
    skos:definition "Synonym for Ishikawa Diagram." ;
    skos:prefLabel "Fishbone Diagram" ;
    skos:related itil:Ishikawa_Diagram .

itil:Fixed_Cost a skos:concept ;
    rdfs:label "Fixed Cost" ;
    skos:definition "A Cost that does not vary with IT Service usage. See Variable Cost." ;
    skos:example "For example the cost of Server hardware." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "Fixed Cost" ;
    skos:related itil:Cost,
        itil:IT_Service,
        itil:Service,
        itil:Variable_Cost .

itil:Follow_the_Sun a skos:concept ;
    rdfs:label "Follow the Sun" ;
    skos:definition "A methodology for using Service Desks and Support Groups around the world to provide seamless 24 * 7 Service. Calls, Incidents, Problems and Service Requests are passed between groups in different time zones." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Follow the Sun" ;
    skos:related itil:Call,
        itil:Incident,
        itil:Problem,
        itil:Service,
        itil:Service_Desk,
        itil:Service_Request,
        itil:Support_Group .

<http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_27001> a skos:concept ;
    rdfs:label "ISO/IEC 27001" ;
    skos:definition "ISO Specification for Information Security Management. The corresponding Code of Practice is ISO/IEC 17799." ;
    skos:member itil:Continual_Service_Improvement,
        itil:Service_Design ;
    skos:prefLabel "ISO/IEC 27001" ;
    skos:related itil:Code_of_Practice,
        <http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_17799>,
        itil:Information_Security_Management,
        itil:Practice,
        itil:Security,
        itil:Security_Management,
        itil:Specification .

itil:ISO_9001 a skos:concept ;
    rdfs:label "ISO 9001" ;
    skos:definition "An international Standard for Quality Management Systems." ;
    skos:prefLabel "ISO 9001" ;
    skos:related itil:Management_System,
        itil:Quality,
        itil:Quality_Management_System,
        itil:Standard,
        itil:System .

itil:IT_Directorate a skos:concept ;
    rdfs:label "IT Directorate" ;
    skos:definition "Senior Management within a Service Provider, charged with developing and delivering IT services. Most commonly used in UK Government departments." ;
    skos:member itil:Continual_Service_Improvement ;
    skos:prefLabel "IT Directorate" ;
    skos:related itil:Service,
        itil:Service_Provider .

itil:Information_Security_Management_System a skos:concept ;
    rdfs:label "Information Security Management System" ;
    skos:definition "The framework of Policy, Processes, Standards, Guidelines and tools that ensures an Organisation can achieve its Information Security Management Objectives." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Information Security Management System" ;
    skos:related itil:Guideline,
        itil:Information_Security_Management,
        itil:Objective,
        itil:Organisation,
        itil:Policy,
        itil:Process,
        itil:Security,
        itil:Security_Management,
        itil:Standard .

itil:Insourcing a skos:concept ;
    rdfs:label "Insourcing" ;
    skos:definition "Synonym for Internal Sourcing" ;
    skos:prefLabel "Insourcing" ;
    skos:related itil:Internal_Sourcing .

itil:International_Standards_Organisation a skos:concept ;
    rdfs:label "International Standards Organisation" ;
    skos:definition "See International Organization for Standardization (ISO)" ;
    skos:prefLabel "International Standards Organisation" ;
    skos:related itil:International_Organization_for_Standardization,
        itil:Standard .

itil:Internet_Service_Provider a skos:concept ;
    rdfs:label "Internet Service Provider" ;
    skos:definition "An External Service Provider that provides access to the Internet. Most ISPs also provide other IT Services such as web hosting." ;
    skos:prefLabel "Internet Service Provider" ;
    skos:related itil:External_Service_Provider,
        itil:IT_Service,
        itil:Service,
        itil:Service_Provider .

itil:Job_Description a skos:concept ;
    rdfs:label "Job Description" ;
    skos:definition "A Document which defines the Roles, responsibilities, skills and knowledge required by a particular person." ;
    skos:example "One Job Description can include multiple Roles, for example the Roles of Configuration Manager and Change Manager may be carried out by one person." ;
    skos:prefLabel "Job Description" ;
    skos:related itil:Document,
        itil:Role .

<http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#Kepner_&_Tregoe_Analysis> a skos:concept ;
    rdfs:label "Kepner &amp; Tregoe Analysis" ;
    skos:definition "A structured approach to Problem solving. The Problem is analysed in terms of what, where, when and extent. Possible causes are identified. The most probable cause is tested. The true cause is verified." ;
    skos:member itil:Continual_Service_Improvement,
        itil:Service_Operation ;
    skos:prefLabel "Kepner &amp; Tregoe Analysis" ;
    skos:related itil:Problem .

itil:Knowledge_Base a skos:concept ;
    rdfs:label "Knowledge Base" ;
