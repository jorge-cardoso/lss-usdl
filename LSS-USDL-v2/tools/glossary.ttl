        itil:Test .

itil:IT_Service_Provider a skos:concept ;
    rdfs:label "IT Service Provider" ;
    skos:definition "A Service Provider that provides IT Services to Internal Customers or External Customers." ;
    skos:member itil:Service_Strategy ;
    skos:prefLabel "IT Service Provider" ;
    skos:related itil:Customer,
        itil:External_Customer,
        itil:IT_Service,
        itil:Internal_Customer,
        itil:Service,
