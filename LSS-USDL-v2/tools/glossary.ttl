        itil:Service_Provider,
        itil:Transaction .

itil:Restoration_of_Service a skos:concept ;
    rdfs:label "Restoration of Service" ;
    skos:definition "See Restore" ;
    skos:prefLabel "Restoration of Service" ;
    skos:related itil:Restore .

itil:Return_to_Normal a skos:concept ;
    rdfs:label "Return to Normal" ;
    skos:definition "The phase of an IT Service Continuity Plan during which full normal operations are resumed." ;
    skos:example "For example, if an alternate data centre has been in use, then this phase will bring the primary data centre back into operation, and restore the ability to invoke IT Service Continuity Plans again." ;
    skos:member itil:Service_Design ;
    skos:prefLabel "Return to Normal" ;
    skos:related itil:IT_Service,
        itil:IT_Service_Continuity_Plan,
        itil:Plan,
        itil:Service .

<http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#Root_Cause_Analysis_(RCA)> a skos:concept ;
    rdfs:label "Root Cause Analysis (RCA)" ;
    skos:definition "An Activity that identifies the Root Cause of a Incident or Problem. RCA typically concentrates on IT Infrastructure failures." ;
    skos:member itil:Service_Operation ;
    skos:prefLabel "Root Cause Analysis (RCA)" ;
    skos:related itil:Activity,
        itil:IT_Infrastructure,
        itil:Incident,
        itil:Problem,
        itil:Root_Cause .

itil:Running_Costs a skos:concept ;
