itilglos:Core_Service_Package a skos:concept ;
    rdfs:label "Core Service Package" ;
    skos:definition "A detailed description of a Core Service that may be shared by two or more Service Level Packages. See Service Package." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Core Service Package" ;
    skos:related itilglos:Core_Service,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Package,
        itilglos:Service_Package .

itilglos:Cost_Type a skos:concept ;
    rdfs:label "Cost Type" ;
    skos:definition "The highest level of category to which Costs are assigned in Budgeting and Accounting." ;
    skos:example "For example hardware, software, people, accommodation, external and Transfer." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Cost Type" ;
    skos:related itilglos:Accounting,
        itilglos:Budget,
        itilglos:Budgeting,
        itilglos:Cost .

itilglos:Crisis_Management a skos:concept ;
    rdfs:label "Crisis Management" ;
    skos:definition "The Process responsible for managing the wider implications of Business Continuity. A Crisis Management team is responsible for Strategic issues such as managing media relations and shareholder confidence, and decides when to invoke Business Continuity Plans." ;
    skos:prefLabel "Crisis Management" ;
    skos:related itilglos:Business,
        itilglos:Business_Continuity_Plan,
        itilglos:Crisis_Management,
        itilglos:Plan,
        itilglos:Process,
        itilglos:Strategic .

itilglos:Critical_Success_Factor a skos:concept ;
    rdfs:label "Critical Success Factor" ;
    skos:definition "Something that must happen if a Process, Project, Plan, or IT Service is to succeed. KPIs are used to measure the achievement of each CSF." ;
    skos:example "For example a CSF of \"protect IT Services when making Changes\" could be measured by KPIs such as \"percentage reduction of unsuccessful Changes\", \"percentage reduction in Changes causing Incidents\" etc." ;
    skos:prefLabel "Critical Success Factor" ;
    skos:related itilglos:IT_Service,
        itilglos:Plan,
        itilglos:Process,
        itilglos:Project,
        itilglos:Service .

itilglos:Customer_Portfolio a skos:concept ;
    rdfs:label "Customer Portfolio" ;
    skos:definition "A database or structured Document used to record all Customers of the IT Service Provider. The Customer Portfolio is the Business Relationship Manager's view of the Customers who receive Services from the IT Service Provider." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Customer Portfolio" ;
    skos:related itilglos:Business,
        itilglos:Business_Relationship_Manager,
        itilglos:Customer,
        itilglos:Customer_Portfolio,
        itilglos:Document,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Relationship,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Dashboard a skos:concept ;
    rdfs:label "Dashboard" ;
    skos:definition "A graphical representation of overall IT Service Performance and Availability. Dashboard images may be updated in real-time, and can also be included in management reports and web pages. Dashboards can be used to support Service Level Management, Event Management or Incident Diagnosis." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Dashboard" ;
    skos:related itilglos:Availability,
        itilglos:Dashboard,
        itilglos:Diagnosis,
        itilglos:Event,
        itilglos:Event_Management,
        itilglos:IT_Service,
        itilglos:Incident,
        itilglos:Performance,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Management .

itilglos:Depreciation a skos:concept ;
    rdfs:label "Depreciation" ;
    skos:definition "A measure of the reduction in value of an Asset over its life. This is based on wearing out, consumption or other reduction in the useful economic value." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Depreciation" ;
    skos:related itilglos:Asset .

itilglos:Development_Environment a skos:concept ;
    rdfs:label "Development Environment" ;
    skos:definition "An Environment used to create or modify IT Services or Applications. Development Environments are not typically subjected to the same degree of control as Test Environments or Live Environments. See Development." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Development Environment" ;
    skos:related itilglos:Application,
        itilglos:Development,
        itilglos:Development_Environment,
        itilglos:Environment,
        itilglos:IT_Service,
        itilglos:Live,
        itilglos:Live_Environment,
        itilglos:Service,
        itilglos:Test,
        itilglos:Test_Environment .

itilglos:Diagnostic_Script a skos:concept ;
    rdfs:label "Diagnostic Script" ;
    skos:definition "A structured set of questions used by Service Desk staff to ensure they ask the correct questions, and to help them Classify, Resolve and assign Incidents. Diagnostic Scripts may also be made available to Users to help them diagnose and resolve their own Incidents." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Diagnostic Script" ;
    skos:related itilglos:Diagnostic_Script,
        itilglos:Incident,
        itilglos:Service,
        itilglos:Service_Desk,
        itilglos:User .

itilglos:Differential_Charging a skos:concept ;
    rdfs:label "Differential Charging" ;
    skos:definition "A technique used to support Demand Management by charging different amounts for the same IT Service Function at different times." ;
    skos:prefLabel "Differential Charging" ;
    skos:related itilglos:Demand_Management,
        itilglos:Function,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:Direct_Cost a skos:concept ;
    rdfs:label "Direct Cost" ;
    skos:definition "cost of providing an IT Service which can be allocated in full to a specific Customer, Cost Centre, Project etc. See Indirect Cost" ;
    skos:example "For example cost of providing non-shared servers or software licenses." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Direct Cost" ;
    skos:related itilglos:Cost,
        itilglos:Customer,
        itilglos:IT_Service,
        itilglos:Indirect_Cost,
        itilglos:Project,
        itilglos:Service .

itilglos:Do_Nothing a skos:concept ;
    rdfs:label "Do Nothing" ;
    skos:definition "A Recovery Option. The Service Provider formally agrees with the Customer that Recovery of this IT Service will not be performed." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Do Nothing" ;
    skos:related itilglos:Customer,
        itilglos:IT_Service,
        itilglos:Recovery,
        itilglos:Recovery_Option,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Early_Life_Support a skos:concept ;
    rdfs:label "Early Life Support" ;
    skos:definition "Support provided for a new or Changed IT Service for a period of time after it is Released. During Early Life Support the IT Service Provider may review the KPIs, Service Levels and Monitoring Thresholds, and provide additional Resources for Incident and Problem Management." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Early Life Support" ;
    skos:related itilglos:Change,
        itilglos:Early_Life_Support,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Incident,
        itilglos:Monitoring,
        itilglos:Problem,
        itilglos:Problem_Management,
        itilglos:Release,
        itilglos:Resource,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Provider,
        itilglos:Threshold .

itilglos:Emergency_Change_Advisory_Board a skos:concept ;
    rdfs:label "Emergency Change Advisory Board" ;
    skos:definition "A sub-set of the Change Advisory Board who make decisions about high impact Emergency Changes. Membership of the ECAB may be decided at the time a meeting is called, and depends on the nature of the Emergency Change." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Emergency Change Advisory Board" ;
    skos:related itilglos:Change,
        itilglos:Change_Advisory_Board,
        itilglos:Emergency_Change .

itilglos:Estimation a skos:concept ;
    rdfs:label "Estimation" ;
    skos:definition "The use of experience to provide an approximate value for a Metric or Cost. Estimation is also used in Capacity and Availability Management as the cheapest and least accurate Modelling method." ;
    skos:prefLabel "Estimation" ;
    skos:related itilglos:Availability,
        itilglos:Availability_Management,
        itilglos:Capacity,
        itilglos:Cost,
        itilglos:Estimation,
        itilglos:Metric,
        itilglos:Model,
        itilglos:Modelling .

itilglos:Facilities_Management a skos:concept ;
    rdfs:label "Facilities Management" ;
    skos:definition "The Function responsible for managing the physical Environment where the IT Infrastructure is located." ;
    skos:example "Facilities Management includes all aspects of managing the physical Environment, for example power and cooling, building Access Management, and environmental Monitoring." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Facilities Management" ;
    skos:related itilglos:Environment,
        itilglos:Function,
        itilglos:IT_Infrastructure .

itilglos:Fault_Tree_Analysis a skos:concept ;
    rdfs:label "Fault Tree Analysis" ;
    skos:definition "A technique that can be used to determine the chain of Events that leads to a Problem. Fault Tree Analysis represents a chain of Events using Boolean notation in a diagram." ;
    skos:member itilproc:Continual_Service_Improvement,
        itilproc:Service_Design ;
    skos:prefLabel "Fault Tree Analysis" ;
    skos:related itilglos:Event,
        itilglos:Fault,
        itilglos:Fault_Tree_Analysis,
        itilglos:Problem .

itilglos:Fulfilment a skos:concept ;
    rdfs:label "Fulfilment" ;
    skos:definition "Performing Activities to meet a need or Requirement." ;
    skos:example "For example by providing a new IT Service, or meeting a Service Request." ;
    skos:prefLabel "Fulfilment" ;
    skos:related itilglos:Requirement .

itilglos:Functional_Escalation a skos:concept ;
    rdfs:label "Functional Escalation" ;
    skos:definition "Transferring an Incident, Problem or Change to a technical team with a higher level of expertise to assist in an Escalation." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Functional Escalation" ;
    skos:related itilglos:Change,
        itilglos:Escalation,
        itilglos:Incident,
        itilglos:Problem .

itilglos:Gap_Analysis a skos:concept ;
    rdfs:label "Gap Analysis" ;
    skos:definition "An Activity which compares two sets of data and identifies the differences. Gap Analysis is commonly used to compare a set of Requirements with actual delivery. See Benchmarking." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "Gap Analysis" ;
    skos:related itilglos:Activity,
        itilglos:Benchmark,
        itilglos:Benchmarking,
        itilglos:Gap_Analysis,
        itilglos:Requirement .

itilglos:Help_Desk a skos:concept ;
    rdfs:label "Help Desk" ;
    skos:definition "A point of contact for Users to log Incidents. A Help Desk is usually more technically focussed than a Service Desk and does not provide a Single Point of Contact for all interaction. The term Help Desk is often used as a synonym for Service Desk." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Help Desk" ;
    skos:related itilglos:Help_Desk,
        itilglos:Incident,
        itilglos:Service,
        itilglos:Service_Desk,
        itilglos:Single_Point_of_Contact,
        itilglos:User .

itilglos:Hierarchic_Escalation a skos:concept ;
    rdfs:label "Hierarchic Escalation" ;
    skos:definition "Informing or involving more senior levels of management to assist in an Escalation." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Hierarchic Escalation" ;
    skos:related itilglos:Escalation .

itilglos:High_Availability a skos:concept ;
    rdfs:label "High Availability" ;
    skos:definition "An approach or Design that minimises or hides the effects of Configuration Item Failure on the Users of an IT Service. High Availability solutions are Designed to achieve an agreed level of Availability and make use of techniques such as Fault Tolerance, Resilience and fast Recovery to reduce the number of Incidents, and the Impact of Incidents." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "High Availability" ;
    skos:related itilglos:Availability,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Design,
        itilglos:Failure,
        itilglos:Fault,
        itilglos:Fault_Tolerance,
        itilglos:High_Availability,
        itilglos:IT_Service,
        itilglos:Impact,
        itilglos:Incident,
        itilglos:Recovery,
        itilglos:Resilience,
        itilglos:Service,
        itilglos:User .

<http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_17799> a skos:concept ;
    rdfs:label "ISO/IEC 17799" ;
    skos:definition "ISO Code of Practice for Information Security Management." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "ISO/IEC 17799" ;
    skos:related itilglos:Code_of_Practice,
        itilglos:Information_Security_Management,
        itilglos:Practice,
        itilglos:Security,
        itilglos:Security_Management .

itilglos:ISO_9000 a skos:concept ;
    rdfs:label "ISO 9000" ;
    skos:definition "A generic term that refers to a number of international Standards and Guidelines for Quality Management Systems. See http://www.iso.org/ for more information." ;
    skos:prefLabel "ISO 9000" ;
    skos:related itilglos:Guideline,
        itilglos:Management_System,
        itilglos:Quality,
        itilglos:Quality_Management_System,
        itilglos:Standard,
        itilglos:System .

itilglos:IT_Service_Management_Forum a skos:concept ;
    rdfs:label "IT Service Management Forum" ;
    skos:definition "The IT Service Management Forum is an independent Organisation dedicated to promoting a professional approach to IT Service Management. The itSMF is a not-for-profit membership Organisation with representation in many countries around the world (itSMF Chapters). The itSMF and its membership contribute to the development of ITIL and associated IT Service Management Standards. See http://www.itsmf.com/ for more information." ;
    skos:prefLabel "IT Service Management Forum" ;
    skos:related itilglos:ITIL,
        itilglos:IT_Service,
        itilglos:IT_Service_Management,
        itilglos:IT_Service_Management_Forum,
        itilglos:Organisation,
        itilglos:Service,
        itilglos:Service_Management,
        itilglos:Standard .

itilglos:IT_Steering_Group a skos:concept ;
    rdfs:label "IT Steering Group" ;
    skos:definition "A formal group that is responsible for ensuring that Business and IT Service Provider Strategies and Plans are closely aligned. An IT Steering Group includes senior representatives from the Business and the IT Service Provider." ;
    skos:prefLabel "IT Steering Group" ;
    skos:related itilglos:Business,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:IT_Steering_Group,
        itilglos:Plan,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Indirect_Cost a skos:concept ;
    rdfs:label "Indirect Cost" ;
    skos:definition "A Cost of providing an IT Service which cannot be allocated in full to a specific Customer. Also known as Overhead. See Direct Cost." ;
    skos:example "For example Cost of providing shared Servers or software licenses." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Indirect Cost" ;
    skos:related itilglos:Cost,
        itilglos:Customer,
        itilglos:Direct_Cost,
        itilglos:IT_Service,
        itilglos:Overhead,
        itilglos:Service .

itilglos:Information_Security_Policy a skos:concept ;
    rdfs:label "Information Security Policy" ;
    skos:definition "The Policy that governs the Organisation&rsquo;s approach to Information Security Management." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Information Security Policy" ;
    skos:related itilglos:Information_Security_Management,
        itilglos:Organisation,
        itilglos:Policy,
        itilglos:Security,
        itilglos:Security_Management .

itilglos:Interactive_Voice_Response a skos:concept ;
    rdfs:label "Interactive Voice Response" ;
    skos:definition "A form of Automatic Call Distribution that accepts User input, such as key presses and spoken commands, to identify the correct destination for incoming Calls." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Interactive Voice Response" ;
    skos:related itilglos:Automatic_Call_Distribution,
        itilglos:Call,
        itilglos:User .

itilglos:Internal_Rate_of_Return a skos:concept ;
    rdfs:label "Internal Rate of Return" ;
    skos:definition "A technique used to help make decisions about Capital Expenditure. IRR calculates a figure that allows two or more alternative investments to be compared. A larger IRR indicates a better investment. See Net Present Value, Return on Investment." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Internal Rate of Return" ;
    skos:related itilglos:Capital_Expenditure,
        itilglos:Net_Present_Value,
        itilglos:Return_on_Investment .

itilglos:Ishikawa_Diagram a skos:concept ;
    rdfs:label "Ishikawa Diagram" ;
    skos:definition "A technique that helps a team to identify all the possible causes of a Problem. Originally devised by Kaoru Ishikawa, the output of this technique is a diagram that looks like a fishbone." ;
    skos:member itilproc:Continual_Service_Improvement,
        itilproc:Service_Operation ;
    skos:prefLabel "Ishikawa Diagram" ;
    skos:related itilglos:Problem .

itilglos:Kano_Model a skos:concept ;
    rdfs:label "Kano Model" ;
    skos:definition "A Model developed by Noriaki Kano that is used to help understand Customer preferences. The Kano Model considers Attributes of an IT Service grouped into areas such as Basic Factors, Excitement Factors, Performance Factors etc." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Kano Model" ;
    skos:related itilglos:Attribute,
        itilglos:Customer,
        itilglos:IT_Service,
        itilglos:Kano_Model,
        itilglos:Model,
        itilglos:Performance,
        itilglos:Service .

itilglos:Key_Performance_Indicator a skos:concept ;
    rdfs:label "Key Performance Indicator" ;
    skos:definition "A Metric that is used to help manage a Process, IT Service or Activity. Many Metrics may be measured, but only the most important of these are defined as KPIs and used to actively manage and report on the Process, IT Service or Activity. KPIs should be selected to ensure that Efficiency, Effectiveness, and Cost Effectiveness are all managed. See Critical Success Factor." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "Key Performance Indicator" ;
    skos:related itilglos:Activity,
        itilglos:Cost,
        itilglos:Cost_Effectiveness,
        itilglos:Critical_Success_Factor,
        itilglos:Effectiveness,
        itilglos:Efficiency,
        itilglos:IT_Service,
        itilglos:Metric,
        itilglos:Process,
        itilglos:Service .

itilglos:Known_Error_Database a skos:concept ;
    rdfs:label "Known Error Database" ;
    skos:definition "A database containing all Known Error Records. This database is created by Problem Management and used by Incident and Problem Management. The Known Error Database is part of the Service Knowledge Management System." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Known Error Database" ;
    skos:related itilglos:Error,
        itilglos:Incident,
        itilglos:Knowledge_Management,
        itilglos:Known_Error,
        itilglos:Known_Error_Database,
        itilglos:Known_Error_Record,
        itilglos:Management_System,
        itilglos:Problem,
        itilglos:Problem_Management,
        itilglos:Record,
        itilglos:Service,
        itilglos:Service_Knowledge_Management_System,
        itilglos:System .

itilglos:Line_of_Service a skos:concept ;
    rdfs:label "Line of Service" ;
    skos:definition "LOS, A Core Service or Supporting Service that has multiple Service Level Packages. A line of Service is managed by a Product Manager and each Service Level Package is designed to support a particular market segment." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Line of Service" ;
    skos:related itilglos:Core_Service,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Package,
        itilglos:Supporting_Service .

itilglos:Major_Incident a skos:concept ;
    rdfs:label "Major Incident" ;
    skos:definition "The highest Category of Impact for an Incident. A Major Incident results in significant disruption to the Business." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Major Incident" ;
    skos:related itilglos:Business,
        itilglos:Category,
        itilglos:Impact,
        itilglos:Incident,
        itilglos:Major_Incident .

itilglos:Managed_Services a skos:concept ;
    rdfs:label "Managed Services" ;
    skos:definition "A perspective on IT Services which emphasizes the fact that they are managed. The term Managed Services is also used as a synonym for Outsourced IT Services." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Managed Services" ;
    skos:related itilglos:IT_Service,
        itilglos:Managed_Services,
        itilglos:Service .

itilglos:Management_Information a skos:concept ;
    rdfs:label "Management Information" ;
    skos:definition "Information that is used to support decision making by managers. Management Information is often generated automatically by tools supporting the various IT Service Management Processes. Management Information often includes the values of KPIs such as \"Percentage of Changes leading to Incidents\", or \"first time fix rate\"." ;
    skos:prefLabel "Management Information" ;
    skos:related itilglos:Change,
        itilglos:IT_Service,
        itilglos:IT_Service_Management,
        itilglos:Incident,
        itilglos:Management_Information,
        itilglos:Process,
        itilglos:Service,
        itilglos:Service_Management .

itilglos:Manual_Workaround a skos:concept ;
    rdfs:label "Manual Workaround" ;
    skos:definition "Reducing or eliminating the Impact of an Incident or Problem for which a full Resolution is not yet available. Workarounds for Problems are documented in Known Error Records. Workarounds for Incidents that do not have associated Problem Records are documented in the Incident Record." ;
    skos:example "For example by restarting a failed Configuration Item." ;
    skos:prefLabel "Manual Workaround" ;
    skos:related itilglos:Error,
        itilglos:Impact,
        itilglos:Incident,
        itilglos:Incident_Record,
        itilglos:Known_Error,
        itilglos:Known_Error_Record,
        itilglos:Problem,
        itilglos:Problem_Record,
        itilglos:Record,
        itilglos:Resolution,
        itilglos:Workaround .

itilglos:Market_Space a skos:concept ;
    rdfs:label "Market Space" ;
    skos:definition "All opportunities that an IT Service Provider could exploit to meet business needs of Customers. The Market Space identifies the possible IT Services that an IT Service Provider may wish to consider delivering." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Market Space" ;
    skos:related itilglos:Customer,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Market_Space,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Mean_Time_to_Restore_Service a skos:concept ;
    rdfs:label "Mean Time to Restore Service" ;
    skos:definition "The average time taken to Restore a Configuration Item or IT Service after a Failure. MTRS is measured from when the CI or IT Service fails until it is fully Restored and delivering its normal functionality. See Maintainability, Mean Time to Repair." ;
    skos:prefLabel "Mean Time to Restore Service" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Failure,
        itilglos:IT_Service,
        itilglos:Maintainability,
        itilglos:Repair,
        itilglos:Restore,
        itilglos:Service .

itilglos:Mission_Statement a skos:concept ;
    rdfs:label "Mission Statement" ;
    skos:definition "The Mission Statement of an Organisation is a short but complete description of the overall purpose and intentions of that Organisation. It states what is to be achieved, but not how this should be done." ;
    skos:prefLabel "Mission Statement" ;
    skos:related itilglos:Mission_Statement,
        itilglos:Organisation .

itilglos:Notional_Charging a skos:concept ;
    rdfs:label "Notional Charging" ;
    skos:definition "An approach to Charging for IT Services. Charges to Customers are calculated and Customers are informed of the charge, but no money is actually transferred. Notional Charging is sometimes introduced to ensure that Customers are aware of the Costs they incur, or as a stage during the introduction of real Charging." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Notional Charging" ;
    skos:related itilglos:Charging,
        itilglos:Cost,
        itilglos:Customer,
        itilglos:IT_Service,
        itilglos:Notional_Charging,
        itilglos:Service .

itilglos:Operations_Bridge a skos:concept ;
    rdfs:label "Operations Bridge" ;
    skos:definition "A physical location where IT Services and IT Infrastructure are monitored and managed." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Operations Bridge" ;
    skos:related itilglos:IT_Infrastructure,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:Opportunity_Cost a skos:concept ;
    rdfs:label "Opportunity Cost" ;
    skos:definition "A Cost that is used in deciding between investment choices. Opportunity Cost represents the revenue that would have been generated by using the Resources in a different way. Opportunity cost analysis is used as part of a decision making processes, but is not treated as an actual Cost in any financial statement." ;
    skos:example "For example the Opportunity Cost of purchasing a new Server may include not carrying out a Service Improvement activity that the money could have been spent on." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Opportunity Cost" ;
    skos:related itilglos:Cost,
        itilglos:Opportunity_Cost,
        itilglos:Resource .

itilglos:Overhead a skos:concept ;
    rdfs:label "Overhead" ;
    skos:definition "Synonym for Indirect cost" ;
    skos:prefLabel "Overhead" .

itilglos:PMBOK a skos:concept ;
    rdfs:label "PMBOK" ;
    skos:definition "A Project management Standard maintained and published by the Project Management Institute. PMBOK stands for Project Management Body of Knowledge. See http://www.pmi.org/ for more information. See PRINCE2." ;
    skos:prefLabel "PMBOK" ;
    skos:related itilglos:PMBOK,
        itilglos:PRINCE2,
        itilglos:Project,
        itilglos:Standard .

itilglos:Pattern_of_Business_Activity a skos:concept ;
    rdfs:label "Pattern of Business Activity" ;
    skos:definition "A Workload profile of one or more Business Activities. Patterns of Business Activity are used to help the IT Service Provider understand and plan for different levels of Business Activity." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Pattern of Business Activity" ;
    skos:related itilglos:Activity,
        itilglos:Business,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Service,
        itilglos:Service_Provider,
        itilglos:Workload .

itilglos:Performance_Management a skos:concept ;
    rdfs:label "Performance Management" ;
    skos:definition "The Process responsible for day-to-day Capacity Management Activities. These include Monitoring, Threshold detection, Performance analysis and Tuning, and implementing Changes related to Performance and Capacity." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "Performance Management" ;
    skos:related itilglos:Capacity,
        itilglos:Capacity_Management,
        itilglos:Change,
        itilglos:Monitoring,
        itilglos:Performance,
        itilglos:Process,
        itilglos:Threshold,
        itilglos:Tuning .

itilglos:Pilot a skos:concept ;
    rdfs:label "Pilot" ;
    skos:definition "A limited Deployment of an IT Service, a Release or a Process to the Live Environment. A Pilot is used to reduce Risk and to gain User feedback and Acceptance. See Test, Evaluation." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Pilot" ;
    skos:related itilglos:Acceptance,
        itilglos:Deployment,
        itilglos:Environment,
        itilglos:Evaluation,
        itilglos:IT_Service,
        itilglos:Live,
        itilglos:Live_Environment,
        itilglos:Pilot,
        itilglos:Process,
        itilglos:Release,
        itilglos:Risk,
        itilglos:Service,
        itilglos:Test,
        itilglos:User .

itilglos:Planned_Downtime a skos:concept ;
    rdfs:label "Planned Downtime" ;
    skos:definition "Agreed time when an IT Service will not be available. Planned Downtime is often used for maintenance, upgrades and testing. See Change Window, Downtime." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Planned Downtime" ;
    skos:related itilglos:Change,
        itilglos:Change_Window,
        itilglos:Downtime,
        itilglos:IT_Service,
        itilglos:Plan,
        itilglos:Planned_Downtime,
        itilglos:Service .

itilglos:Portable_Facility a skos:concept ;
    rdfs:label "Portable Facility" ;
    skos:definition "A prefabricated building, or a large vehicle, provided by a Third Party and moved to a site when needed by an IT Service Continuity Plan. See Recovery Option, Fixed Facility." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Portable Facility" ;
    skos:related itilglos:Fixed_Facility,
        itilglos:IT_Service,
        itilglos:IT_Service_Continuity_Plan,
        itilglos:Plan,
        itilglos:Recovery,
        itilglos:Recovery_Option,
        itilglos:Service,
        itilglos:Third_Party .

itilglos:Post_Implementation_Review a skos:concept ;
    rdfs:label "Post Implementation Review" ;
    skos:definition "A Review that takes place after a Change or a Project has been implemented. A PIR determines if the Change or Project was successful, and identifies opportunities for improvement." ;
    skos:prefLabel "Post Implementation Review" ;
    skos:related itilglos:Change,
        itilglos:Project,
        itilglos:Review .

itilglos:Proactive_Monitoring a skos:concept ;
    rdfs:label "Proactive Monitoring" ;
    skos:definition "Monitoring that looks for patterns of Events to predict possible future Failures." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Proactive Monitoring" ;
    skos:related itilglos:Event,
        itilglos:Failure,
        itilglos:Monitoring .

itilglos:Proactive_Problem_Management a skos:concept ;
    rdfs:label "Proactive Problem Management" ;
    skos:definition "Part of the Problem Management Process. The Objective of Proactive Problem Management is to identify Problems that might otherwise be missed. Proactive Problem Management analyses Incident Records, and uses data collected by other IT Service Management Processes to identify trends or significant Problems." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Proactive Problem Management" ;
    skos:related itilglos:IT_Service,
        itilglos:IT_Service_Management,
        itilglos:Incident,
        itilglos:Incident_Record,
        itilglos:Objective,
        itilglos:Proactive_Problem_Management,
        itilglos:Problem,
        itilglos:Problem_Management,
        itilglos:Process,
        itilglos:Record,
        itilglos:Service,
        itilglos:Service_Management .

itilglos:Qualification a skos:concept ;
    rdfs:label "Qualification" ;
    skos:definition "Service Transition) An Activity that ensures that IT Infrastructure is appropriate, and correctly configured, to support an Application or IT Service. See Validation." ;
    skos:prefLabel "Qualification" ;
    skos:related itilglos:Activity,
        itilglos:Application,
        itilglos:IT_Infrastructure,
        itilglos:IT_Service,
        itilglos:Service,
        itilglos:Service_Transition,
        itilglos:Transition,
        itilglos:Validation .

itilglos:Reciprocal_Arrangement a skos:concept ;
    rdfs:label "Reciprocal Arrangement" ;
    skos:definition "A Recovery Option. An agreement between two Organisations to share resources in an emergency." ;
    skos:example "For example, Computer Room space or use of a mainframe." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Reciprocal Arrangement" ;
    skos:related itilglos:Organisation,
        itilglos:Recovery,
        itilglos:Recovery_Option .

itilglos:Release_Identification a skos:concept ;
    rdfs:label "Release Identification" ;
    skos:definition "A naming convention used to uniquely identify a Release. The Release Identification typically includes a reference to the Configuration Item and a version number." ;
    skos:example "For example Microsoft Office 2003 SR2." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Release Identification" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Release,
        itilglos:Release_Identification .

itilglos:Release_Process a skos:concept ;
    rdfs:label "Release Process" ;
    skos:definition "The name used by ISO/IEC 20000 for the Process group that includes Release Management. This group does not include any other Processes. Release Process is also used as a synonym for Release Management Process." ;
    skos:prefLabel "Release Process" ;
    skos:related <http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_20000>,
        itilglos:Process,
        itilglos:Release,
        itilglos:Release_Management,
        itilglos:Release_Process .

itilglos:Release_Record a skos:concept ;
    rdfs:label "Release Record" ;
    skos:definition "A Record in the CMDB that defines the content of a Release. A Release Record has Relationships with all Configuration Items that are affected by the Release." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Release Record" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Record,
        itilglos:Relationship,
        itilglos:Release,
        itilglos:Release_Record .

itilglos:Release_Unit a skos:concept ;
    rdfs:label "Release Unit" ;
    skos:definition "Components of an IT Service that are normally Released together. A Release Unit typically includes sufficient Components to perform a useful Function. A different Release Unit may be the complete Payroll Application, including IT Operations Procedures and User training." ;
    skos:example "For example one Release Unit could be a Desktop PC, including Hardware, Software, Licenses, Documentation etc." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Release Unit" ;
    skos:related itilglos:Application,
        itilglos:Component,
        itilglos:Function,
        itilglos:IT_Operations,
        itilglos:IT_Service,
        itilglos:Operation,
        itilglos:Procedure,
        itilglos:Release,
        itilglos:Release_Unit,
        itilglos:Service,
        itilglos:User .

itilglos:Remediation a skos:concept ;
    rdfs:label "Remediation" ;
    skos:definition "Recovery to a known state after a failed Change or Release." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Remediation" ;
    skos:related itilglos:Change,
        itilglos:Recovery,
        itilglos:Release .

itilglos:Request_Fulfilment a skos:concept ;
    rdfs:label "Request Fulfilment" ;
    skos:definition "The Process responsible for managing the Lifecycle of all Service Requests." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Request Fulfilment" ;
    skos:related itilglos:Lifecycle,
        itilglos:Process,
        itilglos:Service,
        itilglos:Service_Request .

itilglos:Resolution_Processes a skos:concept ;
    rdfs:label "Resolution Processes" ;
    skos:definition "The ISO/IEC 20000 Process group that includes Incident Management and Problem Management." ;
    skos:prefLabel "Resolution Processes" ;
    skos:related <http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_20000>,
        itilglos:Incident,
        itilglos:Incident_Management,
        itilglos:Problem,
        itilglos:Problem_Management,
        itilglos:Process .

itilglos:Response_Time a skos:concept ;
    rdfs:label "Response Time" ;
    skos:definition "A measure of the time taken to complete an Operation or Transaction. Used in Capacity Management as a measure of IT Infrastructure Performance, and in Incident Management as a measure of the time taken to answer the phone, or to start Diagnosis." ;
    skos:prefLabel "Response Time" ;
    skos:related itilglos:Capacity,
        itilglos:Capacity_Management,
        itilglos:Diagnosis,
        itilglos:IT_Infrastructure,
        itilglos:Incident,
        itilglos:Incident_Management,
        itilglos:Operation,
        itilglos:Performance,
        itilglos:Transaction .

itilglos:Risk_Management a skos:concept ;
    rdfs:label "Risk Management" ;
    skos:definition "The Process responsible for identifying, assessing and controlling Risks. See Risk Assessment." ;
    skos:prefLabel "Risk Management" ;
    skos:related itilglos:Assessment,
        itilglos:Process,
        itilglos:Risk,
        itilglos:Risk_Assessment .

itilglos:Rollout a skos:concept ;
    rdfs:label "Rollout" ;
    skos:definition "Synonym for Deployment. Most often used to refer to complex or phased Deployments or Deployments to multiple locations." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Rollout" ;
    skos:related itilglos:Deployment .

itilglos:SLAM_Chart a skos:concept ;
    rdfs:label "SLAM Chart" ;
    skos:definition "A Service Level Agreement Monitoring Chart is used to help monitor and report achievements against Service Level Targets. A SLAM Chart is typically colour coded to show whether each agreed Service Level Target has been met, missed, or nearly missed during each of the previous 12 months." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "SLAM Chart" ;
    skos:related itilglos:Agreement,
        itilglos:Monitoring,
        itilglos:SLAM_Chart,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Agreement,
        itilglos:Service_Level_Target .

itilglos:SMART a skos:concept ;
    rdfs:label "SMART" ;
    skos:definition "An acronym for helping to remember that targets in Service Level Agreements and Project Plans should be Specific, Measurable, Achievable, Relevant and Timely." ;
    skos:member itilproc:Continual_Service_Improvement,
        itilproc:Service_Design ;
    skos:prefLabel "SMART" ;
    skos:related itilglos:Agreement,
        itilglos:Plan,
        itilglos:Project,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Agreement .

itilglos:Security_Policy a skos:concept ;
    rdfs:label "Security Policy" ;
    skos:definition "Synonym for Information Security Policy" ;
    skos:prefLabel "Security Policy" ;
    skos:related itilglos:Information_Security_Policy,
        itilglos:Policy,
        itilglos:Security,
        itilglos:Security_Policy .

itilglos:Service_Analytics a skos:concept ;
    rdfs:label "Service Analytics" ;
    skos:definition "A technique used in the Assessment of the Business Impact of Incidents. Service Analytics Models the dependencies between Configuration Items, and the dependencies of IT Services on Configuration Items." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Service Analytics" ;
    skos:related itilglos:Assessment,
        itilglos:Business,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:IT_Service,
        itilglos:Impact,
        itilglos:Incident,
        itilglos:Model,
        itilglos:Service,
        itilglos:Service_Analytics .

itilglos:Service_Capacity_Management a skos:concept ;
    rdfs:label "Service Capacity Management" ;
    skos:definition "The Activity responsible for understanding the Performance and Capacity of IT Services. The Resources used by each IT Service and the pattern of usage over time are collected, recorded, and analysed for use in the Capacity Plan. See Business Capacity Management, Component Capacity Management." ;
    skos:member itilproc:Continual_Service_Improvement,
        itilproc:Service_Design ;
    skos:prefLabel "Service Capacity Management" ;
    skos:related itilglos:Activity,
        itilglos:Business,
        itilglos:Business_Capacity_Management,
        itilglos:Capacity,
        itilglos:Capacity_Management,
        itilglos:Capacity_Plan,
        itilglos:Component,
        itilglos:Component_Capacity_Management,
        itilglos:IT_Service,
        itilglos:Performance,
        itilglos:Plan,
        itilglos:Resource,
        itilglos:Service .

itilglos:Service_Design_Package a skos:concept ;
    rdfs:label "Service Design Package" ;
    skos:definition "Document(s) defining all aspects of an IT Service and its Requirements through each stage of its Lifecycle. A Service Design Package is produced for each new IT Service, major Change, or IT Service Retirement." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Service Design Package" ;
    skos:related itilglos:Change,
        itilglos:Design,
        itilglos:Document,
        itilglos:IT_Service,
        itilglos:Lifecycle,
        itilglos:Requirement,
        itilglos:Retire,
        itilglos:Service,
        itilglos:Service_Design,
        itilglos:Service_Design_Package .

itilglos:Service_Level_Requirement a skos:concept ;
    rdfs:label "Service Level Requirement" ;
    skos:definition "A Customer Requirement for an aspect of an IT Service. SLRs are based on Business Objectives and are used to negotiate agreed Service Level Targets." ;
    skos:member itilproc:Continual_Service_Improvement,
        itilproc:Service_Design ;
    skos:prefLabel "Service Level Requirement" ;
    skos:related itilglos:Business,
        itilglos:Business_Objective,
        itilglos:Customer,
        itilglos:IT_Service,
        itilglos:Objective,
        itilglos:Requirement,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Target .

itilglos:Service_Management_Lifecycle a skos:concept ;
    rdfs:label "Service Management Lifecycle" ;
    skos:definition "An approach to IT Service Management that emphasizes the importance of coordination and Control across the various Functions, Processes, and Systems necessary to manage the full Lifecycle of IT Services. The Service Management Lifecycle approach considers the Strategy, Design, Transition, Operation and Continuous Improvement of IT Services." ;
    skos:prefLabel "Service Management Lifecycle" ;
    skos:related itilglos:Control,
        itilglos:Design,
        itilglos:Function,
        itilglos:IT_Service,
        itilglos:IT_Service_Management,
        itilglos:Lifecycle,
        itilglos:Operation,
        itilglos:Process,
        itilglos:Service,
        itilglos:Service_Management,
        itilglos:Service_Management_Lifecycle,
        itilglos:Strategy,
        itilglos:System,
        itilglos:Transition .

itilglos:Service_Manager a skos:concept ;
    rdfs:label "Service Manager" ;
    skos:definition "A manager who is responsible for managing the end-to-end Lifecycle of one or more IT Services. The term Service Manager is also used to mean any manager within the IT Service Provider. Most commonly used to refer to a Business Relationship Manager, a Process Manager, an Account Manager or a senior manager with responsibility for IT Services overall." ;
    skos:prefLabel "Service Manager" ;
    skos:related itilglos:Account_Manager,
        itilglos:Business,
        itilglos:Business_Relationship_Manager,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Lifecycle,
        itilglos:Process,
        itilglos:Process_Manager,
        itilglos:Relationship,
        itilglos:Service,
        itilglos:Service_Manager,
        itilglos:Service_Provider .

itilglos:Service_Provider_Interface a skos:concept ;
    rdfs:label "Service Provider Interface" ;
    skos:definition "An interface between the IT Service Provider and a User, Customer, Business Process, or a Supplier. Analysis of Service Provider Interfaces helps to coordinate end-to-end management of IT Services." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Service Provider Interface" ;
    skos:related itilglos:Business,
        itilglos:Business_Process,
        itilglos:Customer,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Process,
        itilglos:Service,
        itilglos:Service_Provider,
        itilglos:Service_Provider_Interface,
        itilglos:Supplier,
        itilglos:User .

itilglos:Service_Reporting a skos:concept ;
    rdfs:label "Service Reporting" ;
    skos:definition "The Process responsible for producing and delivering reports of achievement and trends against Service Levels. Service Reporting should agree the format, content and frequency of reports with Customers." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "Service Reporting" ;
    skos:related itilglos:Customer,
        itilglos:Process,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Reporting .

itilglos:Service_Valuation a skos:concept ;
    rdfs:label "Service Valuation" ;
    skos:definition "A measurement of the total Cost of delivering an IT Service, and the total value to the Business of that IT Service. Service Valuation is used to help the Business and the IT Service Provider agree on the value of the IT Service." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Service Valuation" ;
    skos:related itilglos:Business,
        itilglos:Cost,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Service,
        itilglos:Service_Provider,
        itilglos:Service_Valuation .

itilglos:Serviceability a skos:concept ;
    rdfs:label "Serviceability" ;
    skos:definition "The ability of a Third Party Supplier to meet the terms of their Contract. This Contract will include agreed levels of Reliability, Maintainability or Availability for a Configuration Item." ;
    skos:member itilproc:Continual_Service_Improvement,
        itilproc:Service_Design ;
    skos:prefLabel "Serviceability" ;
    skos:related itilglos:Availability,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Contract,
        itilglos:Maintainability,
        itilglos:Reliability,
        itilglos:Supplier,
        itilglos:Third_Party .

itilglos:Statement_of_Requirements a skos:concept ;
    rdfs:label "Statement of Requirements" ;
    skos:definition "A Document containing all Requirements for a product purchase, or a new or changed IT Service. See Terms of Reference." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Statement of Requirements" ;
    skos:related itilglos:Document,
        itilglos:IT_Service,
        itilglos:Requirement,
        itilglos:Service,
        itilglos:Terms_of_Reference .

itilglos:Super_User a skos:concept ;
    rdfs:label "Super User" ;
    skos:definition "A User who helps other Users, and assists in communication with the Service Desk or other parts of the IT Service Provider. Super Users typically provide support for minor Incidents and training." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Super User" ;
    skos:related itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Incident,
        itilglos:Service,
        itilglos:Service_Desk,
        itilglos:Service_Provider,
        itilglos:Super_User,
        itilglos:User .

itilglos:Supplier_Management a skos:concept ;
    rdfs:label "Supplier Management" ;
    skos:definition "The Process responsible for ensuring that all Contracts with Suppliers support the needs of the Business, and that all Suppliers meet their contractual commitments." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Supplier Management" ;
    skos:related itilglos:Business,
        itilglos:Contract,
        itilglos:Process,
        itilglos:Supplier .

itilglos:Support_Hours a skos:concept ;
    rdfs:label "Support Hours" ;
    skos:definition "The times or hours when support is available to the Users. Typically this is the hours when the Service Desk is available. Support Hours should be defined in a Service Level Agreement, and may be different from Service Hours." ;
    skos:example "For example, Service Hours may be 24 hours a day, but the Support Hours may be 07:00 to 19:00." ;
    skos:member itilproc:Service_Design,
        itilproc:Service_Operation ;
    skos:prefLabel "Support Hours" ;
    skos:related itilglos:Agreement,
        itilglos:Service,
        itilglos:Service_Desk,
        itilglos:Service_Hours,
        itilglos:Service_Level,
        itilglos:Service_Level_Agreement,
        itilglos:Support_Hours,
        itilglos:User .

itilglos:System_Management a skos:concept ;
    rdfs:label "System Management" ;
    skos:definition "The part of IT Service Management that focuses on the management of IT Infrastructure rather than Process." ;
    skos:prefLabel "System Management" ;
    skos:related itilglos:IT_Infrastructure,
        itilglos:IT_Service,
        itilglos:IT_Service_Management,
        itilglos:Process,
        itilglos:Service,
        itilglos:Service_Management .

itilglos:Tag a skos:concept ;
    rdfs:label "Tag" ;
    skos:definition "A short code used to identify a Category. The term Tag is also used to refer to the Activity of assigning Tags to things." ;
    skos:example "For example tags EC1, EC2, EC3 etc. might be used to identify different Customer outcomes when analysing and comparing Strategies." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Tag" ;
    skos:related itilglos:Activity,
        itilglos:Category,
        itilglos:Tag .

itilglos:Technical_Support a skos:concept ;
    rdfs:label "Technical Support" ;
    skos:definition "Synonym for Technical Management." ;
    skos:prefLabel "Technical Support" ;
    skos:related itilglos:Technical_Management .

itilglos:Tension_Metrics a skos:concept ;
    rdfs:label "Tension Metrics" ;
    skos:definition "A set of related Metrics, in which improvements to one Metric have a negative effect on another. Tension Metrics are designed to ensure that an appropriate balance is achieved." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "Tension Metrics" ;
    skos:related itilglos:Metric,
        itilglos:Tension_Metrics .

itilglos:Terms_of_Reference a skos:concept ;
    rdfs:label "Terms of Reference" ;
    skos:definition "A Document specifying the Requirements, Scope, Deliverables, Resources and schedule for a Project or Activity." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Terms of Reference" ;
    skos:related itilglos:Activity,
        itilglos:Deliverable,
        itilglos:Document,
        itilglos:Project,
        itilglos:Requirement,
        itilglos:Resource,
        itilglos:Scope .

itilglos:Test_Environment a skos:concept ;
    rdfs:label "Test Environment" ;
