    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Change Schedule" ;
    skos:related itilglos:Change,
        itilglos:Change_Schedule,
        itilglos:Document .

itilglos:Classification a skos:concept ;
    rdfs:label "Classification" ;
    skos:definition "The act of assigning a Category to something. Classification is used to ensure consistent management and reporting. CIs, Incidents, Problems, Changes etc. are usually classified." ;
