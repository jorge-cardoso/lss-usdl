itilglos:CI_Type a skos:concept ;
    rdfs:label "CI Type" ;
    skos:definition "A Category that is used to Classify CIs. The CI Type identifies the required Attributes and Relationships for a Configuration Record. Common CI Types include: hardware, Document, User etc." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "CI Type" ;
