        itilglos:Known_Error,
        itilglos:Management_System,
        itilglos:Problem,
        itilglos:Process,
        itilglos:Relationship,
        itilglos:Release,
        itilglos:Service,
        itilglos:Service_Knowledge_Management_System,
        itilglos:Service_Management,
        itilglos:Service_Provider,
        itilglos:Supplier,
        itilglos:System,
        itilglos:User .

itilglos:Deliverable a skos:concept ;
    rdfs:label "Deliverable" ;
    skos:definition "Something that must be provided to meet a commitment in a Service Level Agreement or a Contract. Deliverable is also used in a more informal way to mean a planned output of any Process." ;
    skos:prefLabel "Deliverable" ;
    skos:related itilglos:Agreement,
        itilglos:Contract,
        itilglos:Deliverable,
        itilglos:Process,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Agreement .

itilglos:Downtime a skos:concept ;
    rdfs:label "Downtime" ;
    skos:definition "The time when a Configuration Item or IT Service is not Available during its Agreed Service Time. The Availability of an IT Service is often calculated from Agreed Service Time and Downtime." ;
    skos:member itilproc:Service_Design,
        itilproc:Service_Operation ;
    skos:prefLabel "Downtime" ;
    skos:related itilglos:Agreed_Service_Time,
        itilglos:Availability,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Downtime,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:External_Customer a skos:concept ;
    rdfs:label "External Customer" ;
    skos:definition "A Customer who works for a different Business to the IT Service Provider. See External Service Provider, Internal Customer." ;
    skos:prefLabel "External Customer" ;
    skos:related itilglos:Business,
        itilglos:Customer,
        itilglos:External_Service_Provider,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Internal_Customer,
        itilglos:Service,
        itilglos:Service_Provider .

<http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_20000> a skos:concept ;
    rdfs:label "ISO/IEC 20000" ;
    skos:definition "ISO Specification and Code of Practice for IT Service Management. ISO/IEC 20000 is aligned with ITIL Best Practice." ;
    skos:prefLabel "ISO/IEC 20000" ;
    skos:related itilglos:Best_Practice,
        itilglos:Code_of_Practice,
        <http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_20000>,
        itilglos:ITIL,
