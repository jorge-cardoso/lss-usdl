        itilglos:Requirement,
        itilglos:Risk,
        itilglos:Scope,
        itilglos:Service,
        itilglos:Service_Continuity_Management .

itilglos:Business_Continuity_Plan a skos:concept ;
    rdfs:label "Business Continuity Plan" ;
    skos:definition "A Plan defining the steps required to Restore Business Processes following a disruption. The Plan will also identify the triggers for Invocation, people to be involved, communications etc. IT Service Continuity Plans form a significant part of Business Continuity Plans." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Business Continuity Plan" ;
    skos:related itilglos:Business,
        itilglos:Business_Continuity_Plan,
