        itilglos:Value_on_Investment .

itilglos:Culture a skos:concept ;
    rdfs:label "Culture" ;
    skos:definition "A set of values that is shared by a group of people, including expectations about how people should behave, ideas, beliefs, and practices." ;
    skos:prefLabel "Culture" .

itilglos:Event_Management a skos:concept ;
    rdfs:label "Event Management" ;
    skos:definition "Service Operation) The Process responsible for managing Events throughout their Lifecycle. Event Management is one of the main Activities of IT Operations." ;
    skos:prefLabel "Event Management" ;
