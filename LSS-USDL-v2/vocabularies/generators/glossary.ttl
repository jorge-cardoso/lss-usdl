        itilglos:Contract,
        itilglos:Contract_Portfolio,
        itilglos:IT_Service,
        itilglos:Service,
        itilglos:Service_Contract .

itilglos:Service_Pipeline a skos:concept ;
    rdfs:label "Service Pipeline" ;
    skos:definition "A database or structured Document listing all IT Services that are under consideration or Development, but are not yet available to Customers. The Service Pipeline provides a Business view of possible future IT Services and is part of the Service Portfolio which is not normally published to Customers." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Service Pipeline" ;
    skos:related itilglos:Business,
        itilglos:Customer,
        itilglos:Development,
        itilglos:Document,
        itilglos:IT_Service,
        itilglos:Service,
        itilglos:Service_Pipeline,
        itilglos:Service_Portfolio .

itilglos:Single_Point_of_Contact a skos:concept ;
    rdfs:label "Single Point of Contact" ;
    skos:definition "Providing a single consistent way to communicate with an Organisation or Business Unit." ;
    skos:example "For example, a Single Point of Contact for an IT Service Provider is usually called a Service Desk." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Single Point of Contact" ;
    skos:related itilglos:Business,
        itilglos:Business_Unit,
        itilglos:Organisation .

itilglos:Stakeholder a skos:concept ;
    rdfs:label "Stakeholder" ;
    skos:definition "All people who have an interest in an Organisation, Project, IT Service etc. Stakeholders may be interested in the Activities, targets, Resources, or Deliverables. Stakeholders may include Customers, Partners, employees, shareholders, owners, etc. See RACI." ;
    skos:prefLabel "Stakeholder" ;
    skos:related itilglos:Customer,
        itilglos:Deliverable,
