        itilglos:IT_Service_Management,
        itilglos:Practice,
        itilglos:Service,
        itilglos:Service_Management,
        itilglos:Specification .

itilglos:Outcome a skos:concept ;
    rdfs:label "Outcome" ;
    skos:definition "The result of carrying out an Activity; following a Process; delivering an IT Service etc. The term Outcome is used to refer to intended results, as well as to actual results. See Objective." ;
    skos:prefLabel "Outcome" ;
    skos:related itilglos:Activity,
        itilglos:IT_Service,
        itilglos:Objective,
        itilglos:Outcome,
        itilglos:Process,
        itilglos:Service .

itilglos:Reliability a skos:concept ;
    rdfs:label "Reliability" ;
    skos:definition "A measure of how long a Configuration Item or IT Service can perform its agreed Function without interruption. Usually measured as MTBF or MTBSI. The term Reliability can also be used to state how likely it is that a Process, Function etc. will deliver its required outputs. See Availability." ;
    skos:member itilproc:Continual_Service_Improvement,
        itilproc:Service_Design ;
    skos:prefLabel "Reliability" ;
    skos:related itilglos:Availability,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Function,
        itilglos:IT_Service,
        itilglos:Process,
        itilglos:Reliability,
        itilglos:Service .

itilglos:Service_Sourcing a skos:concept ;
    rdfs:label "Service Sourcing" ;
    skos:definition "The Strategy and approach for deciding whether to provide a Service internally or to Outsource it to an External Service Provider. Service Sourcing also means the execution of this Strategy. Service Sourcing includes: Internal Sourcing - Internal or Shared Services using Type I or Type II Service Providers. Traditional Sourcing - Full Service Outsourcing using a Type III Service Provider. Multivendor Sourcing - Prime, Consortium or Selective Outsourcing using Type III Service Providers." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Service Sourcing" ;
    skos:related itilglos:External_Service_Provider,
        itilglos:Internal_Sourcing,
        itilglos:Outsourcing,
        itilglos:Service,
        itilglos:Service_Provider,
        itilglos:Service_Sourcing,
        itilglos:Strategy .

itilglos:Strategic a skos:concept ;
    rdfs:label "Strategic" ;
    skos:definition "The highest of three levels of Planning and delivery (Strategic, Tactical, Operational). Strategic Activities include Objective setting and long term Planning to achieve the overall Vision." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Strategic" ;
    skos:related itilglos:Objective,
        itilglos:Operation,
        itilglos:Operational,
        itilglos:Plan,
        itilglos:Planning,
        itilglos:Strategic,
        itilglos:Tactical,
        itilglos:Vision .

itilglos:Validation a skos:concept ;
    rdfs:label "Validation" ;
    skos:definition "An Activity that ensures a new or changed IT Service, Process, Plan, or other Deliverable meets the needs of the Business. Validation ensures that Business Requirements are met even though these may have changed since the original Design. See Verification, Acceptance, Qualification, Service Validation and Testing." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Validation" ;
    skos:related itilglos:Acceptance,
        itilglos:Activity,
        itilglos:Business,
        itilglos:Deliverable,
        itilglos:Design,
        itilglos:IT_Service,
        itilglos:Plan,
        itilglos:Process,
        itilglos:Qualification,
        itilglos:Requirement,
        itilglos:Service,
        itilglos:Service_Validation_and_Testing,
        itilglos:Test,
        itilglos:Validation,
        itilglos:Verification .

itilglos:Workaround a skos:concept ;
    rdfs:label "Workaround" ;
    skos:definition "Reducing or eliminating the Impact of an Incident or Problem for which a full Resolution is not yet available. Workarounds for Problems are documented in Known Error Records. Workarounds for Incidents that do not have associated Problem Records are documented in the Incident Record." ;
    skos:example "For example by restarting a failed Configuration Item." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Workaround" ;
    skos:related itilglos:Error,
        itilglos:Impact,
        itilglos:Incident,
        itilglos:Incident_Record,
        itilglos:Known_Error,
        itilglos:Known_Error_Record,
        itilglos:Problem,
        itilglos:Problem_Record,
        itilglos:Record,
        itilglos:Resolution,
        itilglos:Workaround .

itilglos:Deployment a skos:concept ;
    rdfs:label "Deployment" ;
    skos:definition "The Activity responsible for movement of new or changed hardware, software, documentation, Process, etc to the Live Environment. Deployment is part of the Release and Deployment Management Process. See Rollout." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Deployment" ;
