        itilglos:Supplier,
        itilglos:Vital_Business_Function .

itilglos:CRAMM a skos:concept ;
    rdfs:label "CRAMM" ;
    skos:definition "A methodology and tool for analysing and managing Risks. CRAMM was developed by the UK Government, but is now privately owned. Further information is available from http://www.cramm.com/" ;
    skos:prefLabel "CRAMM" ;
    skos:related itilglos:CRAMM,
        itilglos:Risk .

itilglos:Call_Type a skos:concept ;
