        itilglos:Service_Level,
        itilglos:Service_Level_Target .

itilglos:Risk_Assessment a skos:concept ;
    rdfs:label "Risk Assessment" ;
    skos:definition "The initial steps of Risk Management. Analysing the value of Assets to the business, identifying Threats to those Assets, and evaluating how Vulnerable each Asset is to those Threats. Risk Assessment can be quantitative (based on numerical data) or qualitative." ;
    skos:prefLabel "Risk Assessment" ;
    skos:related itilglos:Assessment,
        itilglos:Asset,
        itilglos:Risk,
        itilglos:Risk_Assessment,
        itilglos:Risk_Management,
        itilglos:Threat .

itilglos:Server a skos:concept ;
    rdfs:label "Server" ;
    skos:definition "A computer that is connected to a network and provides software Functions that are used by other computers" ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Server" ;
    skos:related itilglos:Function .

itilglos:Service_Contract a skos:concept ;
    rdfs:label "Service Contract" ;
    skos:definition "A Contract to deliver one or more IT Services. The term Service Contract is also used to mean any Agreement to deliver IT Services, whether this is a legal Contract or an SLA. See Contract Portfolio." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Service Contract" ;
