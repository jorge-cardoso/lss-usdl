        itilglos:Service_Provider .

itilglos:Unit_Cost a skos:concept ;
    rdfs:label "Unit Cost" ;
    skos:definition "The Cost to the IT Service Provider of providing a single Component of an IT Service." ;
    skos:example "For example the Cost of a single desktop PC, or of a single Transaction." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Unit Cost" ;
    skos:related itilglos:Component,
        itilglos:Cost,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Service,
        itilglos:Service_Provider .

<http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#User_Profile_(UP)> a skos:concept ;
