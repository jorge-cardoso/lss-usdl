itilglos:Version a skos:concept ;
    rdfs:label "Version" ;
    skos:definition "A Version is used to identify a specific Baseline of a Configuration Item. Versions typically use a naming convention that enables the sequence or date of each Baseline to be identified." ;
    skos:example "For example Payroll Application Version 3 contains updated functionality from Version 2." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Version" ;
    skos:related itilglos:Baseline,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Version .

