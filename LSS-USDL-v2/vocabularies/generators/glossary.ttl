    skos:definition "Synonym for Fast Recovery or Immediate Recovery." ;
    skos:prefLabel "Hot Standby" ;
    skos:related itilglos:Fast_Recovery,
        itilglos:Immediate_Recovery,
        itilglos:Recovery .

itilglos:Identity a skos:concept ;
    rdfs:label "Identity" ;
    skos:definition "A unique name that is used to identify a User, person or Role. The Identity is used to grant Rights to that User, person, or Role. J or the Role \"Change manager\"." ;
    skos:example "Example identities might be the username Smith" ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Identity" ;
