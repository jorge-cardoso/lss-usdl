    rdfs:label "Configuration Identification" ;
    skos:definition "The Activity responsible for collecting information about Configuration Items and their Relationships, and loading this information into the CMDB. Configuration Identification is also responsible for labelling the CIs themselves, so that the corresponding Configuration Records can be found." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Configuration Identification" ;
    skos:related itilglos:Activity,
