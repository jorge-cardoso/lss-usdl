itilglos:Technical_Observation a skos:concept ;
    rdfs:label "Technical Observation" ;
    skos:definition "A technique used in Service Improvement, Problem investigation and Availability Management. Technical support staff meet to monitor the behaviour and Performance of an IT Service and make recommendations for improvement." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "Technical Observation" ;
    skos:related itilglos:Availability,
        itilglos:Availability_Management,
        itilglos:IT_Service,
        itilglos:Performance,
        itilglos:Problem,
        itilglos:Service .

itilglos:Technical_Service a skos:concept ;
    rdfs:label "Technical Service" ;
