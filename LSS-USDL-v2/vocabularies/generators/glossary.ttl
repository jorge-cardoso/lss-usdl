        itilglos:Workload .

itilglos:Single_Point_of_Failure a skos:concept ;
    rdfs:label "Single Point of Failure" ;
    skos:definition "Any Configuration Item that can cause an Incident when it fails, and for which a Countermeasure has not been implemented. A SPOF may be a person, or a step in a Process or Activity, as well as a Component of the IT Infrastructure." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Single Point of Failure" ;
    skos:related itilglos:Activity,
        itilglos:Component,
