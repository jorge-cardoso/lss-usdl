        itilglos:User,
        itilglos:Variable_Cost,
        itilglos:Variable_Cost_Dynamics .

itilglos:Verification a skos:concept ;
    rdfs:label "Verification" ;
    skos:definition "An Activity that ensures a new or changed IT Service, Process, Plan, or other Deliverable is complete, accurate, Reliable and matches its Design Specification. See Validation, Acceptance, Service Validation and Testing." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Verification" ;
