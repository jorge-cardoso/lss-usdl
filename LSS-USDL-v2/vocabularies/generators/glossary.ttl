itilglos:Service_Level_Agreement a skos:concept ;
    rdfs:label "Service Level Agreement" ;
    skos:definition "An Agreement between an IT Service Provider and a Customer. The SLA describes the IT Service, documents Service Level Targets, and specifies the responsibilities of the IT Service Provider and the Customer. A single SLA may cover multiple IT Services or multiple Customers. See Operational Level Agreement." ;
    skos:member itilproc:Continual_Service_Improvement,
        itilproc:Service_Design ;
    skos:prefLabel "Service Level Agreement" ;
