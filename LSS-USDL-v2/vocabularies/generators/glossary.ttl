itilglos:Application_Portfolio a skos:concept ;
    rdfs:label "Application Portfolio" ;
    skos:definition "A database or structured Document used to manage Applications throughout their Lifecycle. The Application Portfolio contains key Attributes of all Applications. The Application Portfolio is sometimes implemented as part of the Service Portfolio, or as part of the Configuration Management System." ;
    skos:member itilproc:Service_Design ;
