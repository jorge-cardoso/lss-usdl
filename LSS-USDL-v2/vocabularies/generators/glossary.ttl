itilglos:Middleware a skos:concept ;
    rdfs:label "Middleware" ;
    skos:definition "Software that connects two or more software Components or Applications. Middleware is usually purchased from a Supplier, rather than developed within the IT Service Provider. See Off the Shelf." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Middleware" ;
