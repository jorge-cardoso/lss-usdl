        itilglos:Supply_Chain,
        itilglos:Third_Party,
        itilglos:Underpinning_Contract .

itilglos:Monitoring a skos:concept ;
    rdfs:label "Monitoring" ;
    skos:definition "Repeated observation of a Configuration Item, IT Service or Process to detect Events and to ensure that the current status is known." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Monitoring" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Event,
        itilglos:IT_Service,
        itilglos:Process,
        itilglos:Service .

itilglos:Problem_Management a skos:concept ;
    rdfs:label "Problem Management" ;
    skos:definition "The Process responsible for managing the Lifecycle of all Problems. The primary Objectives of Problem Management are to prevent Incidents from happening, and to minimise the Impact of Incidents that cannot be prevented." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Problem Management" ;
