        itilglos:Priority,
        itilglos:Problem,
        itilglos:Process,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Urgency .

itilglos:Model a skos:concept ;
    rdfs:label "Model" ;
    skos:definition "A representation of a System, Process, IT Service, Configuration Item etc. that is used to help understand or predict future behaviour." ;
    skos:prefLabel "Model" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:IT_Service,
        itilglos:Process,
        itilglos:Service,
        itilglos:System .

itilglos:Strategy a skos:concept ;
    rdfs:label "Strategy" ;
    skos:definition "Strategic Plan designed to achieve defined Objectives." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Strategy" ;
    skos:related itilglos:Objective,
        itilglos:Plan,
        itilglos:Strategic .

itilglos:Capacity_Management a skos:concept ;
    rdfs:label "Capacity Management" ;
    skos:definition "The Process responsible for ensuring that the Capacity of IT Services and the IT Infrastructure is able to deliver agreed Service Level Targets in a Cost Effective and timely manner. Capacity Management considers all Resources required to deliver the IT Service, and plans for short, medium and long term Business Requirements." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Capacity Management" ;
    skos:related itilglos:Business,
        itilglos:Capacity,
        itilglos:Capacity_Management,
        itilglos:Cost,
        itilglos:IT_Infrastructure,
        itilglos:IT_Service,
        itilglos:Process,
        itilglos:Requirement,
        itilglos:Resource,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Target .

itilglos:Contract a skos:concept ;
    rdfs:label "Contract" ;
    skos:definition "A legally binding Agreement between two or more parties." ;
    skos:prefLabel "Contract" ;
    skos:related itilglos:Agreement .

itilglos:Design a skos:concept ;
    rdfs:label "Design" ;
    skos:definition "Service Design) An Activity or Process that identifies Requirements and then defines a solution that is able to meet these Requirements. See Service Design." ;
    skos:prefLabel "Design" ;
    skos:related itilglos:Activity,
        itilglos:Design,
        itilglos:Process,
        itilglos:Requirement,
        itilglos:Service,
        itilglos:Service_Design .

itilglos:Performance a skos:concept ;
    rdfs:label "Performance" ;
    skos:definition "A measure of what is achieved or delivered by a System, person, team, Process, or IT Service." ;
    skos:prefLabel "Performance" ;
    skos:related itilglos:IT_Service,
        itilglos:Process,
        itilglos:Service,
        itilglos:System .

itilglos:Service_Level_Target a skos:concept ;
    rdfs:label "Service Level Target" ;
    skos:definition "A commitment that is documented in a Service Level Agreement. Service Level Targets are based on Service Level Requirements, and are needed to ensure that the IT Service design is Fit for Purpose. Service Level Targets should be SMART, and are usually based on KPIs." ;
    skos:member itilproc:Continual_Service_Improvement,
        itilproc:Service_Design ;
    skos:prefLabel "Service Level Target" ;
    skos:related itilglos:Agreement,
        itilglos:Fit_for_Purpose,
        itilglos:IT_Service,
        itilglos:Requirement,
        itilglos:SMART,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Agreement,
        itilglos:Service_Level_Requirement,
        itilglos:Service_Level_Target .

itilglos:Agreement a skos:concept ;
    rdfs:label "Agreement" ;
    skos:definition "A Document that describes a formal understanding between two or more parties. An Agreement is not legally binding, unless it forms part of a Contract. See Service Level Agreement, Operational Level Agreement." ;
    skos:prefLabel "Agreement" ;
    skos:related itilglos:Agreement,
        itilglos:Contract,
        itilglos:Document,
        itilglos:Operation,
        itilglos:Operational,
        itilglos:Operational_Level_Agreement,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Agreement .

itilglos:Failure a skos:concept ;
    rdfs:label "Failure" ;
    skos:definition "Loss of ability to Operate to Specification, or to deliver the required output. The term Failure may be used when referring to IT Services, Processes, Activities, Configuration Items etc. A Failure often causes an Incident." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Failure" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Failure,
        itilglos:IT_Service,
        itilglos:Incident,
        itilglos:Operate,
        itilglos:Process,
        itilglos:Service,
        itilglos:Specification .

itilglos:Relationship a skos:concept ;
    rdfs:label "Relationship" ;
    skos:definition "A connection or interaction between two people or things. In Business Relationship Management it is the interaction between the IT Service Provider and the Business. In Configuration Management it is a link between two Configuration Items that identifies a dependency or connection between them." ;
    skos:example "For example Applications may be linked to the Servers they run on, IT Services have many links to all the CIs that contribute to them." ;
    skos:prefLabel "Relationship" ;
    skos:related itilglos:Business,
        itilglos:Business_Relationship_Management,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Configuration_Management,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Relationship,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Configuration_Management a skos:concept ;
    rdfs:label "Configuration Management" ;
    skos:definition "Component The Process responsible for maintaining information about Configuration Items required to deliver an IT Service, including their Relationships. This information is managed throughout the Lifecycle of the CI. Configuration Management is part of an overall Service Asset and Configuration Management Process." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Configuration Management" ;
    skos:related itilglos:Asset,
        itilglos:Component,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Configuration_Management,
        itilglos:IT_Service,
        itilglos:Lifecycle,
        itilglos:Process,
        itilglos:Relationship,
        itilglos:Service,
        itilglos:Service_Asset,
        itilglos:Service_Asset_and_Configuration_Management .

itilglos:Test a skos:concept ;
    rdfs:label "Test" ;
    skos:definition "An Activity that verifies that a Configuration Item, IT Service, Process, etc. meets its Specification or agreed Requirements. See Service Validation and Testing, Acceptance." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Test" ;
    skos:related itilglos:Acceptance,
        itilglos:Activity,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:IT_Service,
        itilglos:Process,
        itilglos:Requirement,
        itilglos:Service,
        itilglos:Service_Validation_and_Testing,
        itilglos:Specification,
        itilglos:Test,
        itilglos:Validation .

itilglos:Capacity a skos:concept ;
    rdfs:label "Capacity" ;
    skos:definition "The maximum Throughput that a Configuration Item or IT Service can deliver whilst meeting agreed Service Level Targets." ;
    skos:example "For some types of CI, Capacity may be the size or volume, for example a disk drive." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Capacity" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:IT_Service,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Target,
        itilglos:Throughput .

itilglos:IT_Service_Management a skos:concept ;
    rdfs:label "IT Service Management" ;
    skos:definition "The implementation and management of Quality IT Services that meet the needs of the Business. IT Service Management is performed by IT Service Providers through an appropriate mix of people, Process and Information Technology." ;
    skos:prefLabel "IT Service Management" ;
    skos:related itilglos:Business,
        itilglos:IT_Service,
        itilglos:IT_Service_Management,
        itilglos:IT_Service_Provider,
        itilglos:Information_Technology,
        itilglos:Process,
        itilglos:Quality,
        itilglos:Service,
        itilglos:Service_Management,
        itilglos:Service_Provider .

itilglos:Management_System a skos:concept ;
    rdfs:label "Management System" ;
    skos:definition "The framework of Policy, Processes and Functions that ensures an Organisation can achieve its Objectives." ;
    skos:prefLabel "Management System" ;
    skos:related itilglos:Function,
        itilglos:Objective,
        itilglos:Organisation,
        itilglos:Policy,
        itilglos:Process .

itilglos:Application a skos:concept ;
    rdfs:label "Application" ;
    skos:definition "Software that provides Functions that are required by an IT Service. Each Application may be part of more than one IT Service. An Application runs on one or more Servers or Clients." ;
    skos:prefLabel "Application" ;
    skos:related itilglos:Application,
        itilglos:Client,
        itilglos:Function,
        itilglos:IT_Service,
        itilglos:Server,
        itilglos:Service .

itilglos:Release a skos:concept ;
    rdfs:label "Release" ;
    skos:definition "A collection of hardware, software, documentation, Processes or other Components required to implement one or more approved Changes to IT Services. The contents of each Release are managed, Tested, and Deployed as a single entity." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Release" ;
    skos:related itilglos:Change,
        itilglos:Component,
        itilglos:IT_Service,
        itilglos:Process,
        itilglos:Release,
        itilglos:Service,
        itilglos:Test .

itilglos:Role a skos:concept ;
    rdfs:label "Role" ;
    skos:definition "A set of responsibilities, Activities and authorities granted to a person or team. A Role is defined in a Process." ;
    skos:example "One person or team may have multiple Roles, for example the Roles of Configuration Manager and Change Manager may be carried out by a single person." ;
    skos:prefLabel "Role" ;
    skos:related itilglos:Process,
        itilglos:Role .

itilglos:Service_Management a skos:concept ;
    rdfs:label "Service Management" ;
    skos:definition "Service Management is a set of specialized organizational capabilities for providing value to customers in the form of services." ;
    skos:prefLabel "Service Management" ;
    skos:related itilglos:Service,
        itilglos:Service_Management .

itilglos:IT_Infrastructure a skos:concept ;
    rdfs:label "IT Infrastructure" ;
    skos:definition "All of the hardware, software, networks, facilities etc. that are required to Develop, Test, deliver, Monitor, Control or support IT Services. The term IT Infrastructure includes all of the Information Technology but not the associated people, Processes and documentation." ;
    skos:prefLabel "IT Infrastructure" ;
    skos:related itilglos:Control,
        itilglos:IT_Infrastructure,
        itilglos:IT_Service,
        itilglos:Information_Technology,
        itilglos:Process,
        itilglos:Service,
        itilglos:Test .

itilglos:Availability a skos:concept ;
    rdfs:label "Availability" ;
    skos:definition "Ability of a Configuration Item or IT Service to perform its agreed Function when required. Availability is determined by Reliability, Maintainability, Serviceability, Performance, and Security. Availability is usually calculated as a percentage. This calculation is often based on Agreed Service Time and Downtime. It is Best Practice to calculate Availability using measurements of the Business output of the IT Service." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Availability" ;
    skos:related itilglos:Agreed_Service_Time,
        itilglos:Availability,
        itilglos:Best_Practice,
        itilglos:Business,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Downtime,
        itilglos:Function,
        itilglos:IT_Service,
        itilglos:Maintainability,
        itilglos:Performance,
        itilglos:Practice,
        itilglos:Reliability,
        itilglos:Security,
        itilglos:Service,
        itilglos:Serviceability .

itilglos:Recovery a skos:concept ;
    rdfs:label "Recovery" ;
    skos:definition "Returning a Configuration Item or an IT Service to a working state. Recovery of an IT Service often includes recovering data to a known consistent state. After Recovery, further steps may be needed before the IT Service can be made available to the Users (Restoration)." ;
    skos:member itilproc:Service_Design,
        itilproc:Service_Operation ;
    skos:prefLabel "Recovery" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:IT_Service,
        itilglos:Recovery,
        itilglos:Service,
        itilglos:User .

itilglos:Resource a skos:concept ;
    rdfs:label "Resource" ;
    skos:definition "A generic term that includes IT Infrastructure, people, money or anything else that might help to deliver an IT Service. Resources are considered to be Assets of an Organisation. See Capability, Service Asset." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Resource" ;
    skos:related itilglos:Asset,
        itilglos:Capability,
        itilglos:IT_Infrastructure,
        itilglos:IT_Service,
        itilglos:Organisation,
        itilglos:Resource,
        itilglos:Service,
        itilglos:Service_Asset .

itilglos:Asset a skos:concept ;
    rdfs:label "Asset" ;
    skos:definition "Service Strategy) Any Resource or Capability. Assets of a Service Provider include anything that could contribute to the delivery of a Service. Assets can be one of the following types: Management, Organisation, Process, Knowledge, People, Information, Applications, Infrastructure, and Financial Capital." ;
    skos:prefLabel "Asset" ;
    skos:related itilglos:Application,
        itilglos:Asset,
        itilglos:Capability,
        itilglos:Organisation,
        itilglos:Process,
        itilglos:Resource,
        itilglos:Service,
        itilglos:Service_Provider,
        itilglos:Service_Strategy,
        itilglos:Strategy .

itilglos:Document a skos:concept ;
    rdfs:label "Document" ;
    skos:definition "Information in readable form. A Document may be paper or electronic. See Record." ;
    skos:example "For example a Policy statement, Service Level Agreement, Incident Record, diagram of computer room layout." ;
    skos:prefLabel "Document" ;
    skos:related itilglos:Document,
        itilglos:Record .

itilglos:Record a skos:concept ;
    rdfs:label "Record" ;
    skos:definition "A Document containing the results or other output from a Process or Activity. Records are evidence of the fact that an Activity took place and may be paper or electronic." ;
    skos:example "For example, an Audit report, an Incident Record, or the minutes of a meeting." ;
    skos:prefLabel "Record" ;
    skos:related itilglos:Activity,
        itilglos:Document,
        itilglos:Process,
        itilglos:Record .

itilglos:Standard a skos:concept ;
    rdfs:label "Standard" ;
    skos:definition "A mandatory Requirement. The term Standard is also used to refer to a Code of Practice or Specification published by a Standards Organisation such as ISO or BSI. See Guideline." ;
    skos:example "Examples include ISO/IEC 20000 (an international Standard), an internal security Standard for Unix configuration, or a government Standard for how financial Records should be maintained." ;
    skos:prefLabel "Standard" ;
    skos:related itilglos:Code_of_Practice,
        itilglos:Guideline,
        itilglos:Organisation,
        itilglos:Practice,
        itilglos:Requirement,
        itilglos:Specification,
        itilglos:Standard .

itilglos:Requirement a skos:concept ;
    rdfs:label "Requirement" ;
    skos:definition "A formal statement of what is needed." ;
    skos:example "For example a Service Level Requirement, a Project Requirement or the required Deliverables for a Process." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Requirement" .

itilglos:Operation a skos:concept ;
    rdfs:label "Operation" ;
    skos:definition "Day-to-day management of an IT Service, System, or other Configuration Item. Operation is also used to mean any pre-defined Activity or Transaction." ;
    skos:example "For example loading a magnetic tape, accepting money at a point of sale, or reading data from a disk drive." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Operation" ;
    skos:related itilglos:Activity,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:IT_Service,
        itilglos:Operation,
        itilglos:Service,
        itilglos:System,
        itilglos:Transaction .

itilglos:User a skos:concept ;
    rdfs:label "User" ;
    skos:definition "A person who uses the IT Service on a day-to-day basis. Users are distinct from Customers, as some Customers do not use the IT Service directly" ;
    skos:prefLabel "User" ;
    skos:related itilglos:Customer,
        itilglos:IT_Service,
        itilglos:Service,
        itilglos:User .

itilglos:System a skos:concept ;
    rdfs:label "System" ;
    skos:definition "A number of related things that work together to achieve an overall Objective. A management System, including multiple Processes that are planned and managed together. A Database Management System or Operating System that includes many software modules that are designed to perform a set of related Functions." ;
    skos:example "For example a Quality Management System.",
        "For example: A computer System including hardware, software and Applications." ;
    skos:prefLabel "System" ;
    skos:related itilglos:Function,
        itilglos:Management_System,
        itilglos:Objective,
        itilglos:Process,
        itilglos:System .

itilglos:Function a skos:concept ;
    rdfs:label "Function" ;
    skos:definition "A team or group of people and the tools they use to carry out one or more Processes or Activities. The term Function also has two other meanings: An intended purpose of a Configuration Item, Person, Team, Process, or IT Service. And to perform the intended purpose correctly, \"The computer is Functioning\"" ;
    skos:example "For example one Function of an Email Service may be to store and forward outgoing mails, one Function of a Business Process may be to dispatch goods to Customers.",
        "For example the Service Desk." ;
    skos:prefLabel "Function" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Function,
        itilglos:IT_Service,
        itilglos:Process,
        itilglos:Service .

itilglos:Service_Level a skos:concept ;
    rdfs:label "Service Level" ;
    skos:definition "Measured and reported achievement against one or more Service Level Targets. The term Service Level is sometimes used informally to mean Service Level Target." ;
    skos:prefLabel "Service Level" ;
    skos:related itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Target .

itilglos:Objective a skos:concept ;
    rdfs:label "Objective" ;
    skos:definition "defined purpose or aim of a Process, an Activity or an Organisation as a whole. Objectives are usually expressed as measurable targets. The term Objective is also informally used to mean a Requirement. See Outcome." ;
    skos:prefLabel "Objective" ;
    skos:related itilglos:Activity,
        itilglos:Objective,
        itilglos:Organisation,
        itilglos:Outcome,
        itilglos:Process,
        itilglos:Requirement .

itilglos:Organisation a skos:concept ;
    rdfs:label "Organisation" ;
    skos:definition "A company, legal entity or other institution. SMF. The term Organisation is sometimes used to refer to any entity which has People, Resources and Budgets." ;
    skos:example "Examples of Organisations that are not companies include International Standards Organisation or it",
        "For example a Project or Business Unit." ;
    skos:prefLabel "Organisation" ;
    skos:related itilglos:Budget,
        itilglos:Organisation,
        itilglos:Resource .

itilglos:Problem a skos:concept ;
    rdfs:label "Problem" ;
    skos:definition "A cause of one or more Incidents. The cause is not usually known at the time a Problem Record is created, and the Problem Management Process is responsible for further investigation." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Problem" ;
    skos:related itilglos:Incident,
        itilglos:Problem,
        itilglos:Problem_Management,
        itilglos:Problem_Record,
        itilglos:Process,
        itilglos:Record .

itilglos:Cost a skos:concept ;
    rdfs:label "Cost" ;
    skos:definition "The amount of money spent on a specific Activity, IT Service, or Business Unit. Costs consist of real cost (money), notional cost such as people's time, and Depreciation." ;
    skos:prefLabel "Cost" ;
    skos:related itilglos:Activity,
        itilglos:Business,
        itilglos:Business_Unit,
        itilglos:Cost,
        itilglos:Depreciation,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:Lifecycle a skos:concept ;
    rdfs:label "Lifecycle" ;
    skos:definition "The various stages in the life of an IT Service, Configuration Item, Incident, Problem, Change etc. The Lifecycle defines the Categories for Status and the Status transitions that are permitted. The Expanded Incident Lifecycle includes Detect, Respond, Diagnose, Repair, Recover, Restore. The lifecycle of a Server may include: Ordered, Received, In Test, Live, Disposed etc." ;
    skos:example "For example: The Lifecycle of an Application includes Requirements, Design, Build, Deploy, Operate, Optimise." ;
    skos:prefLabel "Lifecycle" ;
    skos:related itilglos:Change,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Expanded_Incident_Lifecycle,
        itilglos:IT_Service,
        itilglos:Incident,
        itilglos:Lifecycle,
        itilglos:Live,
        itilglos:Problem,
        itilglos:Repair,
        itilglos:Restore,
        itilglos:Server,
        itilglos:Service,
        itilglos:Status,
        itilglos:Test .

itilglos:IT_Service_Provider a skos:concept ;
    rdfs:label "IT Service Provider" ;
    skos:definition "A Service Provider that provides IT Services to Internal Customers or External Customers." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "IT Service Provider" ;
    skos:related itilglos:Customer,
        itilglos:External_Customer,
        itilglos:IT_Service,
        itilglos:Internal_Customer,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Activity a skos:concept ;
    rdfs:label "Activity" ;
    skos:definition "A set of actions designed to achieve a particular result. Activities are usually defined as part of Processes or Plans, and are documented in Procedures." ;
    skos:prefLabel "Activity" ;
    skos:related itilglos:Plan,
        itilglos:Procedure,
        itilglos:Process .

itilglos:Plan a skos:concept ;
    rdfs:label "Plan" ;
    skos:definition "A detailed proposal which describes the Activities and Resources needed to achieve an Objective. ISO/IEC 20000 requires a Plan for the management of each IT Service Management Process." ;
    skos:example "For example a Plan to implement a new IT Service or Process." ;
    skos:prefLabel "Plan" ;
    skos:related <http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_20000>,
        itilglos:IT_Service,
        itilglos:IT_Service_Management,
        itilglos:Objective,
        itilglos:Plan,
        itilglos:Process,
        itilglos:Resource,
        itilglos:Service,
        itilglos:Service_Management .

itilglos:Incident a skos:concept ;
    rdfs:label "Incident" ;
    skos:definition "Service Operation) An unplanned interruption to an IT Service or a reduction in the Quality of an IT Service. Failure of a Configuration Item that has not yet impacted Service is also an Incident." ;
    skos:example "For example Failure of one disk from a mirror set." ;
    skos:prefLabel "Incident" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Failure,
        itilglos:IT_Service,
        itilglos:Incident,
        itilglos:Operation,
        itilglos:Quality,
        itilglos:Service,
        itilglos:Service_Operation .

itilglos:Change a skos:concept ;
    rdfs:label "Change" ;
    skos:definition "Service Transition) The addition, modification or removal of anything that could have an effect on IT Services. The Scope should include all IT Services, Configuration Items, Processes, Documentation etc." ;
    skos:prefLabel "Change" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Document,
        itilglos:IT_Service,
        itilglos:Process,
        itilglos:Scope,
        itilglos:Service,
        itilglos:Service_Transition,
        itilglos:Transition .

itilglos:Customer a skos:concept ;
    rdfs:label "Customer" ;
    skos:definition "Someone who buys goods or Services. The Customer of an IT Service Provider is the person or group who defines and agrees the Service Level Targets." ;
    skos:example "The term Customers is also sometimes informally used to mean Users, for example \"this is a Customer focussed Organisation\"." ;
    skos:prefLabel "Customer" ;
    skos:related itilglos:Customer,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Target,
        itilglos:Service_Provider .

itilglos:Service_Provider a skos:concept ;
    rdfs:label "Service Provider" ;
    skos:definition "An Organisation supplying Services to one or more Internal Customers or External Customers. Service Provider is often used as an abbreviation for IT Service Provider. See Type I Service Provider, Type II Service Provider, Type III Service Provider" ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Service Provider" ;
    skos:related itilglos:Customer,
        itilglos:External_Customer,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Internal_Customer,
        itilglos:Organisation,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Configuration_Item a skos:concept ;
    rdfs:label "Configuration Item" ;
    skos:definition "Component that needs to be managed in order to deliver an IT Service. Information about each CI is recorded in a Configuration Record within the Configuration Management System and is maintained throughout its Lifecycle by Configuration Management. CIs are under the control of Change Management. CIs typically include IT Services, hardware, software, buildings, people, and formal documentation such as Process documentation and SLAs." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Configuration Item" ;
    skos:related itilglos:Change,
        itilglos:Change_Management,
        itilglos:Component,
        itilglos:Configuration,
        itilglos:Configuration_Management,
        itilglos:Configuration_Management_System,
        itilglos:Configuration_Record,
        itilglos:IT_Service,
        itilglos:Lifecycle,
        itilglos:Management_System,
        itilglos:Process,
        itilglos:Record,
        itilglos:Service,
        itilglos:System .

itilglos:Business a skos:concept ;
    rdfs:label "Business" ;
    skos:definition "An overall corporate entity or Organisation formed of a number of Business Units. In the context of ITSM, the term Business includes public sector and not-for-profit organisations, as well as companies. An IT Service Provider provides IT Services to a Customer within a Business. The IT Service Provider may be part of the same Business as their Customer (Internal Service Provider), or part of another Business (External Service Provider)." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Business" ;
    skos:related itilglos:Business,
        itilglos:Business_Unit,
        itilglos:Customer,
        itilglos:External_Service_Provider,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Internal_Service_Provider,
        itilglos:Organisation,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Configuration a skos:concept ;
    rdfs:label "Configuration" ;
    skos:definition "A generic term, used to describe a group of Configuration Items that work together to deliver an IT Service, or a recognizable part of an IT Service. Configuration is also used to describe the parameter settings for one or more CIs." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Configuration" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:Process a skos:concept ;
    rdfs:label "Process" ;
    skos:definition "A structured set of Activities designed to accomplish a specific Objective. A Process takes one or more defined inputs and turns them into defined outputs. A Process may include any of the Roles, responsibilities, tools and management Controls required to reliably deliver the outputs. A Process may define Policies, Standards, Guidelines, Activities, and Work Instructions if they are needed." ;
    skos:prefLabel "Process" ;
    skos:related itilglos:Control,
        itilglos:Guideline,
        itilglos:Objective,
        itilglos:Process,
        itilglos:Role,
        itilglos:Standard,
        itilglos:Work_Instruction .

itilglos:IT_Service a skos:concept ;
    rdfs:label "IT Service" ;
    skos:definition "A Service provided to one or more Customers by an IT Service Provider. An IT Service is based on the use of Information Technology and supports the Customer's Business Processes. An IT Service is made up from a combination of people, Processes and technology and should be defined in a Service Level Agreement." ;
    skos:prefLabel "IT Service" ;
    skos:related itilglos:Agreement,
        itilglos:Business,
        itilglos:Business_Process,
        itilglos:Customer,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Information_Technology,
        itilglos:Process,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Service_Level_Agreement,
        itilglos:Service_Provider .

itilglos:Service a skos:concept ;
    rdfs:label "Service" ;
    skos:definition "A means of delivering value to Customers by facilitating Outcomes Customers want to achieve without the ownership of specific Costs and Risks." ;
    skos:prefLabel "Service" ;
    skos:related itilglos:Cost,
        itilglos:Customer,
        itilglos:Outcome,
        itilglos:Risk .

