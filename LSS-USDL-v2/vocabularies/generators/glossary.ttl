        itilglos:Asset_Register .

itilglos:Automatic_Call_Distribution a skos:concept ;
    rdfs:label "Automatic Call Distribution" ;
    skos:definition "Use of Information Technology to direct an incoming telephone call to the most appropriate person in the shortest possible time. ACD is sometimes called Automated Call Distribution." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Automatic Call Distribution" ;
    skos:related itilglos:Call,
