itilglos:Business_Capacity_Management a skos:concept ;
    rdfs:label "Business Capacity Management" ;
    skos:definition "In the context of ITSM, Business Capacity Management is the Activity responsible for understanding future Business Requirements for use in the Capacity Plan." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Business Capacity Management" ;
