itilglos:Off_the_Shelf a skos:concept ;
    rdfs:label "Off the Shelf" ;
    skos:definition "Synonym for Commercial Off the Shelf." ;
    skos:prefLabel "Off the Shelf" ;
    skos:related itilglos:Off_the_Shelf .

itilglos:Operational_Cost a skos:concept ;
    rdfs:label "Operational Cost" ;
    skos:definition "Cost resulting from running the IT Services. Often repeating payments." ;
    skos:example "For example staff costs, hardware maintenance and electricity (also known as \"current expenditure\" or \"revenue expenditure\")." ;
    skos:prefLabel "Operational Cost" ;
    skos:related itilglos:Cost,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:Outsourcing a skos:concept ;
    rdfs:label "Outsourcing" ;
    skos:definition "Using an External Service Provider to manage IT Services. See Service Sourcing, Type III Service Provider." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Outsourcing" ;
    skos:related itilglos:External_Service_Provider,
        itilglos:IT_Service,
        itilglos:Service,
        itilglos:Service_Provider,
        itilglos:Service_Sourcing .

itilglos:Pareto_Principle a skos:concept ;
    rdfs:label "Pareto Principle" ;
    skos:definition "A technique used to prioritise Activities. The Pareto Principle says that 80% of the value of any Activity is created with 20% of the effort. Pareto Analysis is also used in Problem Management to prioritise possible Problem causes for investigation." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Pareto Principle" ;
    skos:related itilglos:Activity,
        itilglos:Pareto_Principle,
        itilglos:Problem,
        itilglos:Problem_Management .

itilglos:Partnership a skos:concept ;
    rdfs:label "Partnership" ;
    skos:definition "A relationship between two Organisations which involves working closely together for common goals or mutual benefit. The IT Service Provider should have a Partnership with the Business, and with Third Parties who are critical to the delivery of IT Services. See Value Network." ;
    skos:prefLabel "Partnership" ;
    skos:related itilglos:Business,
