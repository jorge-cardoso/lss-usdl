itilglos:Asset_Management a skos:concept ;
    rdfs:label "Asset Management" ;
    skos:definition "Asset Management is the Process responsible for tracking and reporting the value and ownership of financial Assets throughout their Lifecycle. Asset Management is part of an overall Service Asset and Configuration Management Process." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Asset Management" ;
    skos:related itilglos:Asset,
