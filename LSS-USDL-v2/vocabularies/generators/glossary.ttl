itilglos:Knowledge_Management a skos:concept ;
    rdfs:label "Knowledge Management" ;
    skos:definition "The Process responsible for gathering, analysing, storing and sharing knowledge and information within an Organisation. The primary purpose of Knowledge Management is to improve Efficiency by reducing the need to rediscover knowledge." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Knowledge Management" ;
