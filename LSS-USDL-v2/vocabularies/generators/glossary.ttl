        itilglos:System .

itilglos:Impact a skos:concept ;
    rdfs:label "Impact" ;
    skos:definition "A measure of the effect of an Incident, Problem or Change on Business Processes. Impact is often based on how Service Levels will be affected. Impact and Urgency are used to assign Priority." ;
    skos:member itilproc:Service_Operation,
        itilproc:Service_Transition ;
    skos:prefLabel "Impact" ;
