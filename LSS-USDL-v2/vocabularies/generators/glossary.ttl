        itilglos:Metric,
        itilglos:Plan,
        itilglos:Service .

itilglos:Live a skos:concept ;
    rdfs:label "Live" ;
    skos:definition "Refers to an IT Service or Configuration Item that is being used to deliver Service to a Customer." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Live" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Customer,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:Planning a skos:concept ;
    rdfs:label "Planning" ;
    skos:definition "An Activity responsible for creating one or more Plans." ;
    skos:example "For example, Capacity Planning." ;
    skos:prefLabel "Planning" ;
    skos:related itilglos:Activity,
        itilglos:Plan .

itilglos:Practice a skos:concept ;
    rdfs:label "Practice" ;
    skos:definition "A way of working, or a way in which work must be done. Practices can include Activities, Processes, Functions, Standards and Guidelines. See Best Practice." ;
