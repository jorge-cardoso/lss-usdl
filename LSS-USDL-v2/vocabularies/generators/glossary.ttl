itilglos:eSourcing_Capability_Model_for_Service_Providers a skos:concept ;
    rdfs:label "eSourcing Capability Model for Service Providers" ;
    skos:definition "A framework to help IT Service Providers develop their IT Service Management Capabilities from a Service Sourcing perspective. eSCM-SP was developed by Carnegie Mellon University. See eSCM-CL." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "eSourcing Capability Model for Service Providers" ;
    skos:related itilglos:IT_Service,
        itilglos:IT_Service_Management,
        itilglos:IT_Service_Provider,
        itilglos:Service,
        itilglos:Service_Management,
        itilglos:Service_Provider,
        itilglos:Service_Sourcing .

itilglos:pro-forma a skos:concept ;
    rdfs:label "pro-forma" ;
    skos:definition "" ;
    skos:example "A template, or example Document containing example data that will be replaced with the real values when these are available." ;
    skos:prefLabel "pro-forma" .

itilglos:Access_Management a skos:concept ;
    rdfs:label "Access Management" ;
    skos:definition "The Process responsible for allowing Users to make use of IT Services, data, or other Assets. Access Management helps to protect the Confidentiality, Integrity and Availability of Assets by ensuring that only authorized Users are able to access or modify the Assets. Access Management is sometimes referred to as Rights Management or Identity Management." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Access Management" ;
    skos:related itilglos:Access_Management,
        itilglos:Asset,
        itilglos:Availability,
        itilglos:Confidentiality,
        itilglos:IT_Service,
        itilglos:Identity,
        itilglos:Integrity,
        itilglos:Process,
        itilglos:Rights,
        itilglos:Service,
        itilglos:User .

itilglos:Account_Manager a skos:concept ;
    rdfs:label "Account Manager" ;
    skos:definition "A Role that is very similar to Business Relationship Manager, but includes more commercial aspects. Most commonly used when dealing with External Customers." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Account Manager" ;
    skos:related itilglos:Business,
        itilglos:Business_Relationship_Manager,
        itilglos:Customer,
        itilglos:External_Customer,
        itilglos:Relationship,
        itilglos:Role .

itilglos:Accredited a skos:concept ;
    rdfs:label "Accredited" ;
    skos:definition "Officially authorised to carry out a Role." ;
    skos:example "For example an Accredited body may be authorised to provide training or to conduct Audits." ;
    skos:prefLabel "Accredited" ;
    skos:related itilglos:Role .

itilglos:Active_Monitoring a skos:concept ;
    rdfs:label "Active Monitoring" ;
    skos:definition "Monitoring of a Configuration Item or an IT Service that uses automated regular checks to discover the current status." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Active Monitoring" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:IT_Service,
        itilglos:Monitoring,
        itilglos:Service .

itilglos:Architecture a skos:concept ;
    rdfs:label "Architecture" ;
    skos:definition "The structure of a System or IT Service, including the Relationships of Components to each other and to the environment they are in. Architecture also includes the Standards and Guidelines which guide the design and evolution of the System." ;
