    skos:related itilglos:Change,
        itilglos:Incident,
        itilglos:Problem,
        itilglos:Status .

itilglos:eSourcing_Capability_Model_for_Client_Organizations a skos:concept ;
    rdfs:label "eSourcing Capability Model for Client Organizations" ;
    skos:definition "A framework to help Organisations guide their analysis and decisions on Service Sourcing Models and Strategies. eSCM-CL was developed by Carnegie Mellon University. See eSCM-SP" ;
