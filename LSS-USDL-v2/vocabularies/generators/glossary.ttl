    skos:prefLabel "Business Case" ;
    skos:related itilglos:Cost,
        itilglos:Cost_Benefit_Analysis,
        itilglos:Risk .

itilglos:Business_Impact_Analysis a skos:concept ;
    rdfs:label "Business Impact Analysis" ;
    skos:definition "BIA is the Activity in Business Continuity Management that identifies Vital Business Functions and their dependencies. These dependencies may include Suppliers, people, other Business Processes, IT Services etc. BIA defines the recovery requirements for IT Services. These requirements include Recovery Time Objectives, Recovery Point Objectives and minimum Service Level Targets for each IT Service." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Business Impact Analysis" ;
    skos:related itilglos:Activity,
        itilglos:Business,
        itilglos:Business_Continuity_Management,
        itilglos:Business_Process,
        itilglos:Function,
        itilglos:IT_Service,
        itilglos:Objective,
        itilglos:Process,
        itilglos:Recovery,
        itilglos:Recovery_Point_Objective,
