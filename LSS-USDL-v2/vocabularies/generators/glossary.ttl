    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Call Type" ;
    skos:related itilglos:Call,
        itilglos:Call_Type,
        itilglos:Category,
        itilglos:Incident,
        itilglos:Service,
        itilglos:Service_Desk,
        itilglos:Service_Request .

itilglos:Change_History a skos:concept ;
    rdfs:label "Change History" ;
    skos:definition "Information about all changes made to a Configuration Item during its life. Change History consists of all those Change Records that apply to the CI." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Change History" ;
    skos:related itilglos:Change,
        itilglos:Change_History,
