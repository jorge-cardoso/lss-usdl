itilglos:Supply_Chain a skos:concept ;
    rdfs:label "Supply Chain" ;
    skos:definition "The Activities in a Value Chain carried out by Suppliers. A Supply Chain typically involves multiple Suppliers, each adding value to the product or Service. See Value Network." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Supply Chain" ;
    skos:related itilglos:Service,
        itilglos:Supplier,
        itilglos:Supply_Chain,
        itilglos:Value_Chain,
        itilglos:Value_Network .

itilglos:Throughput a skos:concept ;
    rdfs:label "Throughput" ;
    skos:definition "A measure of the number of Transactions, or other Operations, performed in a fixed time." ;
    skos:example "For example 5000 emails sent per hour, or 200 disk I/Os per second." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Throughput" ;
    skos:related itilglos:Operation,
        itilglos:Transaction .

itilglos:Tuning a skos:concept ;
    rdfs:label "Tuning" ;
    skos:definition "The Activity responsible for Planning Changes to make the most efficient use of Resources. Tuning is part of Performance Management, which also includes Performance Monitoring and implementation of the required Changes." ;
    skos:prefLabel "Tuning" ;
    skos:related itilglos:Activity,
        itilglos:Change,
        itilglos:Monitoring,
        itilglos:Performance,
        itilglos:Performance_Management,
        itilglos:Plan,
        itilglos:Planning,
        itilglos:Resource,
        itilglos:Tuning .

itilglos:Value_Chain a skos:concept ;
    rdfs:label "Value Chain" ;
    skos:definition "A sequence of Processes that creates a product or Service that is of value to a Customer. Each step of the sequence builds on the previous steps and contributes to the overall product or Service. See Value Network." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Value Chain" ;
    skos:related itilglos:Customer,
        itilglos:Process,
        itilglos:Service,
        itilglos:Value_Network .

itilglos:Value_for_Money a skos:concept ;
    rdfs:label "Value for Money" ;
    skos:definition "An informal measure of Cost Effectiveness. Value for Money is often based on a comparison with the Cost of alternatives. See Cost Benefit Analysis." ;
    skos:prefLabel "Value for Money" ;
    skos:related itilglos:Cost,
        itilglos:Cost_Benefit_Analysis,
        itilglos:Cost_Effectiveness,
        itilglos:Effectiveness,
        itilglos:Value_for_Money .

itilglos:Value_on_Investment a skos:concept ;
    rdfs:label "Value on Investment" ;
    skos:definition "A measurement of the expected benefit of an investment. VOI considers both financial and intangible benefits. See Return on Investment." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "Value on Investment" ;
    skos:related itilglos:Return_on_Investment .

itilglos:Variable_Cost a skos:concept ;
    rdfs:label "Variable Cost" ;
    skos:definition "A Cost that depends on how much the IT Service is used, how many products are produced, the number and type of Users, or something else that cannot be fixed in advance. See Variable Cost Dynamics" ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Variable Cost" ;
