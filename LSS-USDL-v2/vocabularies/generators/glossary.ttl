@prefix itilglos: <http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#> .
@prefix itilproc: <http://eden.dei.uc.pt/~jcardoso/rdf/itil/processes.ttl#> .
@prefix itilrole: <http://eden.dei.uc.pt/~jcardoso/rdf/itil/roles.ttl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl> a <http://www.w3.org/2002/07/owl#ontology> .

itilglos: dc:creator itilglos:jcardoso .

itilglos:Analytical_Modelling a skos:concept ;
    rdfs:label "Analytical Modelling" ;
    skos:definition "A technique that uses mathematical Models to predict the behaviour of a Configuration Item or IT Service. Analytical Models are commonly used in Capacity Management and Availability Management." ;
    skos:member itilproc:Continual_Service_Improvement,
        itilproc:Service_Design,
        itilproc:Service_Strategy ;
    skos:prefLabel "Analytical Modelling" ;
    skos:related itilglos:Availability,
        itilglos:Availability_Management,
        itilglos:Capacity,
        itilglos:Capacity_Management,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:IT_Service,
        itilglos:Model,
        itilglos:Service .

itilglos:Application_Management a skos:concept ;
    rdfs:label "Application Management" ;
    skos:definition "A database or structured Document used to manage Applications throughout their Lifecycle. The Application Portfolio contains key Attributes of all Applications. The Application Portfolio is sometimes implemented as part of the Service Portfolio, or as part of the Configuration Management System." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Application Management" ;
    skos:related itilglos:Application,
        itilglos:Application_Portfolio,
        itilglos:Attribute,
        itilglos:Configuration,
        itilglos:Configuration_Management,
        itilglos:Configuration_Management_System,
        itilglos:Document,
        itilglos:Lifecycle,
        itilglos:Management_System,
        itilglos:Service,
        itilglos:Service_Portfolio,
        itilglos:System .

itilglos:Application_Service_Provider a skos:concept ;
    rdfs:label "Application Service Provider" ;
    skos:definition "The Function responsible for managing Applications throughout their Lifecycle." ;
    skos:member itilproc:Service_Design,
        itilproc:Service_Operation ;
    skos:prefLabel "Application Service Provider" ;
    skos:related itilglos:Application,
        itilglos:Function,
        itilglos:Lifecycle .

itilglos:Application_Sizing a skos:concept ;
    rdfs:label "Application Sizing" ;
    skos:definition "An External Service Provider that provides IT Services using Applications running at the Service Provider's premises. Users access the Applications by network connections to the Service Provider." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Application Sizing" ;
    skos:related itilglos:Application,
        itilglos:External_Service_Provider,
        itilglos:IT_Service,
        itilglos:Service,
        itilglos:Service_Provider,
        itilglos:User .

itilglos:Authority_Matrix a skos:concept ;
    rdfs:label "Authority Matrix" ;
    skos:definition "Synonym f√ºr RACI" ;
    skos:prefLabel "Authority Matrix" ;
    skos:related itilglos:RACI .

itilglos:Availability_Management_Information_System a skos:concept ;
    rdfs:label "Availability Management Information System" ;
    skos:definition "virtual repository of all Availability Management data, usually stored in multiple physical locations.  See Service Knowledge Management System." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Availability Management Information System" ;
    skos:related itilglos:Availability,
        itilglos:Availability_Management,
        itilglos:Knowledge_Management,
        itilglos:Management_System,
        itilglos:Service,
        itilglos:Service_Knowledge_Management_System,
        itilglos:System .

itilglos:Availability_Plan a skos:concept ;
    rdfs:label "Availability Plan" ;
    skos:definition "A Plan to ensure that existing and future Availability Requirements for IT Services can be provided Cost Effectively." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Availability Plan" ;
    skos:related itilglos:Availability,
        itilglos:Cost,
        itilglos:IT_Service,
        itilglos:Plan,
        itilglos:Requirement,
        itilglos:Service .

itilglos:Back-out a skos:concept ;
    rdfs:label "Back-out" ;
    skos:definition "Synonym for Remediation" ;
    skos:prefLabel "Back-out" ;
    skos:related itilglos:Remediation .

itilglos:British_Standards_Institution a skos:concept ;
    rdfs:label "British Standards Institution" ;
    skos:definition "The UK National Standards body, responsible for creating and maintaining British Standards. See http://www.bsi-global.com for more information." ;
    skos:prefLabel "British Standards Institution" ;
    skos:related itilglos:Standard .

itilglos:Build_Environment a skos:concept ;
    rdfs:label "Build Environment" ;
    skos:definition "controlled Environment where Applications, IT Services and other Builds are assembled prior to being moved into a Test or Live Environment." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Build Environment" ;
    skos:related itilglos:Application,
        itilglos:Build,
        itilglos:Environment,
        itilglos:IT_Service,
        itilglos:Live,
        itilglos:Live_Environment,
        itilglos:Service,
        itilglos:Test .

itilglos:Business_Operations a skos:concept ;
    rdfs:label "Business Operations" ;
    skos:definition "The day-to-day execution, monitoring and management of Business Processes." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Business Operations" ;
    skos:related itilglos:Business,
        itilglos:Business_Process,
        itilglos:Process .

itilglos:Business_Perspective a skos:concept ;
    rdfs:label "Business Perspective" ;
    skos:definition "An understanding of the Service Provider and IT Services from the point of view of the Business, and an understanding of the Business from the point of view of the Service Provider." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "Business Perspective" ;
    skos:related itilglos:Business,
        itilglos:IT_Service,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Business_Service_Management a skos:concept ;
    rdfs:label "Business Service Management" ;
    skos:definition "An IT Service that directly supports a Business Process, as opposed to an Infrastructure Service which is used internally by the IT Service Provider and is not usually visible to the Business. The term Business Service is also used to mean a Service that is delivered to Business Customers by Business Units. Successful delivery of Business Services often depends on one or more IT Services." ;
    skos:example "For example delivery of financial services to Customers of a bank, or goods to the Customers of a retail store." ;
    skos:member itilproc:Service_Design,
        itilproc:Service_Strategy ;
    skos:prefLabel "Business Service Management" ;
    skos:related itilglos:Business,
        itilglos:Business_Customer,
        itilglos:Business_Process,
        itilglos:Business_Service,
        itilglos:Business_Unit,
        itilglos:Customer,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Infrastructure_Service,
        itilglos:Process,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Call_Centre a skos:concept ;
    rdfs:label "Call Centre" ;
    skos:definition "An Organisation or Business Unit which handles large numbers of incoming and outgoing telephone calls. See Service Desk." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Call Centre" ;
    skos:related itilglos:Business,
        itilglos:Business_Unit,
        itilglos:Organisation,
        itilglos:Service,
        itilglos:Service_Desk .

itilglos:Capacity_Management_Information_System a skos:concept ;
    rdfs:label "Capacity Management Information System" ;
    skos:definition "A virtual repository of all Capacity Management data, usually stored in multiple physical locations. See Service Knowledge Management System." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Capacity Management Information System" ;
    skos:related itilglos:Capacity,
        itilglos:Capacity_Management,
        itilglos:Knowledge_Management,
        itilglos:Management_System,
        itilglos:Service,
        itilglos:Service_Knowledge_Management_System,
        itilglos:System .

itilglos:Capacity_Planning a skos:concept ;
    rdfs:label "Capacity Planning" ;
    skos:definition "Activity within Capacity Management responsible for creating a Capacity Plan." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Capacity Planning" ;
    skos:related itilglos:Activity,
        itilglos:Capacity,
        itilglos:Capacity_Management,
        itilglos:Capacity_Plan,
        itilglos:Plan .

itilglos:Capital_Item a skos:concept ;
    rdfs:label "Capital Item" ;
    skos:definition "An Asset that is of interest to Financial Management because it is above an agreed financial value." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Capital Item" ;
    skos:related itilglos:Asset,
        itilglos:Financial_Management .

itilglos:Capitalization a skos:concept ;
    rdfs:label "Capitalization" ;
    skos:definition "Identifying major Cost as capital, even though no Asset is purchased. This is done to spread the impact of the Cost over multiple accounting periods." ;
    skos:example "The most common example of this is software development, or purchase of a software license." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Capitalization" ;
    skos:related itilglos:Asset,
        itilglos:Cost .

itilglos:Change_Request a skos:concept ;
    rdfs:label "Change Request" ;
    skos:definition "Synonym for Request for Change." ;
    skos:prefLabel "Change Request" ;
    skos:related itilglos:Change,
        itilglos:Request_for_Change .

itilglos:Chronological_Analysis a skos:concept ;
    rdfs:label "Chronological Analysis" ;
    skos:definition "A technique used to help identify possible causes of Problems. All available data about the Problem is collected and sorted by date and time to provide a detailed timeline. This can make it possible to identify which Events may have been triggered by others." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Chronological Analysis" ;
    skos:related itilglos:Event,
        itilglos:Problem .

itilglos:Commercial_off_the_Shelf a skos:concept ;
    rdfs:label "Commercial off the Shelf" ;
    skos:definition "Service Design) Application software or Middleware that can be purchased from a Third Party." ;
    skos:prefLabel "Commercial off the Shelf" ;
    skos:related itilglos:Application,
        itilglos:Design,
        itilglos:Middleware,
        itilglos:Service,
        itilglos:Service_Design,
        itilglos:Third_Party .

itilglos:Component_CI a skos:concept ;
    rdfs:label "Component CI" ;
    skos:definition "A Configuration Item that is part of an Assembly." ;
    skos:example "For example, a CPU or Memory CI may be part of a Server CI." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Component CI" ;
    skos:related itilglos:Assembly,
        itilglos:Configuration,
        itilglos:Configuration_Item .

itilglos:Component_Failure_Impact_Analysis a skos:concept ;
    rdfs:label "Component Failure Impact Analysis" ;
    skos:definition "A technique that helps to identify the impact of CI failure on IT Services. A matrix is created with IT Services on one edge and CIs on the other. This enables the identification of critical CIs (that could cause the failure of multiple IT Services) and of fragile IT Services (that have multiple Single Points of Failure)." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Component Failure Impact Analysis" ;
    skos:related itilglos:Failure,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:Computer_Telephony_Integration a skos:concept ;
    rdfs:label "Computer Telephony Integration" ;
    skos:definition "CTI is a general term covering any kind of integration between computers and telephone Systems. It is most commonly used to refer to Systems where an Application displays detailed screens relating to incoming or outgoing telephone calls. See Automatic Call Distribution, Interactive Voice Response." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Computer Telephony Integration" ;
    skos:related itilglos:Application,
        itilglos:Automatic_Call_Distribution,
        itilglos:Call,
        itilglos:Interactive_Voice_Response,
        itilglos:System .

itilglos:Concurrency a skos:concept ;
    rdfs:label "Concurrency" ;
    skos:definition "A measure of the number of Users engaged in the same Operation at the same time." ;
    skos:prefLabel "Concurrency" ;
    skos:related itilglos:Operation,
        itilglos:User .

itilglos:Configuration_Control a skos:concept ;
    rdfs:label "Configuration Control" ;
    skos:definition "The Activity responsible for ensuring that adding, modifying or removing a CI is properly managed, for example by submitting a Request for Change or Service Request." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Configuration Control" ;
    skos:related itilglos:Activity,
        itilglos:Change,
        itilglos:Request_for_Change,
        itilglos:Service,
        itilglos:Service_Request .

itilglos:Configuration_Structure a skos:concept ;
    rdfs:label "Configuration Structure" ;
    skos:definition "The hierarchy and other Relationships between all the Configuration Items that comprise a Configuration." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Configuration Structure" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Relationship .

itilglos:Continuous_Availability a skos:concept ;
    rdfs:label "Continuous Availability" ;
    skos:definition "An approach or design to achieve 100% Availability. A Continuously Available IT Service has no planned or unplanned Downtime." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Continuous Availability" ;
    skos:related itilglos:Availability,
        itilglos:Downtime,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:Continuous_Operation a skos:concept ;
    rdfs:label "Continuous Operation" ;
    skos:definition "An approach or design to eliminate planned Downtime of an IT Service. Note that individual Configuration Items may be down even though the IT Service is Available." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Continuous Operation" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Downtime,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:Control_Processes a skos:concept ;
    rdfs:label "Control Processes" ;
    skos:definition "The ISO/IEC 20000 Process group that includes Change Management and Configuration Management." ;
    skos:prefLabel "Control Processes" ;
    skos:related itilglos:Change,
        itilglos:Change_Management,
        itilglos:Configuration,
        itilglos:Configuration_Management,
        <http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_20000>,
        itilglos:Process .

itilglos:Control_perspective a skos:concept ;
    rdfs:label "Control perspective" ;
    skos:definition "An approach to the management of IT Services, Processes, Functions, Assets etc. There can be several different Control Perspectives on the same IT Service, Process etc., allowing different individuals or teams to focus on what is important and relevant to their specific Role." ;
    skos:example "Example Control Perspectives include Reactive and Proactive management within IT Operations, or a Lifecycle view for an Application Project team." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Control perspective" ;
    skos:related itilglos:Asset,
        itilglos:Control,
        itilglos:Function,
        itilglos:IT_Service,
        itilglos:Process,
        itilglos:Role,
        itilglos:Service .

itilglos:Cost_Center a skos:concept ;
    rdfs:label "Cost Center" ;
    skos:definition "A Business Unit or Project to which Costs are assigned. A Cost Centre does not charge for Services provided. An IT Service Provider can be run as a Cost Centre or a Profit Centre." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Cost Center" ;
    skos:related itilglos:Business,
        itilglos:Business_Unit,
        itilglos:Cost,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Profit_Centre,
        itilglos:Project,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Cost_Management a skos:concept ;
    rdfs:label "Cost Management" ;
    skos:definition "A general term that is used to refer to Budgeting and Accounting, sometimes used as a synonym for Financial Management" ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Cost Management" ;
    skos:related itilglos:Accounting,
        itilglos:Budget,
        itilglos:Budgeting,
        itilglos:Financial_Management .

itilglos:Course_Corrections a skos:concept ;
    rdfs:label "Course Corrections" ;
    skos:definition "Changes made to a Plan or Activity that has already started, to ensure that it will meet its Objectives. Course corrections are made as a result of Monitoring progress." ;
    skos:prefLabel "Course Corrections" ;
    skos:related itilglos:Activity,
        itilglos:Change,
        itilglos:Monitoring,
        itilglos:Objective,
        itilglos:Plan .

itilglos:Data-to-Information-to-Knowledge-to-Wisdom a skos:concept ;
    rdfs:label "Data-to-Information-to-Knowledge-to-Wisdom" ;
    skos:definition "A way of understanding the relationships between data, information, knowledge, and wisdom. DIKW shows how each of these builds on the others." ;
    skos:prefLabel "Data-to-Information-to-Knowledge-to-Wisdom" .

itilglos:Definitive_Media_Library a skos:concept ;
    rdfs:label "Definitive Media Library" ;
    skos:definition "One or more locations in which the definitive and approved versions of all software Configuration Items are securely stored. The DML may also contain associated CIs such as licenses and documentation. The DML is a single logical storage area even if there are multiple locations. All software in the DML is under the control of Change and Release Management and is recorded in the Configuration Management System. Only software from the DML is acceptable for use in a Release." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Definitive Media Library" ;
    skos:related itilglos:Change,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Configuration_Management,
        itilglos:Configuration_Management_System,
        itilglos:Management_System,
        itilglos:Release,
        itilglos:Release_Management,
        itilglos:System .

itilglos:Dependency a skos:concept ;
    rdfs:label "Dependency" ;
    skos:definition "The direct or indirect reliance of one Process or Activity upon another." ;
    skos:prefLabel "Dependency" ;
    skos:related itilglos:Activity,
        itilglos:Process .

itilglos:Directory_Service a skos:concept ;
    rdfs:label "Directory Service" ;
    skos:definition "An Application that manages information about IT Infrastructure available on a network, and corresponding User access Rights." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Directory Service" ;
    skos:related itilglos:Application,
        itilglos:IT_Infrastructure,
        itilglos:Rights,
        itilglos:User .

itilglos:Driver a skos:concept ;
    rdfs:label "Driver" ;
    skos:definition "Something that influences Strategy, Objectives or Requirements." ;
    skos:example "For example new legislation or the actions of competitors." ;
    skos:prefLabel "Driver" ;
    skos:related itilglos:Objective,
        itilglos:Requirement,
        itilglos:Strategy .

itilglos:Economies_of_scale a skos:concept ;
    rdfs:label "Economies of scale" ;
    skos:definition "Service Strategy) The reduction in average Cost that is possible from increasing the usage of an IT Service or Asset. See Economies of Scope." ;
    skos:prefLabel "Economies of scale" ;
    skos:related itilglos:Asset,
        itilglos:Cost,
        itilglos:IT_Service,
        itilglos:Scope,
        itilglos:Service,
        itilglos:Service_Strategy,
        itilglos:Strategy .

itilglos:Economies_of_scope a skos:concept ;
    rdfs:label "Economies of scope" ;
    skos:definition "The reduction in Cost that is allocated to an IT Service by using an existing Asset for an additional purpose. See Economies of Scale." ;
    skos:example "For example delivering a new IT Service from existing IT Infrastructure." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Economies of scope" ;
    skos:related itilglos:Asset,
        itilglos:Cost,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:Exception_Report a skos:concept ;
    rdfs:label "Exception Report" ;
    skos:definition "A Document containing details of one or more KPIs or other important targets that have exceeded defined Thresholds." ;
    skos:example "Examples include SLA targets being missed or about to be missed, and a Performance Metric indicating a potential Capacity problem." ;
    skos:prefLabel "Exception Report" ;
    skos:related itilglos:Document,
        itilglos:Threshold .

itilglos:External_Sourcing a skos:concept ;
    rdfs:label "External Sourcing" ;
    skos:definition "Synonym for Outsourcing." ;
    skos:prefLabel "External Sourcing" ;
    skos:related itilglos:Outsourcing .

itilglos:Failure_Modes_and_Effects_Analysis a skos:concept ;
    rdfs:label "Failure Modes and Effects Analysis" ;
    skos:definition "An approach to assessing the potential Impact of Failures. FMEA involves analysing what would happen after Failure of each Configuration Item, all the way up to the effect on the Business. FMEA is often used in Information Security Management and in IT Service Continuity Planning." ;
    skos:prefLabel "Failure Modes and Effects Analysis" ;
    skos:related itilglos:Business,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Failure,
        itilglos:IT_Service,
        itilglos:IT_Service_Continuity_Plan,
        itilglos:Impact,
        itilglos:Information_Security_Management,
        itilglos:Plan,
        itilglos:Planning,
        itilglos:Security,
        itilglos:Security_Management,
        itilglos:Service .

itilglos:First-line_Support a skos:concept ;
    rdfs:label "First-line Support" ;
    skos:definition "The first level in a hierarchy of Support Groups involved in the resolution of Incidents. Each level contains more specialist skills, or has more time or other Resources. See Escalation." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "First-line Support" ;
    skos:related itilglos:Escalation,
        itilglos:Incident,
        itilglos:Resource,
        itilglos:Support_Group .

itilglos:Fishbone_Diagram a skos:concept ;
    rdfs:label "Fishbone Diagram" ;
    skos:definition "Synonym for Ishikawa Diagram." ;
    skos:prefLabel "Fishbone Diagram" ;
    skos:related itilglos:Ishikawa_Diagram .

itilglos:Fixed_Cost a skos:concept ;
    rdfs:label "Fixed Cost" ;
    skos:definition "A Cost that does not vary with IT Service usage. See Variable Cost." ;
    skos:example "For example the cost of Server hardware." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Fixed Cost" ;
    skos:related itilglos:Cost,
        itilglos:IT_Service,
        itilglos:Service,
        itilglos:Variable_Cost .

itilglos:Follow_the_Sun a skos:concept ;
    rdfs:label "Follow the Sun" ;
    skos:definition "A methodology for using Service Desks and Support Groups around the world to provide seamless 24 * 7 Service. Calls, Incidents, Problems and Service Requests are passed between groups in different time zones." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Follow the Sun" ;
    skos:related itilglos:Call,
        itilglos:Incident,
        itilglos:Problem,
        itilglos:Service,
        itilglos:Service_Desk,
        itilglos:Service_Request,
        itilglos:Support_Group .

<http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_27001> a skos:concept ;
    rdfs:label "ISO/IEC 27001" ;
    skos:definition "ISO Specification for Information Security Management. The corresponding Code of Practice is ISO/IEC 17799." ;
    skos:member itilproc:Continual_Service_Improvement,
        itilproc:Service_Design ;
    skos:prefLabel "ISO/IEC 27001" ;
    skos:related itilglos:Code_of_Practice,
        <http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_17799>,
        itilglos:Information_Security_Management,
        itilglos:Practice,
        itilglos:Security,
        itilglos:Security_Management,
        itilglos:Specification .

itilglos:ISO_9001 a skos:concept ;
    rdfs:label "ISO 9001" ;
    skos:definition "An international Standard for Quality Management Systems." ;
    skos:prefLabel "ISO 9001" ;
    skos:related itilglos:Management_System,
        itilglos:Quality,
        itilglos:Quality_Management_System,
        itilglos:Standard,
        itilglos:System .

itilglos:IT_Directorate a skos:concept ;
    rdfs:label "IT Directorate" ;
    skos:definition "Senior Management within a Service Provider, charged with developing and delivering IT services. Most commonly used in UK Government departments." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "IT Directorate" ;
    skos:related itilglos:Service,
        itilglos:Service_Provider .

itilglos:Information_Security_Management_System a skos:concept ;
    rdfs:label "Information Security Management System" ;
    skos:definition "The framework of Policy, Processes, Standards, Guidelines and tools that ensures an Organisation can achieve its Information Security Management Objectives." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Information Security Management System" ;
    skos:related itilglos:Guideline,
        itilglos:Information_Security_Management,
        itilglos:Objective,
        itilglos:Organisation,
        itilglos:Policy,
        itilglos:Process,
        itilglos:Security,
        itilglos:Security_Management,
        itilglos:Standard .

itilglos:Insourcing a skos:concept ;
    rdfs:label "Insourcing" ;
    skos:definition "Synonym for Internal Sourcing" ;
    skos:prefLabel "Insourcing" ;
    skos:related itilglos:Internal_Sourcing .

itilglos:International_Standards_Organisation a skos:concept ;
    rdfs:label "International Standards Organisation" ;
    skos:definition "See International Organization for Standardization (ISO)" ;
    skos:prefLabel "International Standards Organisation" ;
    skos:related itilglos:International_Organization_for_Standardization,
        itilglos:Standard .

itilglos:Internet_Service_Provider a skos:concept ;
    rdfs:label "Internet Service Provider" ;
    skos:definition "An External Service Provider that provides access to the Internet. Most ISPs also provide other IT Services such as web hosting." ;
    skos:prefLabel "Internet Service Provider" ;
    skos:related itilglos:External_Service_Provider,
        itilglos:IT_Service,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Job_Description a skos:concept ;
    rdfs:label "Job Description" ;
    skos:definition "A Document which defines the Roles, responsibilities, skills and knowledge required by a particular person." ;
    skos:example "One Job Description can include multiple Roles, for example the Roles of Configuration Manager and Change Manager may be carried out by one person." ;
    skos:prefLabel "Job Description" ;
    skos:related itilglos:Document,
        itilglos:Role .

<http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#Kepner_&_Tregoe_Analysis> a skos:concept ;
    rdfs:label "Kepner &amp; Tregoe Analysis" ;
    skos:definition "A structured approach to Problem solving. The Problem is analysed in terms of what, where, when and extent. Possible causes are identified. The most probable cause is tested. The true cause is verified." ;
    skos:member itilproc:Continual_Service_Improvement,
        itilproc:Service_Operation ;
    skos:prefLabel "Kepner &amp; Tregoe Analysis" ;
    skos:related itilglos:Problem .

itilglos:Knowledge_Base a skos:concept ;
    rdfs:label "Knowledge Base" ;
    skos:definition "A logical database containing the data used by the Service Knowledge Management System." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Knowledge Base" ;
    skos:related itilglos:Knowledge_Management,
        itilglos:Management_System,
        itilglos:Service,
        itilglos:Service_Knowledge_Management_System,
        itilglos:System .

itilglos:Management_of_Risk a skos:concept ;
    rdfs:label "Management of Risk" ;
    skos:definition "The OGC methodology for managing Risks. MoR includes all the Activities required to identify and Control the exposure to Risk which may have an impact on the achievement of an Organisation&rsquo;s Business Objectives. See http://www.m-o-r.org/ for more details." ;
    skos:prefLabel "Management of Risk" ;
    skos:related itilglos:Business,
        itilglos:Business_Objective,
        itilglos:Control,
        itilglos:Objective,
        itilglos:Organisation,
        itilglos:Risk .

itilglos:Marginal_Cost a skos:concept ;
    rdfs:label "Marginal Cost" ;
    skos:definition "The Cost of continuing to provide the IT Service." ;
    skos:example "Marginal Cost does not include investment already made, for example the cost of developing new software and delivering training." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Marginal Cost" ;
    skos:related itilglos:Cost,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:Maturity_Level a skos:concept ;
    rdfs:label "Maturity Level" ;
    skos:definition "A named level in a Maturity model such as the Carnegie Mellon Capability Maturity Model Integration." ;
    skos:prefLabel "Maturity Level" ;
    skos:related itilglos:Capability,
        itilglos:Capability_Maturity_Model,
        itilglos:Capability_Maturity_Model_Integration,
        itilglos:Maturity,
        itilglos:Model .

itilglos:Mean_Time_Between_Failures a skos:concept ;
    rdfs:label "Mean Time Between Failures" ;
    skos:definition "A Metric for measuring and reporting Reliability. MTBF is the average time that a Configuration Item or IT Service can perform its agreed Function without interruption. This is measured from when the CI or IT Service starts working, until it next fails." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Mean Time Between Failures" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Function,
        itilglos:IT_Service,
        itilglos:Metric,
        itilglos:Reliability,
        itilglos:Service .

itilglos:Mean_Time_Between_Service_Incidents a skos:concept ;
    rdfs:label "Mean Time Between Service Incidents" ;
    skos:definition "A Metric used for measuring and reporting Reliability. MTBSI is the mean time from when a System or IT Service fails, until it next fails. MTBSI is equal to MTBF + MTRS." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Mean Time Between Service Incidents" ;
    skos:related itilglos:IT_Service,
        itilglos:Metric,
        itilglos:Reliability,
        itilglos:Service,
        itilglos:System .

itilglos:Mean_Time_To_Repair a skos:concept ;
    rdfs:label "Mean Time To Repair" ;
    skos:definition "The average time taken to repair a Configuration Item or IT Service after a Failure. MTTR is measured from when the CI or IT Service fails until it is Repaired. MTTR does not include the time required to Recover or Restore. MTTR is sometimes incorrectly used to mean Mean Time to Restore Service." ;
    skos:prefLabel "Mean Time To Repair" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Failure,
        itilglos:IT_Service,
        itilglos:Mean_Time_to_Restore_Service,
        itilglos:Repair,
        itilglos:Restore,
        itilglos:Service .

itilglos:Monitor_Control_Loop a skos:concept ;
    rdfs:label "Monitor Control Loop" ;
    skos:definition "Monitoring the output of a Task, Process, IT Service or Configuration Item; comparing this output to a predefined norm; and taking appropriate action based on this comparison." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Monitor Control Loop" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:IT_Service,
        itilglos:Monitoring,
        itilglos:Process,
        itilglos:Service .

itilglos:Nearshore a skos:concept ;
    rdfs:label "Nearshore" ;
    skos:definition "Provision of Services from a country near the country where the Customer is based. This can be the provision of an IT Service, or of supporting Functions such as Service Desk. See On-shore, Off-shore." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Nearshore" ;
    skos:related itilglos:Customer,
        itilglos:Function,
        itilglos:IT_Service,
        itilglos:Service,
        itilglos:Service_Desk .

itilglos:Office_of_Government_Commerce a skos:concept ;
    rdfs:label "Office of Government Commerce" ;
    skos:definition "OGC owns the ITIL brand (copyright and trademark). OGC is a UK Government department that supports the delivery of the government's procurement agenda through its work in collaborative procurement and in raising levels of procurement skills and capability with departments. It also provides support for complex public sector projects" ;
    skos:prefLabel "Office of Government Commerce" ;
    skos:related itilglos:ITIL .

itilglos:Office_of_Public_Sector_Information a skos:concept ;
    rdfs:label "Office of Public Sector Information" ;
    skos:definition "OPSI license the Crown Copyright material used in the ITIL publications. They are a UK Government department who provide online access to UK legislation, license the re-use of Crown copyright material, manage the Information Fair Trader Scheme, maintain the Government&rsquo;s Information Asset Register and provide advice and guidance on official publishing and Crown copyright." ;
    skos:prefLabel "Office of Public Sector Information" ;
    skos:related itilglos:Asset,
        itilglos:Asset_Register,
        itilglos:ITIL .

itilglos:Offshore a skos:concept ;
    rdfs:label "Offshore" ;
    skos:definition "Provision of Services from a location outside the country where the Customer is based, often in a different continent. This can be the provision of an IT Service, or of supporting Functions such as Service Desk. See On-shore, Near-shore." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Offshore" ;
    skos:related itilglos:Customer,
        itilglos:Function,
        itilglos:IT_Service,
        itilglos:Service,
        itilglos:Service_Desk .

itilglos:Onshore a skos:concept ;
    rdfs:label "Onshore" ;
    skos:definition "Provision of Services from a location within the country where the Customer is based. See Off-shore, Near-shore." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Onshore" ;
    skos:related itilglos:Customer,
        itilglos:Service .

itilglos:Operational_Expenditure a skos:concept ;
    rdfs:label "Operational Expenditure" ;
    skos:definition "Synonym for Operational Cost" ;
    skos:prefLabel "Operational Expenditure" ;
    skos:related itilglos:Cost,
        itilglos:Operation,
        itilglos:Operational,
        itilglos:Operational_Cost .

itilglos:Optimise a skos:concept ;
    rdfs:label "Optimise" ;
    skos:definition "Review, Plan and request Changes, in order to obtain the maximum Efficiency and Effectiveness from a Process, Configuration Item, Application etc." ;
    skos:prefLabel "Optimise" ;
    skos:related itilglos:Application,
        itilglos:Change,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Effectiveness,
        itilglos:Efficiency,
        itilglos:Plan,
        itilglos:Process,
        itilglos:Review .

itilglos:PRojects_IN_Controlled_Environments a skos:concept ;
    rdfs:label "PRojects IN Controlled Environments" ;
    skos:definition "See PRINCE2" ;
    skos:prefLabel "PRojects IN Controlled Environments" ;
    skos:related itilglos:PRINCE2 .

itilglos:Pain_Value_Analysis a skos:concept ;
    rdfs:label "Pain Value Analysis" ;
    skos:definition "A technique used to help identify the Business Impact of one or more Problems. A formula is used to calculate Pain Value based on the number of Users affected, the duration of the Downtime, the Impact on each User, and the cost to the Business (if known)." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Pain Value Analysis" ;
    skos:related itilglos:Business,
        itilglos:Downtime,
        itilglos:Impact,
        itilglos:Problem,
        itilglos:User .

itilglos:Passive_Monitoring a skos:concept ;
    rdfs:label "Passive Monitoring" ;
    skos:definition "Monitoring of a Configuration Item, an IT Service or a Process that relies on an Alert or notification to discover the current status. See Active Monitoring." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Passive Monitoring" ;
    skos:related itilglos:Active_Monitoring,
        itilglos:Alert,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:IT_Service,
        itilglos:Monitoring,
        itilglos:Process,
        itilglos:Service .

itilglos:Percentage_utilisation a skos:concept ;
    rdfs:label "Percentage utilisation" ;
    skos:definition "The amount of time that a Component is busy over a given period of time." ;
    skos:example "For example, if a CPU is busy for 1800 seconds in a one hour period, its utilisation is 50%" ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Percentage utilisation" ;
    skos:related itilglos:Component .

itilglos:Performance_Anatomy a skos:concept ;
    rdfs:label "Performance Anatomy" ;
    skos:definition "An approach to Organisational Culture that integrates, and actively manages, leadership and strategy, people development, technology enablement, performance management and innovation." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Performance Anatomy" ;
    skos:related itilglos:Culture,
        itilglos:Organisation .

itilglos:Prerequisite_for_Success a skos:concept ;
    rdfs:label "Prerequisite for Success" ;
    skos:definition "PFS, An Activity that needs to be completed, or a condition that needs to be met, to enable successful implementation of a Plan or Process. A PFS is often an output from one Process that is a required input to another Process." ;
    skos:prefLabel "Prerequisite for Success" ;
    skos:related itilglos:Activity,
        itilglos:Plan,
        itilglos:Process .

itilglos:Pricing a skos:concept ;
    rdfs:label "Pricing" ;
    skos:definition "The Activity for establishing how much Customers will be Charged." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Pricing" ;
    skos:related itilglos:Activity,
        itilglos:Customer .

itilglos:Process_Control a skos:concept ;
    rdfs:label "Process Control" ;
    skos:definition "The Activity of planning and regulating a Process, with the Objective of performing the Process in an Effective, Efficient, and consistent manner." ;
    skos:prefLabel "Process Control" ;
    skos:related itilglos:Activity,
        itilglos:Objective,
        itilglos:Process .

itilglos:Production_Environment a skos:concept ;
    rdfs:label "Production Environment" ;
    skos:definition "Synonym for Live environment" ;
    skos:prefLabel "Production Environment" ;
    skos:related itilglos:Live .

itilglos:Programme a skos:concept ;
    rdfs:label "Programme" ;
    skos:definition "A number of Projects and Activities that are planned and managed together to achieve an overall set of related Objectives and other Outcomes." ;
    skos:prefLabel "Programme" ;
    skos:related itilglos:Objective,
        itilglos:Outcome,
        itilglos:Project .

itilglos:Projected_Service_Outage a skos:concept ;
    rdfs:label "Projected Service Outage" ;
    skos:definition "A Document that identifies the effect of planned Changes, maintenance Activities and Test Plans on agreed Service Levels." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Projected Service Outage" ;
    skos:related itilglos:Change,
        itilglos:Document,
        itilglos:Plan,
        itilglos:Service,
        itilglos:Service_Level,
        itilglos:Test .

itilglos:Quality_Assurance a skos:concept ;
    rdfs:label "Quality Assurance" ;
    skos:definition "The Process responsible for ensuring that the Quality of a product, Service or Process will provide its intended Value." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Quality Assurance" ;
    skos:related itilglos:Process,
        itilglos:Quality,
        itilglos:Service .

itilglos:Qualitycycle_DEMING a skos:concept ;
    rdfs:label "Qualitycycle DEMING" ;
    skos:definition "Synonym for Plan-Do-Check-Act" ;
    skos:prefLabel "Qualitycycle DEMING" ;
    skos:related itilglos:Plan,
        itilglos:Plan-Do-Check-Act .

itilglos:Quick_Win a skos:concept ;
    rdfs:label "Quick Win" ;
    skos:definition "An improvement Activity which is expected to provide a Return on Investment in a short period of time with relatively small Cost and effort. See Pareto Principle." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "Quick Win" ;
    skos:related itilglos:Activity,
        itilglos:Cost,
        itilglos:Pareto_Principle,
        itilglos:Return_on_Investment .

itilglos:Reactive_Monitoring a skos:concept ;
    rdfs:label "Reactive Monitoring" ;
    skos:definition "Monitoring that takes action in response to an Event. See Proactive Monitoring." ;
    skos:example "For example submitting a batch job when the previous job completes, or logging an Incident when an Error occurs." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Reactive Monitoring" ;
    skos:related itilglos:Event,
        itilglos:Monitoring,
        itilglos:Proactive_Monitoring .

itilglos:Redundancy a skos:concept ;
    rdfs:label "Redundancy" ;
    skos:definition "Synonym for Fault Tolerance. The term Redundant also has a generic meaning of obsolete, or no longer needed." ;
    skos:prefLabel "Redundancy" ;
    skos:related itilglos:Fault,
        itilglos:Fault_Tolerance .

itilglos:Relationship_Processes a skos:concept ;
    rdfs:label "Relationship Processes" ;
    skos:definition "The ISO/IEC 20000 Process group that includes Business Relationship Management and Supplier Management." ;
    skos:prefLabel "Relationship Processes" ;
    skos:related itilglos:Business,
        itilglos:Business_Relationship_Management,
        <http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#ISO/IEC_20000>,
        itilglos:Process,
        itilglos:Relationship,
        itilglos:Supplier,
        itilglos:Supplier_Management .

itilglos:Release_Window a skos:concept ;
    rdfs:label "Release Window" ;
    skos:definition "Synonym for Change Window." ;
    skos:prefLabel "Release Window" ;
    skos:related itilglos:Change,
        itilglos:Change_Window .

itilglos:Responsiveness a skos:concept ;
    rdfs:label "Responsiveness" ;
    skos:definition "A measurement of the time taken to respond to something. This could be Response Time of a Transaction, or the speed with which an IT Service Provider responds to an Incident or Request for Change etc." ;
    skos:prefLabel "Responsiveness" ;
    skos:related itilglos:Change,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Incident,
        itilglos:Request_for_Change,
        itilglos:Response_Time,
        itilglos:Service,
        itilglos:Service_Provider,
        itilglos:Transaction .

itilglos:Restoration_of_Service a skos:concept ;
    rdfs:label "Restoration of Service" ;
    skos:definition "See Restore" ;
    skos:prefLabel "Restoration of Service" ;
    skos:related itilglos:Restore .

itilglos:Return_to_Normal a skos:concept ;
    rdfs:label "Return to Normal" ;
    skos:definition "The phase of an IT Service Continuity Plan during which full normal operations are resumed." ;
    skos:example "For example, if an alternate data centre has been in use, then this phase will bring the primary data centre back into operation, and restore the ability to invoke IT Service Continuity Plans again." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Return to Normal" ;
    skos:related itilglos:IT_Service,
        itilglos:IT_Service_Continuity_Plan,
        itilglos:Plan,
        itilglos:Service .

<http://eden.dei.uc.pt/~jcardoso/rdf/itil/glossary.ttl#Root_Cause_Analysis_(RCA)> a skos:concept ;
    rdfs:label "Root Cause Analysis (RCA)" ;
    skos:definition "An Activity that identifies the Root Cause of a Incident or Problem. RCA typically concentrates on IT Infrastructure failures." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Root Cause Analysis (RCA)" ;
    skos:related itilglos:Activity,
        itilglos:IT_Infrastructure,
        itilglos:Incident,
        itilglos:Problem,
        itilglos:Root_Cause .

itilglos:Running_Costs a skos:concept ;
    rdfs:label "Running Costs" ;
    skos:definition "Synonym for Operational Costs" ;
    skos:prefLabel "Running Costs" ;
    skos:related itilglos:Cost,
        itilglos:Operation,
        itilglos:Operational,
        itilglos:Operational_Cost .

itilglos:SWOT_Analysis a skos:concept ;
    rdfs:label "SWOT Analysis" ;
    skos:definition "A technique that reviews and analyses the internal strengths and weaknesses of an Organisation and the external opportunities and threats which it faces SWOT stands for Strengths, Weaknesses, Opportunities and Threats." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "SWOT Analysis" ;
    skos:related itilglos:Organisation,
        itilglos:Threat .

itilglos:Scalability a skos:concept ;
    rdfs:label "Scalability" ;
    skos:definition "The ability of an IT Service, Process, Configuration Item etc. to perform its agreed Function when the Workload or Scope changes." ;
    skos:prefLabel "Scalability" ;
    skos:related itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Function,
        itilglos:IT_Service,
        itilglos:Process,
        itilglos:Scope,
        itilglos:Service,
        itilglos:Workload .

itilglos:Second-line_Support a skos:concept ;
    rdfs:label "Second-line Support" ;
    skos:definition "The second level in a hierarchy of Support Groups involved in the resolution of Incidents and investigation of Problems. Each level contains more specialist skills, or has more time or other Resources." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Second-line Support" ;
    skos:related itilglos:Incident,
        itilglos:Problem,
        itilglos:Resource,
        itilglos:Support_Group .

itilglos:Separation_of_Concerns a skos:concept ;
    rdfs:label "Separation of Concerns" ;
    skos:definition "An approach to Designing a solution or IT Service that divides the problem into pieces that can be solved independently. This approach separates \"what\" is to be done from \"how\" it is to be done." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Separation of Concerns" ;
    skos:related itilglos:Design,
        itilglos:IT_Service,
        itilglos:Service .

itilglos:Service_Acceptance_Criteria a skos:concept ;
    rdfs:label "Service Acceptance Criteria" ;
    skos:definition "A set of criteria used to ensure that an IT Service meets its functionality and Quality Requirements and that the IT Service Provider is ready to Operate the new IT Service when it has been Deployed." ;
    skos:member itilproc:Service_Transition ;
    skos:prefLabel "Service Acceptance Criteria" ;
    skos:related itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Operate,
        itilglos:Quality,
        itilglos:Requirement,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Service_Failure_Analysis a skos:concept ;
    rdfs:label "Service Failure Analysis" ;
    skos:definition "An Activity that identifies underlying causes of one or more IT Service interruptions. SFA identifies opportunities to improve the IT Service Provider's Processes and tools, and not just the IT Infrastructure. SFA is a time constrained, project-like activity, rather than an ongoing process of analysis. See Root Cause Analysis." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Service Failure Analysis" ;
    skos:related itilglos:Activity,
        itilglos:IT_Infrastructure,
        itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Process,
        itilglos:Root_Cause,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Service_Improvement_Plan a skos:concept ;
    rdfs:label "Service Improvement Plan" ;
    skos:definition "SIP, A formal Plan to implement improvements to a Process or IT Service." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "Service Improvement Plan" ;
    skos:related itilglos:IT_Service,
        itilglos:Plan,
        itilglos:Process,
        itilglos:Service .

itilglos:Service_Maintenance_Objective a skos:concept ;
    rdfs:label "Service Maintenance Objective" ;
    skos:definition "The expected time that a Configuration Item will be unavailable due to planned maintenance Activity." ;
    skos:member itilproc:Service_Operation ;
    skos:prefLabel "Service Maintenance Objective" ;
    skos:related itilglos:Activity,
        itilglos:Configuration,
        itilglos:Configuration_Item .

itilglos:Service_Owner a skos:concept ;
    rdfs:label "Service Owner" ;
    skos:definition "A Role which is accountable for the delivery of a specific IT Service." ;
    skos:member itilproc:Continual_Service_Improvement ;
    skos:prefLabel "Service Owner" ;
    skos:related itilglos:IT_Service,
        itilglos:Role,
        itilglos:Service .

itilglos:Service_Potential a skos:concept ;
    rdfs:label "Service Potential" ;
    skos:definition "The total possible value of the overall Capabilities and Resources of the IT Service Provider." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Service Potential" ;
    skos:related itilglos:IT_Service,
        itilglos:IT_Service_Provider,
        itilglos:Resource,
        itilglos:Service,
        itilglos:Service_Provider .

itilglos:Service_Provisioning_Optimization a skos:concept ;
    rdfs:label "Service Provisioning Optimization" ;
    skos:definition "Analysing the finances and constraints of an IT Service to decide if alternative approaches to Service delivery might reduce Costs or improve Quality." ;
    skos:member itilproc:Service_Strategy ;
    skos:prefLabel "Service Provisioning Optimization" ;
    skos:related itilglos:Cost,
