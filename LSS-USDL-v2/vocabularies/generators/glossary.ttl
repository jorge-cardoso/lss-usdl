itilglos:Fault_Tolerance a skos:concept ;
    rdfs:label "Fault Tolerance" ;
    skos:definition "The ability of an IT Service or Configuration Item to continue to Operate correctly after Failure of a Component part. See Resilience, Countermeasure." ;
    skos:member itilproc:Service_Design ;
    skos:prefLabel "Fault Tolerance" ;
    skos:related itilglos:Component,
        itilglos:Configuration,
        itilglos:Configuration_Item,
        itilglos:Countermeasure,
